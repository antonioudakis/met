					                                                                                                                            Message Actions 
						                                                                                                                            On SAM_Click
							!
							                                                                                                                            If ErrorMessageOk( 'Διαγραφή του/της φοιτητή/τριας από όλους τους πίνακες της βάσης. 
									                                         Επιβεβαίωση διαγραφής   ? ','',3)
								!
								                                                                                                                            If NOT hProg.bConnected
									!
									                                                                                                                            Call hProg.Connect()
								                                                                                                                            If NOT hDipl.bConnected
									!
									                                                                                                                            Call hDipl.Connect()
								                                                                                                                            Set nNub = 0
								                                                                                                                            Call hProg.Initialize( ' SELECT  PROGCODE FROM METSTUPROG INTO :sProgramEgg
										where MITROO = :dfStf   '  )
								                                                                                                                            Call hProg.Prepare()
								                                                                                                                            Call hProg.Execute()
								                                                                                                                            Call hProg.Next( nFetch1 )
								                                                                                                                            While nFetch1 != FETCH_EOF
									                                                                                                                            Call hDipl.Initialize( ' SELECT  CODESYN FROM METDSYNERG INTO :nCodSyn
											where MITROO = :dfStf and PROGCODE = :sProgramEgg and TMIMACODE = :nCodeTmima '  )
									                                                                                                                            Call hDipl.Prepare()
									                                                                                                                            Call hDipl.Execute()
									                                                                                                                            Call hDipl.Next( nFetch )
									                                                                                                                            Call hDipl.Initialize( ' SELECT  COUNT(CODESYN) FROM METDSYNERG INTO :nNub
											where CODESYN = :nCodSyn and PROGCODE = :sProgramEgg and TMIMACODE = :nCodeTmima '  )
									                                                                                                                            Call hDipl.Prepare()
									                                                                                                                            Call hDipl.Execute()
									                                                                                                                            Call hDipl.Next( nFetch )
									                                                                                                                            Call hProg.Next( nFetch1 )
									                                                                                                                            If nNub =1
										                                                                                                                            Call hDipl.Initialize( ' DELETE FROM METDIPLGRADE
												where  CODESYN = :nCodSyn  ' )
										                                                                                                                            Call hDipl.Prepare()
										                                                                                                                            Call hDipl.Execute()
										                                                                                                                            Call hDipl.Commit()
										                                                                                                                            Call hProg.Next( nFetch1 )
								                                                                                                                            Call hDipl.Initialize( ' DELETE FROM METDSYNERG
										where MITROO = :dfStf   and TMIMACODE = :nCodeTmima ' )
								                                                                                                                            Call hDipl.Prepare()
								                                                                                                                            Call hDipl.Execute()
								                                                                                                                            Call hDipl.Commit()
