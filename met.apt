						Call hDelSyn.Prepare()
						Call hDelSyn.Execute()
						Call hDelSyn.Commit()
					Call hDelSyn.Initialize( ' DELETE FROM METANATHESI
							where MITROO = :colamSyn  and CODESYN = :colCodeSynerg and TMIMACODE = :nCodeTmima  '  )
					Call hDelSyn.Prepare()
					Call hDelSyn.Execute()
					Call hDelSyn.Commit()
					Call SalSendMsg( tblAnathesi, PAM_POPULATE, 0, 0 )
					Call SalSendMsg( tblEpitropi, PAM_POPULATE, 0, 0 )
