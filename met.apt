								                                                                                                                            Call hDipl.Prepare()
								                                                                                                                            Call hDipl.Execute()
								                                                                                                                            Call hDipl.Commit()
								                                                                                                                            Call hDipl.Initialize( ' DELETE FROM METSTUPROG
										where MITROO = :dfStf   ' )
								                                                                                                                            Call hDipl.Prepare()
								                                                                                                                            Call hDipl.Execute()
								                                                                                                                            Call hDipl.Commit()
								                                                                                                                            Call hDipl.Initialize( ' DELETE FROM METKARTL
