					!
					Set bFirstTime = TRUE
					Select Case nRep
						Case CATALOG_Report
							!
							Set sLang = SalListQueryTextX( cmbLang, SalListQuerySelection( cmbLang ) )
							Set sSelect = 'SELECT  plcode, plstext, pltext
									 INTO  :dlgPlain.sCode,  :dlgPlain.sStext, :dlgPlain.sPltext
									FROM  pltab
									WHERE  plcolumn = :dlgPlain.dfCode AND PLLANG = \'' ||  cmbLang.GetSelection() || '\''
							Call hPln.Initialize( sSelect )
							Call hPln.Prepare()
							! If NOT PrepareCatalogOk
								!
								! Make selection in pltab
								                                                                                                                                                                                                                                                                                                                  Set sLang = SalListQueryTextX( cmbLang, SalListQuerySelection( cmbLang ) )
								                                                                                                                                                                                                                                Set sSelect = 'SELECT  plcode, plstext, pltext
										 INTO  :dlgPlain.sCode,  :dlgPlsal.sStext, :dlgPlsal.sPltext
										FROM  pltab
										WHERE  PLTAB.plcolumn = :dlgPlain.dfCode AND PLTAB.PLLANG = \'' ||  cmbLang.GetSelection() || '\''
								                                                                                                                                                                                                                                                                                                                  Call hPln.Initialize( sSelect )
								                                                                                                                                                                                                                                                                                                                  Call hPln.Prepare()
								!
								                                                                                                                                                                                                                                                                                                                  Set PrepareCatalogOk = TRUE
							Return TRUE
							Break
						Case BASIC_Report
							!
							Set sLang = SalListQueryTextX( cmbLang, SalListQuerySelection( cmbLang ) )
							Set sSelect = 'SELECT  plcode, pllang, plstext, pltext
									 INTO  :dlgPlain.sCode, :dlgPlain.sLang, :dlgPlain.sStext :dlgPlain.sPltext
									FROM  pltab
									WHERE plcolumn like \'COLUMN%\'  AND PLLANG = \'' ||  cmbLang.GetSelection() || '\''
							Call hPla.Initialize( sSelect )
							! Call hPla.Prepare()
							! Set PrepareBasicOk = TRUE
							! If NOT PrepareBasicOk
								!
								! Make selection in pltab
								                                                                                                                                                                                                                                                                                                                  Set sLang = SalListQueryTextX( cmbLang, SalListQuerySelection( cmbLang ) )
								                                                                                                                                                                                                                                Set sSelect = 'SELECT  plcode, pllang, plstext, pltext
										 INTO  :dlgPlain.sCode, :dlgPlsal.sLang, :dlgPlsal.sStext :dlgPlsal.sPltext
										FROM  pltab
										WHERE  PLTAB.plcolumn like \'COLUMN%\'  AND PLTAB.PLLANG = \'' ||  cmbLang.GetSelection() || '\''
								                                                                                                                                                                                                                                                                                                                  Call hPla.Initialize( sSelect )
