							! If bFT
								                                                                                                                                                          Set nSeq = 0
								                                                                                                                                                          If NOT hGetSyn.bConnected
									!
									                                                                                                                                                          Call hGetSyn.Connect()
								                                                                                                                                                          If NOT hInsSyn.bConnected
									!
									                                                                                                                                                          Call hInsSyn.Connect()
