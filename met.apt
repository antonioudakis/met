			Function: FileChooser
				Description: Ανοίγει το dlg για την επιλογή αρχείου
				Returns
					Boolean:
				Parameters
					String: sChooserType
					String: sFileType
					Receive String: sExamDestination
					Receive String: sExamDestinationFolder
					Receive String: sExamName
					Number: nAk
					String: sPeriod
					Number: nCourCode
				Static Variables
				Local variables
					String: sFilters[*]
					Number: nFilters
					String: sFile
					String: sPath
					Number: nIndex
				Actions
					If sFileType = 'BAU'
						Set sFilters[0] = "Αρχεία βαθμολογιών (*.bau) "
						Set sFilters[1] = "MET_GRADES_*_*.bau"
						Set nFilters = 2
					Else If sFileType = 'CSV'
						Set sFilters[0] = "Comma delimited (*.csv) "
						Set sFilters[1] = "*.csv"
						Set nFilters = 2
					Else If sFileType = 'XML'
						Set sFilters[0] = "XML Files (*.xml) "
						Set sFilters[1] = "*.xml"
						Set nFilters = 2
					Else
						Set sFilters[0] = "Όλα τα αρχεία (*.*) "
						Set sFilters[1] = "*.*"
						Set nFilters = 2
					If sChooserType = "Open"
						If SalDlgOpenFile(hWndForm, "’νοιγμα του αρχείου καταχώρησης βαθμολογιών", sFilters, nFilters, nIndex, sFile, sPath)
							! Set sFilePath = sPath
							Set sExamDestination = sPath
							Set sExamName = sFile
							Set sExamDestinationFolder = SalStrLeftX(sPath, SalStrLength(sPath) - SalStrLength(sExamName) )
							Return TRUE
						Else
							Return FALSE
					Else If sChooserType = "Save"
						If sPeriod = '2' 
							Set sFile = "MET_GRADES_"||SalNumberToStrX(nAk,0)||"_E_"||SalNumberToStrX(nCourCode,0)
						Else
							Set sFile = "MET_GRADES_"||SalNumberToStrX(nAk,0)||"_K_"||SalNumberToStrX(nCourCode,0)
						If SalDlgSaveFile(hWndForm, "Αποθήκευση του αρχείου καταχώρησης βαθμολογιών", sFilters, nFilters, nIndex, sFile, sPath)
							! Set sFilePath = sPath
							Set sExamDestination = sPath
							Set sExamName = sFile
							Set sExamDestinationFolder = SalStrLeftX(sPath, SalStrLength(sPath) - SalStrLength(sExamName) )
							Return TRUE
						Else
							Return FALSE
					Else
						Return FALSE
			Function: FileWrite
				Description:
				Returns
				Parameters
					String: sFile
				Static Variables
				Local variables
					File Handle: hFile
					String: sWriteBuffer
					Boolean: bCreate
					Number: nIndexGrades
				Actions
					! ’νοιγμα Αρχείου
					If not FileOpen(hFile,sFile,1)
						!
						Return FALSE
					! Εκτύπωση στο αρχείο των σταθερών στοιχείων του μαθήματος
					! XML
					! Set sWriteBuffer = "<?xml version=\"1.0\" encoding=\"UTF-8\"?>
							<grades>
							    <course>"
					! Call VisFileWriteString( hFile, sWriteBuffer )
					Set bEnglish = FALSE
					Call GetTitlos(TRUE)
					Set sWriteBuffer = "U
							{
							#;Κωδικός Σχολής;"||SalNumberToStrX( nCodeTmima, 0 )||";#
							#;Τίτλος Σχολής;"||sTmima1||";#
							#;Κωδικός Περιόδου Δελτίου;"||sPeriodTest||";#
							#;Λεκτικό Περιόδου Δελτίου;"||SalListQueryTextX( cmbPeriodTest, SalListQuerySelection( cmbPeriodTest ))||";#
							#;Ακ. Έτος;"||SalNumberToStrX(dfAkEtosVa,0)||";#
							#;Κωδικός Προγράμματος;"||SalNumberToStrX(nProgCode,0)||";#
							#;Λεκτικό Προγράμματος;"||cmbProgCode||";#
							#;Κωδικός Δελτίου;"||";#
							#;Λεκτικό Δελτίου;"||";#
							#;Τίτλος Μαθήματος;"||dfTitlosCourseV||";#
							#;Κωδικός Μαθήματος;"||SalNumberToStrX( dfCourseCodeV,0)||";#
							#;Εξάμηνο Μαθήματος;"||GetEjAniki()||";#
							#;Διδάσκοντες;"||GetDep()||";#
							#;Α/Α;ΚΩΔ.ΦΟΙΤΗΤΗ;ΕΠΩΝΥΜΟ;ΟΝΟΜΑ;ΠΑΤΡΩΝΥΜΟ;ΕΞΑΜ.ΦΟΙΤΗΤΗ;ΑΔ;ΒΑΘΜΟΣ ΚΑΝΟΝΙΚΗΣ;ΒΑΘΜΟΣ ΕΠΑΝΑΛΗΠΤΙΚΗΣ;ΚΩΔ.ΚΑΤΕΥΘΥΝΣΗΣ;ΒΕΛΤΙΩΣΗ;#"
					! XML
					! Set sWriteBuffer = "        <description><tmimaCode>"||SalNumberToStrX( nCodeTmima, 0 )||"</tmimaCode><tmimaDescr>"||sTmima1||"</tmimaDescr><periodCode>"||sPeriodTest||"</periodCode>"
					! Set sWriteBuffer = sWriteBuffer||"<periodDescr>"||SalListQueryTextX( cmbPeriodTest, SalListQuerySelection( cmbPeriodTest ))||"</periodDescr><etos>"||SalNumberToStrX(dfAkEtosVa,0)||"</etos>"
					! Set sWriteBuffer = sWriteBuffer||"<progCode>"||SalNumberToStrX( nProgCode,0 )||"</progCode><progDescr>"||cmbProgCode||"</progDescr>"
					! Set sWriteBuffer = sWriteBuffer||"<courseCode>"||SalNumberToStrX( dfCourseCodeV,0)||"</courseCode><courseDescr>"||dfTitlosCourseV||"</courseDescr><ejAniki>"||GetEjAniki()||"</ejAniki><dep>"||GetDep()||"</dep><epibl>"||GetEpibl()||"</epibl>"
					! Set sWriteBuffer = sWriteBuffer||"</description>"
					! BAU
					! Set sWriteBuffer = SalNumberToStrX( nCodeTmima, 0 )||"^"||sTmima1||"^"||sPeriodTest||"^"
					! Set sWriteBuffer = sWriteBuffer||SalListQueryTextX( cmbPeriodTest, SalListQuerySelection( cmbPeriodTest ))||"^"||SalNumberToStrX(dfAkEtosVa,0)||"^"
					! Set sWriteBuffer = sWriteBuffer||SalNumberToStrX( nProgCode,0 )||"^"||cmbProgCode||"^"
					! Set sWriteBuffer = sWriteBuffer||SalNumberToStrX( dfCourseCodeV,0)||"^"||dfTitlosCourseV||"^"||GetEjAniki()||"^"||GetDep()||"^"||GetEpibl()||"^"
					Call VisFileWriteString( hFile, sWriteBuffer )
					! Εκτύπωση στο αρχείο των αναλυτικών εγγραφών των φοιτητών οι οποίοι έχουν δηλώσει το μάθημα
					Set nIndexGrades = 0
