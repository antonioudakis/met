			Function: GetStudents
				Description:
				Returns
					! Boolean: 
				Parameters
				Static Variables
				Local variables
					! String: sSelect
					! Number: nFetch
				Actions
					Set sCurrentDate =SalNumberToStrX( SalDateMonth( SalDateCurrent(  ) ),0)||'/'||SalNumberToStrX( SalDateDay( SalDateCurrent(  ) ),0)||'/'||SalNumberToStrX( SalDateYear( SalDateCurrent(  ) ),0)
					Set nGradeNor = NUMBER_Null
					Set nGradePan = NUMBER_Null
					If NOT hGetStudents.bConnected
						!
						Call hGetStudents.Connect()
					Call SalTblReset( tblKartelaVa )
					If sPeriodEg != STRING_Null
						If sPeriodTest = '1'
							! Το παρακάτω select εξαιρεί τους διπλωματούχους.
									! Αφορά την περίπτωση που ένα φοιτητής πέρασε το μάθημα στην κανονική περίοδο δεν το έμαθε αμέσως δήλωσε το μάθημα στην επαναληπτική περίοδο και αναγορεύτηκε πριν την επαναληπτική. 
							! Set sSelect = 'SELECT metkartl.mitroo , epvn , onoma , patr  , gradenor
									FROM metkartl , met
									INTO :sK_F, :sEpvn, :sOnoma , :frmVathmi.sPatr , :nGradeNor
									WHERE courcode = :dfCourseCodeV  and metkartl.mitroo = met.mitroo  and periodaniki = :frmVathmi.sPeriodEg and
									 metkartl.ak = :dfAkEtosVa  and  metkartl.tmimacode = :nCodeTmima  and metkartl.progcode =:frmVathmi.nProgCode and (metkartl.apalag != 1 or metkartl.apalag is null) 
									and met.mitroo not in (select mitroo from metdiakop where dateapo <= '||sCurrentDate||' and (dateeos >= '||sCurrentDate||' or dateeos is null))
									and met.mitroo not in (select metdsynerg.mitroo from metdsynerg,metanathesi where metdsynerg.mitroo = metanathesi.mitroo 
									and dateanag is not null )
									order by epvn ,onoma,patr'
							Set sSelect = 'SELECT metkartl.mitroo , epvn , onoma , patr  , gradenor, apon
									FROM metkartl , met
									INTO :sK_F, :sEpvn, :sOnoma , :frmVathmi.sPatr , :nGradeNor, :sApon
									WHERE courcode = :dfCourseCodeV  and metkartl.mitroo = met.mitroo  and periodaniki = :frmVathmi.sPeriodEg and
									 metkartl.ak = :dfAkEtosVa  and  metkartl.tmimacode = :nCodeTmima  and metkartl.progcode =:frmVathmi.nProgCode and (metkartl.apalag != 1 or metkartl.apalag is null) 
									and met.mitroo not in (select mitroo from metdiakop where dateapo <= '||sCurrentDate||' and (dateeos >= '||sCurrentDate||' or dateeos is null))'
						Else
							! Το παρακάτω select εξαιρεί τους διπλωματούχους.
									! Αφορά την περίπτωση που ένα φοιτητής πέρασε το μάθημα στην κανονική περίοδο δεν το έμαθε αμέσως δήλωσε το μάθημα στην επαναληπτική περίοδο και αναγορεύτηκε πριν την επαναληπτική. 
							! Set sSelect = 'SELECT metkartl.mitroo , epvn , onoma , patr  , gradenor , gradepan
									FROM metkartl , met
									INTO :sK_F, :sEpvn, :sOnoma , :frmVathmi.sPatr , :nGradeNor , :nGradePan
									WHERE courcode = :dfCourseCodeV  and metkartl.mitroo = met.mitroo  and (gradenor<5 or gradenor is null) and
									 metkartl.ak = :dfAkEtosVa  and  metkartl.tmimacode = :nCodeTmima  and metkartl.progcode =:frmVathmi.nProgCode and periodaniki = :frmVathmi.sPeriodEg and (metkartl.apalag != 1 or metkartl.apalag is null)  
									and met.mitroo not in (select mitroo from metdiakop where dateapo <= '||sCurrentDate||' and (dateeos >= '||sCurrentDate||' or dateeos is null))
									and met.mitroo not in (select metdsynerg.mitroo from metdsynerg,metanathesi where metdsynerg.mitroo = metanathesi.mitroo 
									and dateanag is not null )
									order by epvn ,onoma,patr'
							Call SqlImmediate( 'Select limit from metprogat into :nVathmiLimit where tmimacode='||SalNumberToStrX( nCodeTmima, 0 )||' and progcode='||SalNumberToStrX( frmVathmi.nProgCode, 0 )||' and ak='||SalNumberToStrX( dfAkEtosVa, 0 ))
							If nVathmiLimit = NUMBER_Null or nVathmiLimit = 0
								Set nVathmiLimit = 5
							Set sSelect = 'SELECT metkartl.mitroo , epvn , onoma , patr  , gradenor , gradepan, apon
									FROM metkartl , met
									INTO :sK_F, :sEpvn, :sOnoma , :frmVathmi.sPatr , :nGradeNor , :nGradePan, , :sApon
									WHERE courcode = :dfCourseCodeV  and metkartl.mitroo = met.mitroo  and (gradenor<'||SalNumberToStrX( nVathmiLimit,0)||' or gradenor is null) and
									 metkartl.ak = :dfAkEtosVa  and  metkartl.tmimacode = :nCodeTmima  and metkartl.progcode =:frmVathmi.nProgCode and periodaniki = :frmVathmi.sPeriodEg and (metkartl.apalag != 1 or metkartl.apalag is null)  
									and met.mitroo not in (select mitroo from metdiakop where dateapo <= '||sCurrentDate||' and (dateeos >= '||sCurrentDate||' or dateeos is null))'
					Else
						If sPeriodTest = '1'
							! Το παρακάτω select εξαιρεί τους διπλωματούχους.
									! Αφορά την περίπτωση που ένα φοιτητής πέρασε το μάθημα στην κανονική περίοδο δεν το έμαθε αμέσως δήλωσε το μάθημα στην επαναληπτική περίοδο και αναγορεύτηκε πριν την επαναληπτική. 
							! Set sSelect = 'SELECT metkartl.mitroo , epvn , onoma , patr  , gradenor
									FROM metkartl , met
									INTO :sK_F, :sEpvn, :sOnoma , :frmVathmi.sPatr , :nGradeNor
									WHERE courcode = :dfCourseCodeV  and metkartl.mitroo = met.mitroo   and
									 metkartl.ak = :dfAkEtosVa  and  metkartl.tmimacode = :nCodeTmima  and metkartl.progcode =:frmVathmi.nProgCode and (metkartl.apalag != 1 or metkartl.apalag is null)  
									and met.mitroo not in (select mitroo from metdiakop where dateapo <= '||sCurrentDate||' and (dateeos >= '||sCurrentDate||' or dateeos is null))
									and met.mitroo not in (select metdsynerg.mitroo from metdsynerg,metanathesi where metdsynerg.mitroo = metanathesi.mitroo 
									and dateanag is not null )
									order by epvn ,onoma,patr'
							Set sSelect = 'SELECT metkartl.mitroo , epvn , onoma , patr  , gradenor, apon
									FROM metkartl , met
									INTO :sK_F, :sEpvn, :sOnoma , :frmVathmi.sPatr , :nGradeNor , :sApon
									WHERE courcode = :dfCourseCodeV  and metkartl.mitroo = met.mitroo   and
									 metkartl.ak = :dfAkEtosVa  and  metkartl.tmimacode = :nCodeTmima  and metkartl.progcode =:frmVathmi.nProgCode and (metkartl.apalag != 1 or metkartl.apalag is null)  
									and met.mitroo not in (select mitroo from metdiakop where dateapo <= '||sCurrentDate||' and (dateeos >= '||sCurrentDate||' or dateeos is null))'
						Else
							! Το παρακάτω select εξαιρεί τους διπλωματούχους.
									! Αφορά την περίπτωση που ένα φοιτητής πέρασε το μάθημα στην κανονική περίοδο δεν το έμαθε αμέσως δήλωσε το μάθημα στην επαναληπτική περίοδο και αναγορεύτηκε πριν την επαναληπτική. 
							! Set sSelect = 'SELECT metkartl.mitroo , epvn , onoma , patr  , gradenor , gradepan
									FROM metkartl , met
									INTO :sK_F, :sEpvn, :sOnoma , :frmVathmi.sPatr , :nGradeNor , :nGradePan
									WHERE courcode = :dfCourseCodeV  and metkartl.mitroo = met.mitroo  and (gradenor<5 or gradenor is null) and
									 metkartl.ak = :dfAkEtosVa  and  metkartl.tmimacode = :nCodeTmima  and metkartl.progcode =:frmVathmi.nProgCode and (metkartl.apalag != 1 or metkartl.apalag is null)  
									and met.mitroo not in (select mitroo from metdiakop where dateapo <= '||sCurrentDate||' and (dateeos >= '||sCurrentDate||' or dateeos is null))
									and met.mitroo not in (select metdsynerg.mitroo from metdsynerg,metanathesi where metdsynerg.mitroo = metanathesi.mitroo 
									and dateanag is not null )
									order by epvn ,onoma,patr'
							Call SqlImmediate( 'Select limit from metprogat into :nVathmiLimit where tmimacode='||SalNumberToStrX( nCodeTmima, 0 )||' and progcode='||SalNumberToStrX( frmVathmi.nProgCode, 0 )||' and ak='||SalNumberToStrX( dfAkEtosVa, 0 ))
							If nVathmiLimit = NUMBER_Null or nVathmiLimit = 0
								Set nVathmiLimit = 5
							Set sSelect = 'SELECT metkartl.mitroo , epvn , onoma , patr  , gradenor , gradepan, apon
									FROM metkartl , met
									INTO :sK_F, :sEpvn, :sOnoma , :frmVathmi.sPatr , :nGradeNor , :nGradePan, :sApon
									WHERE courcode = :dfCourseCodeV  and metkartl.mitroo = met.mitroo  and (gradenor<'||SalNumberToStrX( nVathmiLimit,0)||' or gradenor is null) and
									 metkartl.ak = :dfAkEtosVa  and  metkartl.tmimacode = :nCodeTmima  and metkartl.progcode =:frmVathmi.nProgCode and (metkartl.apalag != 1 or metkartl.apalag is null)  
									and met.mitroo not in (select mitroo from metdiakop where dateapo <= '||sCurrentDate||' and (dateeos >= '||sCurrentDate||' or dateeos is null))'
					Set sSelect = sSelect||sExceptMitroo||' order by epvn,onoma,patr'
					Call hGetStudents.Initialize(sSelect)
					Call hGetStudents.Prepare()
					Call hGetStudents.Execute()
					Call hGetStudents.Next( nFetch )
					While nFetch != FETCH_EOF
						!
						Call SalTblInsertRow( tblKartelaVa, TBL_MaxRow )
						Call SalTblSetCellTextColor( tblKartelaVa.colK_fVa, COLOR_Red, FALSE )
						Call SalTblSetColumnText( tblKartelaVa, 1, sK_F )
						Call SalTblSetColumnText( tblKartelaVa, 2, sEpvn )
						Call SalTblSetColumnText( tblKartelaVa, 3, sOnoma )
						Call SalTblSetColumnText( tblKartelaVa, 4, frmVathmi.sPatr )
