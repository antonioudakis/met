			! Function: ChangeStatus
				                                                                                  Description: 
				                                                                                  Returns 
					                                                                                  Boolean: bCancelStatus
				                                                                                  Parameters 
				                                                                                  Static Variables 
				                                                                                  Local variables 
					                                                                                  String: sSelect
					                                                                                  Number: nDateapo
					                                                                                  Number: nDateeos
					                                                                                  String: sStatusFoit
					                                                                                  String: sDateanag
					                                                                                  String: sDateeos
					                                                                                  Boolean: bCancelStatus
				                                                                                  Actions 
					! tom
					                                                                                  Set bCancelStatus =TRUE
					                                                                                  Set sSelect = "Select STUSTATUS  FROM METSTUPROG  INTO :sStatusFoit
							WHERE MITROO = :dfStudCodeDiak and PROGCODE = :sProgramEgg"
					                                                                                  Call SqlImmediate ( sSelect )
					!
					                                                                                  Set sSelect = "Select dateanag FROM METANATHESI,METDSYNERG INTO :sDateanag
							                            WHERE METDSYNERG.MITROO = :dfStudCodeDiak and METDSYNERG.MITROO = METANATHESI.MITROO
							                            AND METDSYNERG.PROGCODE = :sProgramEgg and METDSYNERG.CODESYN = METANATHESI.CODESYN 
							                            and dateanag is not null ORDER BY dateanag desc"
					                                                                                  Call SqlImmediate ( sSelect )
					!
					                                                                                  Set sSelect = "Select @MID(dateapo,0,4),@MID(dateeos,0,4),dateeos FROM METDIAKOP INTO :nDateapo, :nDateeos,:sDateeos
							WHERE MITROO = :dfStudCodeDiak AND PROGCODE = :sProgramEgg ORDER BY SEQNO DESC"
					                                                                                  Call SqlImmediate ( sSelect )
					                                                                                  If (sDateeos > sDateanag or sDateeos =STRING_Null)  and sDateanag != STRING_Null
						                                                                                  Call ErrorMessageOk( 'Ο Σπουδαστής έχει ήδη αναγορευτεί ! ', '',1 )
						                                                                                  Set bCancelStatus =FALSE
						!
					                                                                                  Else 
						                                                                                  If nDateeos = NUMBER_Null
							                                                                                  If sStatusFoit !='DO'
								                                                                                  Set sStatusFoit = 'DO'
								                                                                                  Call SqlImmediate (' UPDATE METSTUPROG SET STUSTATUS  = :sStatusFoit
										                                        WHERE MITROO = :dfStudCodeDiak and PROGCODE = :sProgramEgg')
								                                                                                  When SqlError
									                                                                                  Call SqlImmediate( 'ROLLBACK')
								                                                                                  Call SqlImmediate( 'COMMIT')
						                                                                                  Else 
							                                                                                  If nDateeos < nAkTrexon  and (sStatusFoit ='DP' or sStatusFoit = 'DO')
								                                                                                  Set sStatusFoit = ''
								                                                                                  Call SqlImmediate (' UPDATE METSTUPROG SET STUSTATUS  = :sStatusFoit
										                                        WHERE MITROO = :dfStudCodeDiak and PROGCODE = :sProgramEgg')
								                                                                                  When SqlError
									                                                                                  Call SqlImmediate( 'ROLLBACK')
								                                                                                  Call SqlImmediate( 'COMMIT')
							                                                                                  Else If nDateeos >= nAkTrexon and sStatusFoit !='DP' and sStatusFoit !='AN'
								                                                                                  Set sStatusFoit = 'DP'
								                                                                                  Call SqlImmediate (' UPDATE METSTUPROG SET STUSTATUS  = :sStatusFoit
										                                        WHERE MITROO = :dfStudCodeDiak and PROGCODE = :sProgramEgg')
								                                                                                  When SqlError
									                                                                                  Call SqlImmediate( 'ROLLBACK')
								                                                                                  Call SqlImmediate( 'COMMIT')
					                                                                                  Return bCancelStatus
					                                                                                  Call SqlClearImmediate()
					! tom
