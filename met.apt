					                                                                                                                                                                                                                                Number: nRow
					                                                                                                                                                                                                                                Number: nMinRow
					                                                                                                                                                                                                                                Number: nMaxRow
					                                                                                                                                                                                                                                Number: nCurrentRow
					                                                                                                                                                                                                                                Boolean: bFound
				                                                                                                                                                                                                                                Actions 
					!
					                                                                                                                                                                                                                                Call SalTblSetRow(TblParam, TBL_SetFirstRow )
					                                                                                                                                                                                                                                Call SalTblQueryScroll( TblParam, nRow, nMinRow, nMaxRow )
					                                                                                                                                                                                                                                Loop 
						!
						                                                                                                                                                                                                                                If nCurrentRow <= nMaxRow
							!
							                                                                                                                                                                                                                                Call SalTblGetColumnText(TblParam, nPos, sCode )
							                                                                                                                                                                                                                                If sSub = sCode
								!
								                                                                                                                                                                                                                                Set bFound = TRUE
								                                                                                                                                                                                                                                Break 
							                                                                                                                                                                                                                                Set nCurrentRow = nCurrentRow + 1
							                                                                                                                                                                                                                                Call SalTblSetRow(TblParam, TBL_SetNextRow )
						                                                                                                                                                                                                                                Else 
							!
							                                                                                                                                                                                                                                Set bFound = FALSE
