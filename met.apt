								Set frmEgrafi.dfStf = sCodeFoit
								Call SalSetFocus ( frmEgrafi.dfStf )
								Call SalSetFocus ( frmEgrafi.dfArt)
								Call SalSendMsg(frmEgrafi.pbRetrive,SAM_Click,0,0)
