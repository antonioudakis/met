							If nProgram != 1
								Call SqlImmediate( "select AK from METSTUPROG into :nAk where  MITROO =  '"||sMitroo||"' and PROGCODE = :nProgram")
								Call SqlClearImmediate(  )
								If (nAk = NUMBER_Null) or (nAk = 0)
									Set nErrorCode = 8
									Set sErrorText = "Δεν υπάρχει καταχωρημένη τιμή στο πεδίο ακαδημαϊκό έτος πρώτης εγγραφής. "
									Return FALSE
								Else
									Call SqlImmediate( 'select ARCOURDIPL,LIMIT from METPROGAT into  :nCoursesNeeded, :nLimit  where   PROGCODE = :nProgram and TMIMACODE = :nTmimacode and Ak = :nAk')
									Call SqlClearImmediate(  )
									If (nLimit = NUMBER_Null) or (nLimit = 0)
										Set nLimit = 5
									If (nCoursesNeeded = NUMBER_Null) or (nCoursesNeeded = 0)
