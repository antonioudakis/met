									where MITROO = :dfStf  and TMIMACODE = :nCodeTmima  '  )
							Call hDipl.Prepare()
							Call hDipl.Execute()
							Call hDipl.Initialize( ' DELETE FROM METSTUPROG
									where MITROO = :dfStf   ' )
							Call hDipl.Prepare()
							Call hDipl.Execute()
							Call hDipl.Initialize( ' DELETE FROM METKARTL
									where MITROO = :dfStf  and TMIMACODE = :nCodeTmima ' )
							Call hDipl.Prepare()
							Call hDipl.Execute()
							Call hDipl.Initialize( ' DELETE FROM METEGR
									where MITROO = :dfStf  and TMIMACODE = :nCodeTmima ' )
							Call hDipl.Prepare()
							Call hDipl.Execute()
							Call hDipl.Initialize( ' DELETE FROM METDIAKOP
									where MITROO = :dfStf ' )
							Call hDipl.Prepare()
							Call hDipl.Execute()
							Call hDipl.Initialize( ' DELETE FROM METXRHMA
									where MITROO = :dfStf ' )
							Call hDipl.Prepare()
							Call hDipl.Execute()
							Call hDipl.Initialize( ' DELETE FROM METPROOD
									where MITROO = :dfStf  ' )
							Call hDipl.Prepare()
							Call hDipl.Execute()
							Call hDipl.Initialize( ' DELETE FROM METPTYX
									where MITROO = :dfStf  ' )
							Call hDipl.Prepare()
							Call hDipl.Execute()
							Call hDipl.Initialize( ' DELETE FROM METLANG
									where MITROO = :dfStf  ' )
							Call hDipl.Prepare()
							Call hDipl.Execute()
							Call hDipl.Commit()
							If hDipl.bConnected
								!
								Call hDipl.Disconnect()
							If hProg.bConnected
								!
								Call hProg.Disconnect()
							Call SqlClearImmediate(  )
							Call SalSendMsg( hWndForm, PAM_DELETE, 0, 0 )
