						If nNum = 1
							Set sNumText = 'ΕΝΑ'
						If nNum = 2
							Set sNumText =  'ΔΥΟ'
						If nNum = 3
							Set sNumText = 'ΤΡΙΑ'
						If nNum = 4
							Set sNumText = 'ΤΕΣΣΕΡΑ'
						If nNum = 5
