					Window Handle: hWndLastCol
					String: sTextCol
				Message Actions 
					On SAM_Create
						!
						!
						Set sTabName = 'WEBAIT_PISTOP'
						Set sWhereClauseTbl =  " WEBAIT_PISTOP.K_TM  = :nCodeTmima   "
						Set sTabColList = 'K_TM '
						Set sTabValList = ':nCodeTmima '
						! Set sWhereTbl =  " WEBAIT_PISTOP.k_tm = :nCodeTmima and WEBAIT_PISTOP.UriID = :colUriIDRep and WEBAIT_PISTOP.k_Pistop = :colK_Pistop"
						Set sWhereTbl =  " WEBAIT_PISTOP.k_tm = :nCodeTmima and WEBAIT_PISTOP.AID = :colaIDRep "
						Set nFill = TBL_FillAll
						! Set nFill = TBL_FillNormal
					On PAM_POPULATE
						!
						Set nPrintAit = 0
						Call Populate()
						Call colStatusLektAit.SetSelectionCol(tblWebAitisi,colPISTOSTATUS)
						Call CalcAllRows(tblWebAitisi)
						Set dfAllAit = SalNumberToStrX(nPrintAit,0) || ' επιλεγμένες αιτήσεις '
					On SAM_FetchRowDone
						! Μετά το populate της γραμμής ελέγχει το status της αίτησης και ανάλογα χρωματίζει τα πεδία
						! η αίτηση σε εξέλιξη ...
						If colPISTOSTATUS = 0
							Call SalTblSetCellTextColor( tblWebAitisi.colA_A, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colKfRep, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colEpvnRep, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colOnomaRep, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colPistopLekt, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colArPrvt, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colAITDATE, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colPISTOSTATUS, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colAitio, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colStatusLektAit, COLOR_Black, FALSE )
							! για να εκτυπωθεί ...
							Set colStatusRep = 1
							Set nPrintAit = nPrintAit + 1
						! η διαδικασία ολοκληρώθηκε αλλά η εκτύπωση δεν έγινε για λόγους που αφορούν τον φοιτητή . Υπάρχουν σχόλια.
								 Από εμάς θεωρείται τελειωμένη και στέλνεται στον Server του πρωτοκόλλου.
						Else If colPISTOSTATUS = 2
							Call SalTblSetCellTextColor( tblWebAitisi.colA_A, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colKfRep, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colEpvnRep, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colOnomaRep, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colPistopLekt, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colArPrvt, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colAITDATE, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colPISTOSTATUS, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colAitio, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colStatusLektAit, COLOR_Red, FALSE )
							! Set colStatusRep = 1
							! Set nPrintAit = nPrintAit + 1
						! η διαδικασία της εκτύπωσης του πιστοποιητικού ολοκληρώθηκε.
						Else If colPISTOSTATUS = 1
							Call SalTblSetCellTextColor( tblWebAitisi.colA_A, COLOR_MidnightBlue, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colKfRep, COLOR_MidnightBlue, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colEpvnRep, COLOR_MidnightBlue, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colOnomaRep, COLOR_MidnightBlue, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colPistopLekt, COLOR_MidnightBlue, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colArPrvt, COLOR_MidnightBlue, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colAITDATE, COLOR_MidnightBlue, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colPISTOSTATUS, COLOR_MidnightBlue, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colAitio, COLOR_MidnightBlue, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colStatusLektAit, COLOR_MidnightBlue, FALSE )
					On SAM_RowSetContext
						Set LsAitnoPrint = STRING_Null
						! Call SqlImmediate (' Select PISTOREPLY  from WEBAIT_PISTOP INTO :LsAitnoPrint WHERE UriID = :colUriIDRep and K_PISTOP = :colK_Pistop ')
						Call SqlImmediate (' Select PISTOREPLY  from WEBAIT_PISTOP INTO :LsAitnoPrint WHERE WEBAIT_PISTOP.AID = :colaIDRep and k_tm = :nCodeTmima  ')
						Set mlMsgPrint  = LsAitnoPrint
						Call SqlClearImmediate()
					! On WM_MOUSEMOVE
						!
						! Call SalStatusSetText(  colPistopLekt , m_sStatusText )
						!
						! get the row and column under the mouse
						Set sTextCol = STRING_Null
						Call SalTblObjectsFromPoint(
									tblWebAitisi,
									SalNumberLow( lParam ),
									SalNumberHigh( lParam ),
									nRowTbl, hWndCol, nFlags )
						! If it's a different item, show the tooltip
						If nRowTbl != nLastRow OR hWndCol != hWndLastCol
							Set nLastRow = nRowTbl
							Set hWndLastCol =  hWndCol
							Call SalTblSetFocusRow ( tblWebAitisi, nRowTbl )
							Call SalTblGetColumnText ( tblWebAitisi, SalTblQueryColumnID ( hWndCol ), sTextCol )
							Call colPistopLekt.CreateTip(sTextCol,sTextCol )
							Call colPistopLekt.SetTip(colPistopLekt.m_sTip)
							Call SalStatusSetText(  colPistopLekt , sTextCol )
					!
					! XSAL
					! On 0x0200 !WM_MOUSEMOVE
						! get the row and column under the mouse
						Set sTextCol = STRING_Null
						Call SalTblObjectsFromPoint(
									tblWebAitisi,
									SalNumberLow( lParam ),
									SalNumberHigh( lParam ),
									nRowTbl, hWndCol, nFlags )
						! If it's a different item, show the tooltip
						If nRowTbl != nLastRow OR hWndCol != hWndLastCol
							Set nLastRow = nRowTbl
							Set hWndLastCol =  hWndCol
							Call SalTblSetFocusRow ( tblWebAitisi, nRowTbl )
							Call SalTblGetColumnText ( tblWebAitisi, SalTblQueryColumnID ( hWndCol ), sTextCol )
							Call XSalTooltipShow( hWndItem, sTextCol  )
					! XSAL
					! On SAM_RowHeaderClick
						Call SalTrackPopupMenu ( hWndForm, 'menuEdit', TPM_CursorX | TPM_CursorY | TPM_CenterAlign, 0, 0 )
					! On WM_SPOOLERSTATUS
						Call ErrorMessageOk( ' View Printer Status! ', '', 1 )
