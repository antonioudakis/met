							Call SalSetFocus (frmApofoit.dfkvdApof)
							Call SalModalDialog(dlgTimerMessage, hWndForm, frmApofoit.dfkvdApof )
							Call SalSendMsg(frmApofoit.pbRetrive,SAM_Click,0,0)
							!
							Set frmApofoit.nArProt = SalStrToNumber(sArPrvt)
							!
							! Ανάλογα με το πιστοποιητικό εκτέλεσε τον απαιτούμενο κώδικα και τύπωσε
							!
							! πιστοπ. αποφοίτου
							If sCodePistop = '11'
								!
								Call SalSendMsg( frmApofoit, PAM_Report,BASICSU_Report, 0 )
								Set bPrintReportOk = TRUE
								!
							! πιστοπ. αποφοίτου με αναλυτική βαθμολογία
							If sCodePistop = '12'
								!
								Call SalSendMsg( frmApofoit, PAM_Report,RENEW_Report, 0 )
								Set bPrintReportOk = TRUE
								!
						Else 
							Set bPrintReportOk = FALSE
						!
					! If sForm = 'frmDiplvmat'
						If NOT bFrmDiplCreate
							Call SalCreateWindowEx(frmDiplvmat,hWndForm,0,0,0,0,CREATE_Hidden)
							Set bFrmDiplCreate = TRUE
					!
					If bPrintReportOk
						!
						! PRINTER
						! Set strPrinterName = MTblPrintGetDefPrinterName()
						! If SalReportGetPrinterSettings ( SalNumberToWindowHandle(nRepType), strPrinterName, nOrientation, nPaperType, nWidth, nHeight) = TRUE
							Set strPrinterName = strPrinterName
						! If SalReportSetPrinterSettings ( SalNumberToWindowHandle(nRepType), strPrinterName, RPT_Landscape, RPT_PaperA5, 0, 0) = TRUE
							Set strPrinterName = strPrinterName
						! If NOT OpenPrinterA('HP1220CA3',hPrinter,0)
							Call ErrorMessageOk( ' Ο φοιτητής δεν εχει εγγραφές στο δοσμένο ακαδημαϊκό έτος ! ', '', 1 )
						! Call SalSendMsg(tblWebAitisi,WM_SPOOLERSTATUS,0,0)
						! PRINTER
						!
						If sForm = 'frmEgrafi'
							Call SalModalDialog(dlgTimerMessage, hWndForm, frmEgrafi.dfStf )
							Call SalReportPrint( frmEgrafi, sQrpPath, sVariables, sInputs, SalStrToNumber(sCopies), RPT_PrintAll ,0,0, nErrR )
						!
						! Else If sForm = 'frmApofoit'
							Call SalModalDialog(dlgTimerMessage, hWndForm, frmApofoit.dfkvdApof )
							Call SalReportPrint( frmApofoit, sQrpPath, sVariables, sInputs, SalStrToNumber(sCopies), RPT_PrintAll ,0,0, nErrR )
						!
						! Else If sForm = 'frmDiplvmat'
							Call SalModalDialog(dlgTimerMessage, hWndForm, frmDiplvmat.dfkvdDiplt )
							Call SalReportPrint( frmDiplvmat, sQrpPath, sVariables, sInputs, SalStrToNumber(sCopies), RPT_PrintAll ,0,0, nErrR )
						!
						! Εάν εμφανιστούν μηνύματα κατά την εκτύπωση βάλτα στον πίνακα WEBAIT_PISTOP  ώστε να τα δεί ο χρήστης.
						If bMsgPrint
							Call UpdateWebAitisi(4)
						! Αν συμβεί λάθος κατά την εκτύπωση ενημέρωσε τον πίνακα WEBAIT_PISTOP και επέστεψε FALSE ώστε να μην εκτελέσει τον υπόλοιπο κώδικα
						If nErrR != 0
							If NOT UpdateWebAitisi(2)
							Return FALSE
						! Αν όχι απλά δώσε την τιμή στο πεδίο printerror
						Else 
							If NOT UpdateWebAitisi(1)
								Return FALSE
					Else 
						If NOT UpdateWebAitisi(4)
						Return FALSE
					!
					Return TRUE
			Function: UpdateWebAitisi
				Description: 
				Returns 
					Boolean: 
				Parameters 
					Number: nMode
				Static Variables 
				Local variables 
				Actions 
