						Set strTempSQL = 
								"select tmimacode, progcode, mitroo, ak, period, dateegr, ap, dtarr
								 from meta_metegr
								 into  :nTmimaCode, :nProgCode, :sMitroo, :nAk, :sPeriod, :dDateEgr, :sAP, :dDtArr
								 where ak = :nAkTrexon and period = "||"0"||sXuTrexon
						If SqlPrepareAndExecute( hMSSQLServer, strTempSQL )
							Loop TData1
								If SqlFetchNext( hMSSQLServer, nRetval )
									Call SqlExecute( hSQLBase)
