						! Set sWhereTbl =  " WEBAIT_PISTOP.k_tm = :nCodeTmima and WEBAIT_PISTOP.UriID = :colUriIDRep and WEBAIT_PISTOP.k_Pistop = :colK_Pistop"
						Set sWhereTbl =  " METWEBAITEGR.TMIMACODE= :nCodeTmima "
						Set nFill = TBL_FillAll
						! Set nFill = TBL_FillNormal
					On PAM_POPULATE
						!
						Set nPrintAit = 0
						Call Populate()
						Call tblWebEgr.colEEpvnRep.SetSelectionCol(tblWebEgr,tblWebEgr.colEMitrooRep)
						Call tblWebEgr.colEOnomaRep.SetSelectionCol(tblWebEgr,tblWebEgr.colEMitrooRep)
						Call tblWebEgr.colEStatusLektAit.SetSelectionCol(tblWebEgr,tblWebEgr.colEPistoStatus)
						Call tblWebEgr.colEPeriodEgrText.SetSelectionCol(tblWebEgr, tblWebEgr.colEPeriodEgrCode)
						Call CalcAllRows(tblWebEgr)
						Set dfAllAit = SalNumberToStrX(nPrintAit,0) || ' επιλεγμένες αιτήσεις '
					On SAM_FetchRowDone
						! Μετά το populate της γραμμής ελέγχει το status της αίτησης και ανάλογα χρωματίζει τα πεδία
						! η αίτηση σε εξέλιξη ...
						If tblWebEgr.colEPistoStatus = 0
							Call SalTblSetCellTextColor( tblWebEgr.colEA_A, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEMitrooRep, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEEpvnRep, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEOnomaRep, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEAkEgr, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEPeriodEgrText, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEArPrvt, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEAitDate, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEEgrDate, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEPistoStatus, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEStatusLektAit, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEPistoReply, COLOR_Black, FALSE )
							! για να εκτυπωθεί ...
							Set tblWebEgr.colEStatusRep = 1
							Set nPrintAit = nPrintAit + 1
						! η διαδικασία ολοκληρώθηκε αλλά η εκτύπωση δεν έγινε για λόγους που αφορούν τον φοιτητή . Υπάρχουν σχόλια.
								 Από εμάς θεωρείται τελειωμένη και στέλνεται στον Server του πρωτοκόλλου.
						Else If tblWebEgr.colEPistoStatus = 2
							Call SalTblSetCellTextColor( tblWebEgr.colEA_A, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEMitrooRep, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEEpvnRep, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEOnomaRep, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEAkEgr, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEPeriodEgrText, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEArPrvt, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEAitDate, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEEgrDate, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEPistoStatus, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEStatusLektAit, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEPistoReply, COLOR_Red, FALSE )
							! Set colStatusRep = 1
							! Set nPrintAit = nPrintAit + 1
						! η διαδικασία της εκτύπωσης του πιστοποιητικού ολοκληρώθηκε.
						Else If tblWebEgr.colEPistoStatus = 1
							Call SalTblSetCellTextColor( tblWebEgr.colEA_A, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEMitrooRep, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEEpvnRep, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEOnomaRep, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEAkEgr, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEPeriodEgrText, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEArPrvt, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEAitDate, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEEgrDate, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEPistoStatus, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEStatusLektAit, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebEgr.colEPistoReply, COLOR_DarkGreen, FALSE )
					On SAM_RowSetContext
						! Set LsAitnoPrint = STRING_Null
						! Call SqlImmediate (
								"select reply  
								 from metwebaitegr 
								 into :LsAitnoPrint 
								 where mitroo = :colEMitrooRep and tmimacode = :colETmimacodeRep and and ak = :colEAkEgr and period = :colEPeriodEgrCode")
