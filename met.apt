							                                                           Call SalDisableWindow(pbSaveEgrafi)
							                                                           Return VALIDATE_Cancel
						! Else
							                                                           Call SalDisableWindow(colPeriod)
							                                                           Call tblEgrafi.SaveTbl( )
							                                                           Call SalSendMsg( tblEgrafi, PAM_POPULATE,0,0 )
							                                                           Call tblEgrafi.colPeriod.SetSelectionCol(tblEgrafi,tblEgrafi.colPeriodCod)
						! periodold
						! new
						If SalTblAnyRows( tblEgrafi, ROW_New, 0 )
							If sPROGCODE = STRING_Null or colPeriodCod = STRING_Null or colAkEgr = NUMBER_Null
								If bMultipleEgr = TRUE
									Set sMsgPrint = "Ο φοιτητής δεν έχει καταχωρήσει τα απαιτούμενα πεδία για να καταχωρηθεί η εγγραφή του. Να γίνει έλεγχος."
									Set bMsgPrint = TRUE
								Else
									Call SalMessageBox( 'Δεν έχετε καταχωρήσει όλες τις απαιτούμενες τιμές', 'Σφάλμα καταχώρησης', MB_Ok )
								Call SalSendMsg( tblEgrafi, PAM_POPULATE,0,0 )
								Call tblEgrafi.colPeriod.SetSelectionCol(tblEgrafi,tblEgrafi.colPeriodCod)
							Else
								Call SqlImmediate( 
										"select count(*)
										 from metegr 
										 into :nRecordsExist
										 where mitroo = :dfStudCodeEgr and progcode = :sPROGCODE and tmimacode = :nCodeTmima and ak = :tblEgrafi.colAkEgr and period = :tblEgrafi.colPeriodCod")
								Call SqlClearImmediate(  )
								If nRecordsExist = 0 or nRecordsExist = NUMBER_Null
									Call tblEgrafi.SaveTbl( )
									Call SalSendMsg( tblEgrafi, PAM_POPULATE,0,0 )
									Call tblEgrafi.colPeriod.SetSelectionCol(tblEgrafi,tblEgrafi.colPeriodCod)
								Else
									If bMultipleEgr = TRUE
										Set sMsgPrint = "Ο φοιτητής έχει ήδη εγγραφεί στο συγκεκριμένο Ακαδημαϊκό έτος και περίοδο."
										Set bMsgPrint = TRUE
									Else
										Call SalMessageBox( 'Ο φοιτητής έχει ήδη εγγραφεί στο συγκεκριμένο Ακαδημαϊκό έτος και περίοδο.','Σφάλμα καταχώρησης', MB_Ok )
									Call SalSendMsg( tblEgrafi, PAM_POPULATE,0,0 )
									Call tblEgrafi.colPeriod.SetSelectionCol(tblEgrafi,tblEgrafi.colPeriodCod)
						Else
							Call tblEgrafi.SaveTbl( )
							Call SalSendMsg( tblEgrafi, PAM_POPULATE,0,0 )
							Call tblEgrafi.colPeriod.SetSelectionCol(tblEgrafi,tblEgrafi.colPeriodCod)
