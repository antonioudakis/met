					Menu Item Name:
				Menu Separator
				Menu Item: Διαγραφή Σπουδαστή
					Resource Id: 2515
					Keyboard Accelerator: (none)
					Status Text:
					Menu Settings
						Enabled when: not SalIsNull(dfStf)  
						Checked when:
					Menu Actions
						If ErrorMessageOk( 'Διαγραφή του/της φοιτητή/τριας από όλους τους πίνακες της βάσης. 
								                                         Επιβεβαίωση διαγραφής   ? ','',3)
							!
							If NOT hProg.bConnected
								!
								Call hProg.Connect()
							If NOT hDipl.bConnected
								!
								Call hDipl.Connect()
							Set nNub = 0
							Call hProg.Initialize( ' SELECT  PROGCODE FROM METSTUPROG INTO :sProgramEgg
									where MITROO = :dfStf   '  )
							Call hProg.Prepare()
							Call hProg.Execute()
							Call hProg.Next( nFetch1 )
							While nFetch1 != FETCH_EOF
								Call SqlImmediate(  ' SELECT  CODESYN FROM METDSYNERG INTO :nCodSyn
										where MITROO = :dfStf and PROGCODE = :sProgramEgg and TMIMACODE = :nCodeTmima ' )
								Call SqlImmediate( ' SELECT  COUNT(CODESYN) FROM METDSYNERG INTO :nNub
										where CODESYN = :nCodSyn and PROGCODE = :sProgramEgg and TMIMACODE = :nCodeTmima ' )
								If nNub =1
									Call hDipl.Initialize( ' DELETE FROM METDIPLGRADE
											where  CODESYN = :nCodSyn  ' )
									Call hDipl.Prepare()
									Call hDipl.Execute()
								Call hProg.Next( nFetch1 )
