		Functions
			Function: DecryptCode
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Set sFileD = STRING_Null
					! !!TO FILE
					Set sFile2D = STRING_Null
					! !!TO FILE
					Set sFirstC = STRING_Null
					Set sWriteBufferD = STRING_Null
					Set sCryptoString = STRING_Null
					Set sCryptoChar = STRING_Null
					Set nFileRow = NUMBER_Null
					Set nColumn = NUMBER_Null
					Set nAsciiChar = NUMBER_Null
					Set nAsciiCryptoChar = NUMBER_Null
					Set nTimes = NUMBER_Null
					Set nLenRow = NUMBER_Null
					!
					! Set sFileD= 'a:' ||'\\GRADES'
					! Set sFileD= sExamFolder ||'\\GRADES'
					Set sFileD= sExamFolder || '\\' || sExamFileName
					! !!TO FILE
					! Set sFile2D = 'a:' ||'\\GRADES_D'
					! Set sFile2D = sExamFolder ||'\\GRADES_D'
					Set sFile2D = sExamFolder || '\\D_' || sExamFileName
					If not SalFileOpen(hWriteDecryptFileD, sFile2D, OF_Create | OF_ReadWrite )
						!
						! Call ErrorMessageOk('Ελέξτε αν υπάρχει αρχείο βαθμολογίας στην δισκέττα ! ','',1)
						Return FALSE
					! !!TO FILE
					If not SalFileOpen(hWriteCryptoFileD, sFileD, OF_Read )
						!
						! Call ErrorMessageOk('Ελέξτε αν υπάρχει αρχείο βαθμολογίας στην δισκέττα ! ','',1)
						Return FALSE
					Set nFileRow = 0
					Set nColumn = 0
					Loop
						If nFileRow < 1
							If SalFileGetStr ( hWriteCryptoFileD, sWriteBufferD, 2000 )
								Set nLenRow = SalStrLength ( sWriteBufferD )
								Set sFirstC = SalStrLeftX(sWriteBufferD,1)
								Set nFileRow = nFileRow + 1
								Set sCryptoString = STRING_Null
								Set nColumn = 0
								! If nMode = 1
								While nColumn < nLenRow/2
									Set nColumn = nColumn + 1
									Call SalStrFirstC ( sWriteBufferD, nTimes )
									Call SalStrFirstC ( sWriteBufferD, nAsciiChar )
									Set nAsciiCryptoChar =  ( ( ( (nTimes -14) * 256) + nAsciiChar -1) /2  ) - nFileRow - nColumn
									Set sCryptoChar = SalNumberToChar ( nAsciiCryptoChar )
									If nColumn = 1
										If sCryptoChar = 'L'
											Set locked = TRUE
											Set sCryptoChar = 'U'
											Call SalFilePutStr( hWriteDecryptFileD, sCryptoChar )
										Else
											Set locked = FALSE
											If SalMessageBox ( 'Το αρχείο δεν είναι κλειδωμένο! Θέλετε να προχωρήσετε στην καταχώρηση των βαθμολογιών; ', 'Μήνυμα', MB_YesNo ) = 6
												Call SalFileClose ( hWriteDecryptFileD )
												Call SalFileOpen ( hWriteDecryptFileD, sFile2D ,OF_Delete)
												Return TRUE
											Else
												Call SalFileClose ( hWriteDecryptFileD )
												Call SalFileOpen ( hWriteDecryptFileD, sFile2D ,OF_Delete)
												Return FALSE
									Set sCryptoString = sCryptoString || sCryptoChar
								Set nLenCrStr = SalStrLength ( sCryptoString )
								If sCryptoString != 'U' || dfCodeDisk
									Call SalMessageBox ( 'Ο κωδικός κλειδώματος της δισκέτας είναι λάθος. Εισάγετε τον σωστό κωδικό! ', 'Μήνυμα λάθους', MB_Ok )
									Set bWrongCode = TRUE
									Call SalFileClose ( hWriteDecryptFileD )
									Call SalFileOpen ( hWriteDecryptFileD, sFile2D ,OF_Delete)
									Return FALSE
								! Else If nMode = 2
									While nColumn < nLenRow/2
										Set nColumn = nColumn + 1
										! Set nColumn = nColumn + 1
										Call SalStrFirstC ( sWriteBuffer, nTimes )
										Call SalStrFirstC ( sWriteBuffer, nAsciiChar )
										Set nAsciiCryptoChar =  ( ( ( (nTimes -1) * 256) + nAsciiChar -1) /2  ) - nFileRow - nColumn
										Set sCryptoChar = SalNumberToChar ( nAsciiCryptoChar )
										If nColumn = 1
											If sCryptoChar = 'L'
												Set sCryptoChar = 'U'
												! P
												! Set sPinax[nFileRow,nColumn] = sCryptoChar
												! P
											Else
												Call SalFileClose ( hWriteDecryptFile )
												Call SalFileOpen ( hWriteDecryptFile, sFile2 ,OF_Delete)
												Return FALSE
										Set sCryptoString = sCryptoString || sCryptoChar
										! P
										Set sPinax[nFileRow, nColumn] = sCryptoChar
										! P
									Set sLockCode = SalStrRightX ( sCryptoString, 8 )
									Set nLenCrStr = SalStrLength ( sCryptoString )
									Call SalFilePutStr( hWriteDecryptFile, sCryptoString )
							Else
								Break
						Else
							Break
					! Loop
						If SalFileGetStr ( hWriteCryptoFileD, sWriteBufferD, 2000 )
							Set nLenRow = SalStrLength ( sWriteBufferD )
							Set sFirstC = SalStrLeftX(sWriteBufferD,1)
							Set nFileRow = nFileRow + 1
							Set sCryptoString = STRING_Null
							Set nColumn = 0
							While nColumn < nLenRow/2
								Set nColumn = nColumn + 1
								Call SalStrFirstC ( sWriteBufferD, nTimes )
								Call SalStrFirstC ( sWriteBufferD, nAsciiChar )
								Set nAsciiCryptoChar =  ( ( ( (nTimes -1) * 256) + nAsciiChar -1) /2  ) - nFileRow - nColumn
								Set sCryptoChar = SalNumberToChar ( nAsciiCryptoChar )
								Set sCryptoString = sCryptoString || sCryptoChar
								! P
								Set sPinax[nFileRow, nColumn] = sCryptoChar
								! P
							Set nLenCrStr = SalStrLength ( sCryptoString )
						Else
							Call SalFileClose ( hWriteCryptoFileD )
							Break
					Return TRUE
