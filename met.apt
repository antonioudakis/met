					Set sFile_Kartella = Define_FileName_Plus_Date_Hour( 'met_kartela_' )
					If nCodeTmima = 2
						! Set sFile = Define_FileName_Plus_Date_Hour( '\\\\' || sPathU || '\\web\\met_kartela_' )
						Set sFile = '\\\\' || sPathU || '\\web\\' || sFile_Kartella
					Else
						! Set sFile = Define_FileName_Plus_Date_Hour( 'c:\\george\\web\\met_kartela_' )
						Set sFile = 'C:\\NTUARIS\\WEB\\' || sFile_Kartella
					!
					! ’νοιξε το αρχείο
					If VisFileOpen( hWriteFile , sFile , OF_Create | OF_Text ) =  VTERR_Ok
						!
						! Call VisFileSetAttribute(sFile,FA_Standard, TRUE )
						Call SalMessageBeep( -1 )
						! Call SalFileOpen( hWriteFile, sFile, OF_ReadWrite )
						Call VisFileOpen( hWriteFile , sFile , OF_Create | OF_Write | OF_Text )
					Else
						Call SalMessageBox( 'An error has occured during opening the  ASCII file ' , '' , MB_Ok )
						Return FALSE
					!
					If NOT hWeb.bConnected
						!
						If NOT  hWeb.Connect()
							Return FALSE
					If NOT hWeb_Lect.bConnected
						!
						If NOT  hWeb_Lect.Connect()
							Return FALSE
					If NOT hTitlos_M.bConnected
						!
						If NOT  hTitlos_M.Connect()
							Return FALSE
					If NOT hTitlos_P.bConnected
						!
						If NOT  hTitlos_P.Connect()
							Return FALSE
					!
					! Αρχικοποίσε τον αριθμό των Insert , Update , Delete
					Set nCountInsert = 0
					Set nCountUpdate = 0
					Set nCountDelete = 0
					!
					! Το παρακάτω SELECT παίρνει ένα ROW  από τον METKARTL_WEB
					Call hWeb.Initialize('
							SELECT
							    MODIFTYPE   , DATEMODIF   , MITROO , TMIMACODE, PROGCODE, @TRIM(AK)  , @TRIM(PERIOD),  @TRIM(COURCODE),
							   @TRIM(TYPOS),@TRIM(GRADENOR), @TRIM(GRADEPAN) ,@TRIM(APALAG) , @TRIM(PERIODANIKI), METKARTL_SEQNO
							INTO
							    :sMODIF_TYPE,  :dDATE_MODIF , :sWMITROO,:nWTMIMACODE, :nWPROGCODE, :nWAK, :sWPERIOD  , :nWCOURCODE,  
							    :sWTYPOS, :nWGRADENOR, :nWGRADEPAN,:sWAPALAG, :sWPERIODANIKI, :nWMETKARTL_SEQNO
							FROM METKARTL_WEB
							ORDER BY  DATEMODIF ' )
					If NOT hWeb.Prepare()
						Return FALSE
					If NOT  hWeb.Execute()
						Return FALSE
					!
					! Το παρακάτω SELECT παίρνει τα λεκτικά από τον PLTAB.
					Call hWeb_Lect.Initialize("
							SELECT
							   @trim(PLTAB.PLTEXT)
							INTO
							   :sLectiko
							FROM PLTAB
							WHERE
							      PLTAB.PLCODE = :sWValue  AND PLTAB.PLLANG='G'  AND   PLTAB.PLCOLUMN= :sPlCol " )
					If NOT hWeb_Lect.Prepare()
						Return FALSE
					!
					! Το παρακάτω SELECT παίρνει τον τίτλο του μαθήματος.
					Call hTitlos_M.Initialize("
							SELECT
							   @trim( TITLOS )
							INTO
							   :sLectiko
							FROM METCOURSE
							WHERE
							      METCOURSE.COURCODE = :nWCOURCODE  AND   METCOURSE.AK =  :nWAK " )
					If NOT hTitlos_M.Prepare()
						Return FALSE
					!
					! Το παρακάτω SELECT παίρνει τον τίτλο του προγράμματος.
					Call hTitlos_P.Initialize("
							SELECT
							   @trim( TITLOS )
							INTO
							   :sLectiko
							FROM METPROGRAM
							WHERE
							      METPROGRAM.PROGCODE = :nWPROGCODE  AND   METPROGRAM.TMIMACODE =  :nWTMIMACODE " )
					If NOT hTitlos_P.Prepare()
						Return FALSE
					!
					! If NOT hWeb.Next( nFetch )
						Return FALSE
					Call hWeb.Next( nFetch )
					!
					!
					Set sWriteBuffer = ''
					Set sLectiko = ''
					!
					! Πάρε και επεξεργάσου όλα τα ROWS του METKARTL_WEB
					While nFetch != FETCH_EOF
						!
						! TOM
						If nCountPerc < nCountST_F_WEB
							Set nCountPerc = nCountPerc + nDivST_F_WEB
						Else
							Set nPerc = nPerc + 5
							Set nCountPerc = nDivST_F_WEB
							Call SalSendMsg( Progress_Meter, SAM_Create, 0, 0 )
						! TOM
						! Set sSN_ROW =  SalStrMidX(sROWID,28,8)
						! Set sUPDT_ROW = SalStrMidX(sROWID,8,10)
						Call Calculate_Modifications( sMODIF_TYPE )
						!
						Set sDateModified = SalFmtFormatDateTime( dDATE_MODIF, 'dd/MM/YYYY' )
						!
						! Set sWriteBuffer = sMODIF_TYPE || ';' || SalStrLeftX( sDateModified, 10 )
						Set sWriteBuffer = sMODIF_TYPE || '~' || Define_Date( dDATE_MODIF ) ||'~'||SalNumberToStrX( nWMETKARTL_SEQNO, 0 ) 
						Set sWriteBuffer = sWriteBuffer || '~' ||sWMITROO ||'~'|| SalNumberToStrX(  nWTMIMACODE, 0 ) 
						!
						Set sLectiko = ''
						If NOT hTitlos_P.Execute()
							Return FALSE
						Call hTitlos_P.Next(nFetch2)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko ||'~'||SalNumberToStrX(nWAK,0)
						!
						Set sLectiko = ''
						Set sWValue = sWPERIOD
						Set sPlCol = 'METTERM'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko
						!
						! @NEO ΠΡΟΣΤΕΘΗΚΕ ΚΑΙ Ο ΚΩΔΙΚΟΣ ΤΟΥ ΜΑΘΗΜΑΤΟΣ!
						Set sWriteBuffer = sWriteBuffer ||'~'|| SalNumberToStrX(nWCOURCODE,0)
						!
						Set sLectiko = ''
						If NOT hTitlos_M.Execute()
							Return FALSE
						Call hTitlos_M.Next(nFetch3)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko
						!
						Set sLectiko = ''
						Set sWValue = sWTYPOS
						Set sPlCol = 'METTYPOS'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						If nWGRADENOR > nWGRADENOR - SalNumberMod ( nWGRADENOR, 1 )
							Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko || '~' || SalNumberToStrX( nWGRADENOR, 2 )
						Else
							Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko || '~' || SalNumberToStrX( nWGRADENOR, 0 )
						!
						If nWGRADEPAN > nWGRADEPAN - SalNumberMod ( nWGRADEPAN, 1 )
							Set sWriteBuffer = sWriteBuffer ||'~'|| SalNumberToStrX( nWGRADEPAN , 2 )
						Else
							Set sWriteBuffer = sWriteBuffer ||'~'|| SalNumberToStrX( nWGRADEPAN , 0 )
						!
						Set sLectiko = ''
						Set sWValue = sWAPALAG
						Set sPlCol = 'NAIOXI'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko
						!
						Set sLectiko = ''
						Set sWValue = sWPERIODANIKI
						Set sPlCol = 'METTERM'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko
						!
						If VisFileWriteString( hWriteFile, sWriteBuffer ) != VTERR_Ok
							Call SalMessageBox( 'An error has occured during  writing the ASCII file ' || sFile ,'' , MB_Ok )
							If VisFileOpen( hWriteFile , sFile , OF_Exist )
								Call VisFileOpen( hWriteFile , sFile , OF_Delete )
							Return FALSE
						!
						Call hWeb.Next(nFetch)
						Set sWriteBuffer = ''
					! TOM
					Set nPerc = 100
					Call SalSendMsg( Progress_Meter, SAM_Create, 0, 0 )
					! TOM
					!
					! Βάλε μία επιπλέον γραμμή που περιέχει τον αριθμό των κινήσεων  Insert , Update , Delete που υπάρχουν στον METKARTL_WEB
					Set sWriteBuffer = 'c' || '~' || SalNumberToStrX( nCountInsert, 0 ) || '~' || SalNumberToStrX( nCountUpdate, 0 ) || '~' || SalNumberToStrX( nCountDelete, 0 )
					! 15 '~'
					Set sWriteBuffer = sWriteBuffer || '~~~~~~~'
					!
					! Γράψε την γραμμή
					If VisFileWriteString( hWriteFile, sWriteBuffer ) != VTERR_Ok
						Call SalMessageBox( 'An error has occured during  writing the ASCII file ' || sFile ,'' , MB_Ok )
						If VisFileOpen( hWriteFile , sFile , OF_Exist )
							Call VisFileOpen( hWriteFile , sFile , OF_Delete )
						Return FALSE
					!
					! Κλείσε το αρχείο
					Set hCloseFile = hWriteFile
					If VisFileClose( hCloseFile ) != VTERR_Ok
						Call SalMessageBox( 'An error has occured during  closing the ASCII file ' || sFile , '' , MB_Ok )
						If VisFileOpen( hWriteFile , sFile , OF_Exist )
							Call VisFileOpen( hWriteFile , sFile , OF_Delete )
						Return FALSE
					!
					If hWeb.bConnected
						!
						Call hWeb.Disconnect()
					If hWeb_Lect.bConnected
						!
						Call hWeb_Lect.Disconnect()
					If hTitlos_M.bConnected
						!
						Call hTitlos_M.Disconnect()
					If hTitlos_P.bConnected
						!
						Call hTitlos_P.Disconnect()
					Return TRUE
			Function: Delete_Web_Tables
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
				Actions
					!
					If NOT SqlImmediate( ' DELETE FROM MET_WEB ' )
						Return FALSE
					If NOT SqlImmediate( ' DELETE FROM METKARTL_WEB ' )
						Return FALSE
					If NOT SqlImmediate( ' DELETE FROM METSTUPROG_WEB ' )
						Return FALSE
					Return TRUE
			Function: Web_Process
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
				Actions
					!
					! Για τους Ναυπηγούς ισχύει αυτόματη μεταφορά των ASCII αρχείων στο μηχάνημα του Κ.Υ. που υπάρχει στη γραμματεία
					! Για τις άλλες γραμματείες δεν ισχύει το παραπάνω.
					!
					!
					! If nCodeTmima = 8 or nCodeTmima = 3 or nCodeTmima = 2 or nCodeTmima  = 7
						! Για τους Ναυπηγούς.
						! Αν υπάρχουν ανεπεξεργαστα αρχεία στο μηχάνημα του Κ.Υ. σταμάτα την διαδικασία.
						If NOT  ASCII_Exists_On_Other_Machine()
							!
							! Set dfMessage = 'Παρακαλώ περιμένετε ...'
							!
							! ΤΟΜ
							Set dfMessage = 'Διαδικασία αποθήκευσης των πινάκων. Παρακαλώ περιμένετε ...'
							! ΤΟΜ
							If NOT Unload_Web_Tables(  )
								Call SalMessageBox( 'Αδυναμία εκτέλεσης UNLOAD πινάκων που αφορούν την λειτουργία!' , '' , MB_Ok )
								Return FALSE
							!
							! Γράψε τα σταθερά στοιχεία
							If NOT Write_Met_Web_Data(  )
								Call SalMessageBox( 'Λάθος κατά την επεξεργασία σταθερών στοιχείων!' , '' , MB_Ok )
								Return FALSE
							!
							! Γράψε τις βαθμολογίες
							If NOT Write_Metkartl_Web_Data(  )
								Call SalMessageBox( 'Λάθος κατά την επεξεργασία βαθμολογιών!' , '' , MB_Ok )
								Return FALSE
							!
							! ΤΟΜ
							Set dfMessage = 'Καθαρισμός των πινάκων...'
							! ΤΟΜ
							! Αντέγραψε τα ASCII αρχεία που παράγονται στο μηχάνημα του Κ.Υ.
							If NOT Copy_Files_To_Other_Machine()
								Call SalMessageBox( ' Η διαδικασία αντιγραφής των αρχείων ASCII στο μηχάνημα του Κ.Υ. απέτυχε ! ' , '' , MB_Ok )
								Return FALSE
							! Σβήσε τα περιεχόμενα των πινάκων της βάσης MET_WEB και  METKARTL_WEB
							If NOT Delete_Web_Tables(  )
								Call SalMessageBox( 'Αδυναμία καθαρισμού πινάκων MET_WEB και  METKARTL_WEB !' , '' , MB_Ok )
								Return FALSE
							!
							Return TRUE
						Else
							!
							Call SalMessageBox( 'Υπάρχουν  μη επεξεργασμένα ASCII αρχεία στη μηχανή του Κ.Υ.! Παρακαλώ επαναλάβετε τη διαδικασία αργότερα. ' , '' , MB_Ok )
							Return FALSE
					! Else
						!
						! Για τις άλλες γραμματείες.
						! Set dfMessage = 'Παρακαλώ περιμένετε ...'
						!
						! ΤΟΜ
						Set dfMessage = 'Διαδικασία αποθήκευσης των πινάκων. Παρακαλώ περιμένετε ...'
						! ΤΟΜ
						If NOT Unload_Web_Tables(  )
							Call SalMessageBox( 'Αδυναμία εκτέλεσης UNLOAD πινάκων που αφορούν την λειτουργία!' , '' , MB_Ok )
							! Set nCountFiles = nCountFiles - 1
							! Call VisFileRename( sPath2, sPath1)
							Return FALSE
						! Γράψε τα σταθερά στοιχεία
						If not Write_Met_Web_Data(  )
							Call SalMessageBox( 'Λάθος κατά την επεξεργασία σταθερών στοιχείων!' , '' , MB_Ok )
							Return FALSE
						!
						! Γράψε τις βαθμολογίες
						If not Write_Metkartl_Web_Data(  )
							Call SalMessageBox( 'Λάθος κατά την επεξεργασία βαθμολογιών!' , '' , MB_Ok )
							Return FALSE
						!
						! Σβήσε τα περιεχόμενα των πινάκων της βάσης MET_WEB και  METKARTL_WEB
						! ΤΟΜ
						Set dfMessage = 'Καθαρισμός των πινάκων...'
						! ΤΟΜ
						If NOT Delete_Web_Tables(  )
							Call SalMessageBox( 'Αδυναμία καθαρισμού πινάκων MET_WEB και  METKARTL_WEB !' , '' , MB_Ok )
							Return FALSE
						Return TRUE
					If nCodeTmima = 2
						Set sPathU = dfPathUnload
					! Αν υπάρχουν ανεπεξεργαστα αρχεία στο μηχάνημα του Κ.Υ. σταμάτα την διαδικασία.
					If NOT  ASCII_Exists_On_Other_Machine()
						!
						! Set dfMessage = 'Παρακαλώ περιμένετε ...'
						!
						! ΤΟΜ
						Set dfMessage = 'Διαδικασία αποθήκευσης των πινάκων. Παρακαλώ περιμένετε ...'
						! ΤΟΜ
						If NOT Unload_Web_Tables(  )
							Call SalMessageBox( 'Αδυναμία εκτέλεσης UNLOAD πινάκων που αφορούν την λειτουργία!' , '' , MB_Ok )
							Return FALSE
						!
						! Γράψε τα σταθερά στοιχεία
						If NOT Write_Met_Web_Data(  )
							Call SalMessageBox( 'Λάθος κατά την επεξεργασία σταθερών στοιχείων!' , '' , MB_Ok )
							Return FALSE
						!
						! Γράψε τις βαθμολογίες
						If NOT Write_Metkartl_Web_Data(  )
							Call SalMessageBox( 'Λάθος κατά την επεξεργασία βαθμολογιών!' , '' , MB_Ok )
							Return FALSE
						!
						! Γράψε τις ημερομηνίες αναγορεύσεων
						!
						! Γράψε τα προγράμματα των φοιτητών
						If NOT Write_Metstuprog_Web_Data(  )
							Call SalMessageBox( 'Λάθος κατά την επεξεργασία των ημερομηνιών αναγορεύσεων!' , '' , MB_Ok )
							Return FALSE
						! ΤΟΜ
						Set dfMessage = 'Καθαρισμός των πινάκων...'
						! ΤΟΜ
						! Αντέγραψε τα ASCII αρχεία που παράγονται στο μηχάνημα του Κ.Υ.
						If NOT Copy_Files_To_Other_Machine()
							Call SalMessageBox( ' Η διαδικασία αντιγραφής των αρχείων ASCII στο μηχάνημα του Κ.Υ. απέτυχε ! ' , '' , MB_Ok )
							Return FALSE
						! Σβήσε τα περιεχόμενα των πινάκων της βάσης MET_WEB και  METKARTL_WEB
						If NOT Delete_Web_Tables(  )
							Call SalMessageBox( 'Αδυναμία καθαρισμού πινάκων MET_WEB, METKARTL_WEB και METSTUPROG_MET !' , '' , MB_Ok )
							Return FALSE
						!
						Return TRUE
					Else
						!
						Call SalMessageBox( 'Υπάρχουν  μη επεξεργασμένα ASCII αρχεία στη μηχανή του Κ.Υ.! Παρακαλώ επαναλάβετε τη διαδικασία αργότερα. ' , '' , MB_Ok )
						Return FALSE
			Function: ASCII_Exists_On_Other_Machine
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					Number: nFetch
				Actions
					If NOT hWeb.bConnected
						!
						Call hWeb.Connect()
					!
					! Πάρε τα ονόματα των ASCII αρχείων που μεταφέρθηκαν την τελευταία φορά στο μηχάνημα του Κ.Υ. από τον πίνακα WEB_FILE_MET
					! Ο ΠΙΝΑΚΑΣ WEB_FILE_MET ΠΕΡΙΕΧΕΙ ΤΑ ΟΝΟΜΑΤΑ ΤΩΝ ASCII ΑΡΧΕΙΩΝ ΠΟΥ ΜΕΤΑΦΕΡΘΗΚΑΝ ΣΤΟ ΜΗΧΑΝΗΜΑ ΤΟΥ Κ.Υ. ΚΑΙ
					! ΠΕΡΙΕΧΕΙ ΠΑΝΤΑ ΜΙΑ ΕΓΓΡΑΦΗ.
					Call hWeb.Initialize('
							SELECT
							filename1 , filename2 ,filename3
							INTO
							   :sFile_On_Other_Machine_1 , :sFile_On_Other_Machine_2, :sFile_On_Other_Machine_3
							FROM WEB_FILE_MET' )
					Call hWeb.Prepare()
					Call hWeb.Execute()
					Call hWeb.Next( nFetch )
					!
					If nFetch != FETCH_EOF
						!
						! Δες αν υπάρχουν τα  ASCII αρχεία στο άλλο μηχάνημα.
						! Αν υπάρχουν επέστρεψε FALSE για να σταματήσει η όλη διαδικασία
						Set sFile_On_Other_Machine_1 = sPath_Of_Other_Machine || sFile_On_Other_Machine_1
						!
						Set sFile_On_Other_Machine_2 = sPath_Of_Other_Machine || sFile_On_Other_Machine_2
						!
						Set sFile_On_Other_Machine_3 = sPath_Of_Other_Machine || sFile_On_Other_Machine_3
						!
						If NOT VisFileOpen( hWriteFile, sFile_On_Other_Machine_1, OF_Read )  = VTERR_Ok
							If NOT VisFileOpen( hWriteFile, sFile_On_Other_Machine_2, OF_Read ) = VTERR_Ok
								If NOT VisFileOpen( hWriteFile, sFile_On_Other_Machine_3, OF_Read ) = VTERR_Ok
									Return FALSE
						!
						Return TRUE
					Else
						Return FALSE
			Function: Copy_Files_To_Other_Machine
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
				Actions
					!
					! Κόπιαρε τα αρχεία του δίσκου στο  μηχάνημα του Κ.Υ. . Αν αποτύχει σε κάποιο τότε επέστρεψε FALSE
					If nCodeTmima = 2
						! Σ Η Μ Ε Ι Ω Σ Η !!!
						! Διαφορετική επιλογή Path γιατί η διαδικασία εκτελείται από μηχάνημα Client
						! Για να δουλέψει σωστά από μηχάνημα Client πρέπει να έχει συμπληρωθεί στο πεδίο 'ΟΝΟΜΑ ΜΗΧΑΝΗΜΑΤΟΣ' το όνομα του Server,
								ο φάκελος web στον Server πρέπει να είναι Shared για τον Client με δικαιώματα Write Delete και επιπλέον στο Server και στον Client
								να έχει γίνει map drive με το γράμμα Z ο φάκελος του Web Server που προορίζεται για την αποθήκευση των αρχείων.
						If NOT  FILE_CopyOK = SalFileCopy( '\\\\' || sPathU || '\\web\\' || sFile_Stathera ,sPath_Of_Other_Machine || sFile_Stathera , TRUE )
							Return FALSE
						If NOT  FILE_CopyOK = SalFileCopy( '\\\\' || sPathU || '\\web\\' || sFile_Kartella ,sPath_Of_Other_Machine|| sFile_Kartella , TRUE )
							Return FALSE
						If NOT  FILE_CopyOK = SalFileCopy( '\\\\' || sPathU || '\\web\\' || sFile_StuProg ,sPath_Of_Other_Machine|| sFile_StuProg , TRUE )
							Return FALSE
						! If NOT  FILE_CopyOK = SalFileCopy( '\\\\demon\\web\\' || sFile_Stathera , 'z:\\' || sFile_Stathera , TRUE )
							Return FALSE
						! If NOT  FILE_CopyOK = SalFileCopy( '\\\\demon\\web\\' || sFile_Kartella , 'z:\\' || sFile_Kartella , TRUE )
							Return FALSE
					Else
						! Για τις γραμματείες που η διαδικασία εκτελείται από τον Server
						If NOT  FILE_CopyOK = SalFileCopy( 'C:\\NTUARIS\\WEB\\' || sFile_Stathera , sPath_Of_Other_Machine|| sFile_Stathera , TRUE )
							Return FALSE
						If NOT  FILE_CopyOK = SalFileCopy( 'C:\\NTUARIS\\WEB\\' || sFile_Kartella , sPath_Of_Other_Machine|| sFile_Kartella , TRUE )
							Return FALSE
						If NOT  FILE_CopyOK = SalFileCopy( 'C:\\NTUARIS\\WEB\\' || sFile_StuProg , sPath_Of_Other_Machine|| sFile_StuProg , TRUE )
							Return FALSE
					!
					! Σβήσε τα ονόματα των  αρχείων που μεταφέρθηκαν  στο  μηχάνημα  του Κ.Υ. από τον πίνακα WEB_FILE_MET
					If NOT hWeb.bConnected
						!
						Call hWeb.Connect()
					!
					Call hWeb.Initialize('
							DELETE
							FROM  WEB_FILE_MET ' )
					Call hWeb.Prepare()
					Call hWeb.Execute()
					Call hWeb.Commit()
					!
					! Βάλε τα ονόματα των αρχείων που μεταφέρθηκαν  στο  μηχάνημα  του Κ.Υ στον πίνακα της βάσης WEB_FILE_MET
					Call hWeb.Initialize('
							INSERT
							INTO WEB_FILE_MET
							( FILENAME1 , FILENAME2, FILENAME3)
							VALUES (  :sFile_Stathera , :sFile_Kartella, :sFile_StuProg ) ' )
					Call hWeb.Prepare()
					Call hWeb.Execute()
					Call hWeb.Commit()
					If hWeb.bConnected
						!
						Call hWeb.Disconnect()
					Return TRUE
			Function: Write_Metstuprog_Web_Data
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					Number: nFetch
					Number: nFetch1
					String: sExtension
				Actions
					Set sFile = ''
					!
					! TOM
					Set dfMessage = 'Συνεχίζεται η διαδικασία με το αρχείο των προγραμμάτων των φοιτητών...'
					Set nCountPerc = NUMBER_Null
					Set nPerc = NUMBER_Null
					Set nDivST_F_WEB = NUMBER_Null
					Call SqlImmediate (' Select count(*) from METSTUPROG_WEB into :nCountST_F_WEB ')
					Set nCountST_F_WEB = (5 * nCountST_F_WEB)/100
					If nCountST_F_WEB >= 1
						Set nDivST_F_WEB = (nCountST_F_WEB)/SalNumberTruncate(nCountST_F_WEB,6,0)
					! TOM
					! Προσδιόρισε το όνομα του αρχείου
					Set sFile_StuProg = Define_FileName_Plus_Date_Hour( 'met_stuprog_' )
					If nCodeTmima = 2
						! Set sFile = Define_FileName_Plus_Date_Hour( '\\\\' || sPathU || '\\web\\met_stuprog_' )
						Set sFile = '\\\\' || sPathU || '\\web\\' || sFile_StuProg
					Else
						! Set sFile = Define_FileName_Plus_Date_Hour( 'c:\\george\\web\\met_stuprog_' )
						Set sFile = 'C:\\NTUARIS\\WEB\\' || sFile_StuProg
					!
					! ’νοιξε το αρχείο
					If VisFileOpen( hWriteFile , sFile , OF_Create | OF_Text ) =  VTERR_Ok
						!
						! Call VisFileSetAttribute(sFile,FA_Standard, TRUE )
						Call SalMessageBeep( -1 )
						! Call SalFileOpen( hWriteFile, sFile, OF_ReadWrite )
						Call VisFileOpen( hWriteFile , sFile , OF_Create | OF_Write | OF_Text )
					Else
						Call SalMessageBox( 'An error has occured during opening the  ASCII file ' , '' , MB_Ok )
						Return FALSE
					!
					If NOT hWeb.bConnected
						!
						If NOT  hWeb.Connect()
							Return FALSE
					If NOT hTitlos_P.bConnected
						!
						If NOT  hTitlos_P.Connect()
							Return FALSE
					!
					! Αρχικοποίσε τον αριθμό των Insert , Update , Delete
					Set nCountInsert = 0
					Set nCountUpdate = 0
					Set nCountDelete = 0
					!
					! Το παρακάτω SELECT παίρνει ένα ROW  από τον METSTUPROG_WEB
					Call hWeb.Initialize('
							SELECT
							    MODIFTYPE, DATEMODIF, MITROO, PROGCODE,TOTBAU,DATEANAG,METSTUPROG_SEQNO
							INTO
							    :sMODIF_TYPE,  :dDATE_MODIF , :sWMITROO,:nWPROGCODE, :nWTOTBAU, :dWDATEANAG,:nWMETSTUPROG_SEQNO
							FROM METSTUPROG_WEB
							ORDER BY  DATEMODIF ' )
					If NOT hWeb.Prepare()
						Return FALSE
					If NOT  hWeb.Execute()
						Return FALSE
					!
					! Το παρακάτω SELECT παίρνει τον τίτλο του προγράμματος.
					Call hTitlos_P.Initialize("
							SELECT
							   @trim( TITLOS )
							INTO
							   :sLectiko
							FROM METPROGRAM
							WHERE
							      METPROGRAM.PROGCODE = :nWPROGCODE  AND   METPROGRAM.TMIMACODE =  :nCodeTmima " )
					If NOT hTitlos_P.Prepare()
						Return FALSE
					! If NOT hWeb.Next( nFetch )
						Return FALSE
					Call hWeb.Next( nFetch )
					!
					!
					Set sWriteBuffer = ''
					Set sLectiko = ''
					!
					! Πάρε και επεξεργάσου όλα τα ROWS του METSTUPROG_WEB
					While nFetch != FETCH_EOF
						!
						! TOM
						If nCountPerc < nCountST_F_WEB
							Set nCountPerc = nCountPerc + nDivST_F_WEB
						Else
							Set nPerc = nPerc + 5
							Set nCountPerc = nDivST_F_WEB
							Call SalSendMsg( Progress_Meter, SAM_Create, 0, 0 )
						! TOM
						! Set sSN_ROW =  SalStrMidX(sROWID,28,8)
						! Set sUPDT_ROW = SalStrMidX(sROWID,8,10)
						Call Calculate_Modifications( sMODIF_TYPE )
						!
						Set sDateModified = SalFmtFormatDateTime( dDATE_MODIF, 'dd/MM/YYYY' )
						!
						! Set sWriteBuffer = sMODIF_TYPE || ';' || SalStrLeftX( sDateModified, 10 )
						Set sWriteBuffer = sMODIF_TYPE || '~' || Define_Date( dDATE_MODIF ) ||'~'||SalNumberToStrX( nWMETSTUPROG_SEQNO, 0 ) 
						Set sWriteBuffer = sWriteBuffer || '~' ||sWMITROO
						!
						Set sLectiko = ''
						If NOT hTitlos_P.Execute()
							Return FALSE
						Call hTitlos_P.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko 
						Set sWriteBuffer = sWriteBuffer ||'~'|| SalNumberToStrX( nWTOTBAU, 2 )||'~'||Define_Date( dWDATEANAG )
						!
						If VisFileWriteString( hWriteFile, sWriteBuffer ) != VTERR_Ok
							Call SalMessageBox( 'An error has occured during  writing the ASCII file ' || sFile ,'' , MB_Ok )
							If VisFileOpen( hWriteFile , sFile , OF_Exist )
								Call VisFileOpen( hWriteFile , sFile , OF_Delete )
							Return FALSE
						!
						Call hWeb.Next(nFetch)
						Set sWriteBuffer = ''
					! TOM
					Set nPerc = 100
					Call SalSendMsg( Progress_Meter, SAM_Create, 0, 0 )
					! TOM
					!
					! Βάλε μία επιπλέον γραμμή που περιέχει τον αριθμό των κινήσεων  Insert , Update , Delete που υπάρχουν στον METKARTL_WEB
					Set sWriteBuffer = 'c' || '~' || SalNumberToStrX( nCountInsert, 0 ) || '~' || SalNumberToStrX( nCountUpdate, 0 ) || '~' || SalNumberToStrX( nCountDelete, 0 )
					! 15 '~'
					Set sWriteBuffer = sWriteBuffer 
					!
					! Γράψε την γραμμή
					If VisFileWriteString( hWriteFile, sWriteBuffer ) != VTERR_Ok
						Call SalMessageBox( 'An error has occured during  writing the ASCII file ' || sFile ,'' , MB_Ok )
						If VisFileOpen( hWriteFile , sFile , OF_Exist )
							Call VisFileOpen( hWriteFile , sFile , OF_Delete )
						Return FALSE
					!
					! Κλείσε το αρχείο
					Set hCloseFile = hWriteFile
					If VisFileClose( hCloseFile ) != VTERR_Ok
						Call SalMessageBox( 'An error has occured during  closing the ASCII file ' || sFile , '' , MB_Ok )
						If VisFileOpen( hWriteFile , sFile , OF_Exist )
							Call VisFileOpen( hWriteFile , sFile , OF_Delete )
						Return FALSE
					!
					If hWeb.bConnected
						!
						Call hWeb.Disconnect()
					If hTitlos_P.bConnected
						!
						Call hTitlos_P.Disconnect()
					Return TRUE
