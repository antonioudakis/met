					!
					Set nRepType = nRep
					Select Case nRep
						Case CATALOG_Report
							!
							!
							Set nErr = 1
							Set sQrpPath = sDriver || "\\NTUARIS\\EKDOSEIS\\REPORTS\\MQRP\\catpl.QRP"
							! Set sQrpPath = sQrpPath || 'catpl.qrp'
							Set sTitle =  dfPltext
