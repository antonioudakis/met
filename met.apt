												Call SqlPrepare ( hUpdtBonus, ' UPDATE ST_BONUS SET GETBONUS = :nGetPistop + 1 WHERE  AK = :nAkTrexon and K_F = :dfStf ' )
											Else 
												Call SqlPrepare ( hUpdtBonus, ' UPDATE ST_BONUS SET GETBONUS = :nGetPistop + 1 WHERE  AK = :nAkTrexon and K_F = :dfStf ' )
