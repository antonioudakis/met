										! Set s3dPar_B = 'ΤΟ ΠΙΣΤΟΠΟΙΗΤΙΚΟ ΕΚΔΙΔΕΤΑΙ ΓΙΑ ' || SalNumberToStrX(nGetPistop + 1, 0) || 'η ΦΟΡΑ ΑΠΟ ΤΟ ΙΔΡΥΜΑ ΜΑΣ ΚΑΤΑ ΤΟ ΤΡΕΧΟΝ ΑΚΑΔΗΜΑΙΚΟ ΕΤΟΣ !'
										Set nGetPistop = nGetPistop + 1
										Call SqlPrepare ( hInsBonus, ' INSERT INTO ST_BONUS (K_TM, K_F, AK, GETBONUS) VALUES (:nCodeTmima, :dfStf, :nAkTrexon, :nGetPistop) ' )
										Call SqlExecute (hInsBonus)
										Call SqlCommit(hInsBonus)
										Call SqlDisconnect(hInsBonus)
										Set bPrintRep = FALSE
										Set bPrintReportOk = TRUE
