								! If sCodePistop = '09'
									If frmEgrafi.cmbStatus.GetSelection() = '05'
										If NOT CheckAit_Diag()
											!
											Return FALSE
									Set frmEgrafi.bTropos = TRUE
									Set frmEgrafi.bOnlyOneYear = TRUE
									Set frmEgrafi.bnotMO = TRUE
									Call GetTitlos()
									Call GetTrexon()
									Set nSign = 1
									Call SignText( nSign, sSignText, sOrderBy, sGIFRA )
									Call SalSendMsg( frmEgrafi, PAM_Report, BASICSU_Report, 0 )
									Set bPrintReportOk = TRUE
									!
								!
								! πιστοπ. τελειοφοίτου χωρίς τρόπο εισαγωγής
								If sCodePistop = '10'
									If frmEgrafi.cmbStatus.GetSelection() = '05'
										If NOT CheckAit_Diag()
											!
