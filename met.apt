			Function: InsertMetSynerg
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					If NOT hInsSyn.bConnected
						!
						Call hInsSyn.Connect()
					Call SalTblGetColumnText( tblProgAStu, 2, sProgramAna )
					Call hInsSyn.Initialize( ' INSERT INTO METDSYNERG
							 ( TMIMACODE, CODESYN, MITROO, PROGCODE ) VALUES
							(:nCodeTmima, :nInsCodeSyn, :dfStudCodeDipl, :sProgramAna)' )
					Call hInsSyn.Prepare()
					Call hInsSyn.Execute()
					Call hInsSyn.Commit()
			Function: GetCodeSyn
				Description:
				Returns
					Number:
				Parameters
				Static Variables
				Local variables
					String: sSelect
					Number: nFetch
				Actions
					Set nCodeSyn = 0
					Call SalTblGetColumnText( tblProgAStu, 2, sProgramAna )
					If NOT hGetSyn.bConnected
						!
						Call hGetSyn.Connect()
					Set sSelect = 'SELECT codesyn from METDSYNERG  INTO :nCodeSyn 
							where mitroo = :dfStudCodeDipl and progcode = :sProgramAna'
					Call hGetSyn.Initialize( sSelect )
					Call hGetSyn.Prepare()
					Call hGetSyn.Execute()
					Call hGetSyn.Next( nFetch )
					! Call hInsSyn.Commit()
					Return nCodeSyn
			Function: DeleteDiplSynerg
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					!
					If NOT hInsSyn.bConnected
						!
						Call hInsSyn.Connect()
					Set nN = 0
					Call hInsSyn.Initialize( ' SELECT COUNT(CODESYN) FROM METDSYNERG INTO :nN
							where CODESYN = :nCodeSyn   '  )
					Call hInsSyn.Prepare()
					Call hInsSyn.Execute()
					Call hInsSyn.Next( nFetch )
					If nN = 1
						Call hInsSyn.Initialize( ' DELETE FROM METDIPLGRADE
								where  CODESYN = :nCodeSyn  ' )
						Call hInsSyn.Prepare()
						If NOT hInsSyn.Execute()
							Return FALSE
						Call hInsSyn.Commit()
					Call hInsSyn.Initialize( ' DELETE FROM METDSYNERG
							where MITROO = :dfStudCodeDipl  and CODESYN = :nCodeSyn  ' )
					Call hInsSyn.Prepare()
					! Call hInsSyn.Execute()
					If NOT hInsSyn.Execute()
						Return FALSE
					Call hInsSyn.Commit()
					Call hInsSyn.Initialize( ' DELETE FROM METANATHESI
							where MITROO = :dfStudCodeDipl  and CODESYN = :nCodeSyn  and TMIMACODE = :nCodeTmima'  )
					Call hInsSyn.Prepare()
					! Call hInsSyn.Execute()
					If NOT hInsSyn.Execute()
						Return FALSE
					Call hInsSyn.Commit()
					Call SalTblReset( tblAnathesi )
					Call SalTblReset( tblEpitropi )
					! ΑΛΛΑΓΗ ΤΟΥ STATUS AN XREIAZETAI
					! If
						                                                                                Set sSel = "Select STUSTATUS  FROM METSTUPROG  INTO :sStatusFoit
								WHERE MITROO = :dfStudCodeDipl and PROGCODE = :colStudAProgH"
						                                                                                Call SqlImmediate ( sSel )
						                                                                                If sStatusFoit = 'AN' 
							                                                                                Set sStatusFoit = STRING_Null
							                                                                                Call SqlImmediate (' UPDATE METSTUPROG SET STUSTATUS  = :sStatusFoit
									                                        WHERE MITROO = :dfStudCodeDipl and PROGCODE = :colStudAProgH')
							                                                                                When SqlError
								                                                                                Call SqlImmediate( 'ROLLBACK')
							                                                                                Call SqlImmediate( 'COMMIT')
							!
			! tom
			! Function: CheckStatus
				                                                                                  Description: 
				                                                                                  Returns 
				                                                                                  Parameters 
				                                                                                  Static Variables 
				                                                                                  Local variables 
					                                                                                  String: sSelect
					                                                                                  String: sStatusFoit
					                                                                                  Number: nCountAnag
					                                                                                  String: sDateeos
					                                                                                  String: sDateanag
					                                                                                  Boolean: bCancelStatus
				                                                                                  Actions 
					!
