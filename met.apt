			String: sOrigUnivDep
			FunctionalVar: hTitle
				Class: clsSqlHandleSelect
			FunctionalVar: hTitleTom
				Class: clsSqlHandleSelect
		Message Actions
			On SAM_Create
				! Initialize the class variables.
				Set sTabName = 'DEP'
				! Call InitializeClass('BASE', 'SYSADM','sysadm' )
				Call InitializeClass(BankName, df1,df2 )
				! Call InitializeClass('DISDB', sUser, sPass )
				! Time to connect the sql handle
				Call Connect()
				! Set DBP_PRESERVE to TRUE
				Call SetIsolationLevel('RL')
				Call SetParameter(DBP_PRESERVE, TRUE, '')
			On SAM_CreateComplete
				!
				! Call SalDestroyWindow( dlgInfo )
				Set frmdep.bDirtyData = FALSE
				Call SalPostMsg( hWndForm, PAM_NEWREC, 0, 0 )
				Call SalWaitCursor( FALSE )
			On SAM_Close
				!
				!
				If hTitle.bConnected
					!
					Call hTitle.Disconnect()
				If hTitleTom.bConnected
					!
					Call hTitleTom.Disconnect()
				Call SalSendMsg( hWndForm, PAM_CLOSE, 0, 0 )
				Call Disconnect()
				Call SalDestroyWindow(  hWndForm  )
			On PAM_Report
				!
				Call Report( wParam )
			On SAM_ReportStart
				!
				Return ReportStart( nRepType )
			On SAM_ReportFetchInit
				!
				Return ReportFetchInit( nRepType )
			On SAM_ReportFetchNext
				!
				Return ReportFetchNext( nRepType )
