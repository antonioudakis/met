									                                                                                                                                                          Call hInsSyn.Execute()
									                                                                                                                                                          Call hGetSyn.Next( nFetch )
								!
								                                                                                                                                                          Call hInsSyn.Disconnect()
								                                                                                                                                                          Call hGetSyn.Disconnect()
