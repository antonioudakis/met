				Message Actions 
					On SAM_Click
						!
						Call SalDisableWindow( colAkEgr )
						Call SalDisableWindow( colEgrafiEj )
						Call tblEgrafi.SaveTbl()
						Call SalSendMsg( tblEgrafi, PAM_POPULATE,0,0 )
						! Call SalDisableWindow( colXuEgrafi )
						! Call SalDisableWindow( colEgrafiKatLekt )
						! If colAkEgr != NUMBER_Null and colEgrafiEj != STRING_Null
							! If colEgrafiKat = '00'
								!
								  If NOT ChkKatEgr()
									!
									  If sKatPar = '00'
										! ΟΧΙ ΓΙΑ ΤΟΥΣ ΑΡΧΙΤΕΚΤΟΝΕΣ (ΔΕΝ ΕΧΟΥΝ ΚΑΤΕΥΘΥΝΣΕΙΣ) , ΟΥΤΕ ΓΙΑ ΤΙΣ ΔΙΑΓΡΑΦΕΣ
										  If nCodeTmima != 4 and NOT SalTblAnyRows(tblEgrafi, ROW_MarkDeleted,0 )
											  If ErrorMessageOk(' Πρέπει να επιλεγεί κατεύθυνση και να ενημερωθεί από τη φόρμα της Δήλωσης ! Ο φοιτητής υπόκειται σε ειδική περίπτωση; ','', 2)
												!
												! Call SalEnableWindow(cmbKatFoitEg)
											  Else 
												! Call SalDisableWindow(cmbKatFoitEg)
												  Call SalSendMsg(  pbCancel , SAM_Click , 0 , 0 )
												  Return FALSE
									  Else If sKatPar != '00' and NOT SalTblAnyRows(tblEgrafi, ROW_MarkDeleted,0 )
										  If ErrorMessageOk(' Πρέπει να επιλεγεί κατεύθυνση ! Ο φοιτητής υπόκειται σε ειδική περίπτωση; ','', 2)
											!
											! Call SalEnableWindow(cmbKatFoitEg)
										! ΟΧΙ ΓΙΑ ΤΟΥΣ ΑΡΧΙΤΕΚΤΟΝΕΣ (ΔΕΝ ΕΧΟΥΝ ΚΑΤΕΥΘΥΝΣΕΙΣ)
										! If nCodeTmima != 4
										  Else 
											! Call SalDisableWindow(cmbKatFoitEg)
											  Call SalSendMsg(  pbCancel , SAM_Click , 0 , 0 )
											  Return FALSE
								  Else If sKatEjFoit = STRING_Null and nCodeTmima != 4
									  Call SalSendMsg(  pbCancel , SAM_Click , 0 , 0 )
									  Return FALSE
							! If colEgrafiKat != '00'
								!
								  If ChkKatEgr() and NOT SalTblAnyRows(tblEgrafi, ROW_MarkDeleted,0 )
									!
									  If ErrorMessageOk(" Πρέπει να επιλεγεί κατεύθυνση 'ΜΗ ΟΡΙΣΜΕΝΗ' ! ",'', 1)
										!
										! Call SalSendMsg(  pbCancel , SAM_Click , 0 , 0 )
										  Call SalSendMsg(  pbCancel , SAM_Click , 0 , 0 )
										! Call SalSetFocus(colEgrafiKatLekt)
										  Return FALSE
										! Call SalEnableWindow(cmbKatFoitEg)
									! Else
										! Call SalDisableWindow(cmbKatFoitEg)
										  Call SalSendMsg(  pbCancel , SAM_Click , 0 , 0 )
										! !!!!
										! Call SalSendMsg(  pbCancel , SAM_Click , 0 , 0 )
										! Call SalSendMsg(  pbCancel , SAM_Click , 0 , 0 )
										! Call SalSendMsg(  pbAddtbl , SAM_Click , 0 , 0 )
										! !!!!
										! Set colEgrafiEj = STRING_Null
										! Set colEgrafiEjCode = STRING_Null
										! Call SalSetFocus(colEgrafiEj)
										! Return TRUE
								  Else If sKatEjFoit = STRING_Null and nCodeTmima != 4
									  Call SalSendMsg(  pbCancel , SAM_Click , 0 , 0 )
									  Return FALSE
							! If SalTblAnyRows(tblEgrafi, ROW_MarkDeleted,0 )
								Call CancelYpoxFoit()
								Call CancelEpilFoit()
								Call tblEgrafi.SaveTbl()
							! If SalTblAnyRows(tblEgrafi, ROW_New,0 )
								  If ColumnCheck(tblEgrafi,1,colAkEgr )
									  If nCodeTmima = 3 and colEgrafiEjCode > '05'
										!
										  Select Case nOpenedFrom
											  Case 1
												  Call frmEgrafi.YpoxrFoit3(colEgrafiEjCode, colAkEgr)
												  Break 
											  Case 2
												  Call frmDilosi.YpoxrFoit3(colEgrafiEjCode, colAkEgr)
												  Break 
											  Case 3
												  Call frmKartella.YpoxrFoit3(colEgrafiEjCode, colAkEgr)
												  Break 
										! Call YpoxrFoit3(colEgrafiEjCode, colAkEgr)
									  Else 
										  Call YpoxrFoit( colEgrafiEjCode , sKatEjFoit , colAkEgr )
									  Call tblEgrafi.SaveTbl()
								  Else 
									!
									  Return FALSE
							! If SalTblAnyRows(tblEgrafi, ROW_Edited,0 )
								!
								  Call tblEgrafi.SaveTbl()
							! Call tblEgrafi.Populate()
							! Call colEgrafiKatLekt.SetSelectionCol(tblEgrafi,colEgrafiKat)
							!
							! Call dlgEgrafi.tblYpoxreotikaEg.Populate()
							! Call dlgEgrafi.tblEpilogesEg.Populate()
						! Else
							  Call ErrorMessageOk( ' Παρακαλώ συμληρώστε τα απαραίτητα πεδία ! ', '', 1 )
							  Call SalSendMsg(  pbCancel , SAM_Click , 0 , 0 )
