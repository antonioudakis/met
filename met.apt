		Message Actions
			On SAM_Create
				Call SalCenterWindow( hWndForm )
				Call SqlConnect( LhSql )
				Call SqlConnect( MhSql )
				Call SalTblPopulate( tblDep, LhSql, "SELECT AM_DEP, EPVN, ONOMA FROM DEP ORDER BY 2,3,1 into :tblDep.am_dep, :tblDep.epvn, :tblDep.onoma",0 )
				Set sDep_Selected = STRING_Null
				Set nidiotita = 1
				Set cmbIdiotitaDep = 'ΕΠΙΒΛΕΠΩΝ'
				Set nStatusFoit = 1
				Set cmbStatusFoit = 'ΕΝΕΡΓΟΙ'
				Call SalDisableWindow( cmbStatusFoit )
				Set cbAllStudents = TRUE
				Call SalDisableWindow( cmbProgramFoit )
				Set cbAllPrograms = TRUE
				Set nProgramFoit = 0
				! Call cmbProgramFoit.SetSelection(tblEpitropi,colDepCodeEp)
				Set cbPrint_e_file = FALSE
				Set bPrint = FALSE
			On SAM_Close
				!
				If cmbIdiotitaDep = 'ΕΠΙΒΛΕΠΩΝ'
					Set nidiotita = 1
				Else If cmbIdiotitaDep = 'ΜΕΛΟΣ ΤΡΙΜΕΛΟΥΣ ΕΠΙΤΡΟΠΗΣ'
					Set nidiotita = 2
				Else If cmbIdiotitaDep = 'ΜΕΛΟΣ ΕΠΤΑΜΕΛΟΥΣ ΕΠΙΤΡΟΠΗΣ'
					Set nidiotita = 3
				Else
					Set nidiotita = 1
				If cbAllStudents = TRUE
					Set nStatusFoit = 0
				Else
					If cmbStatusFoit = 'ΕΝΕΡΓΟΙ'
						Set nStatusFoit = 1
					Else If cmbStatusFoit = 'ΔΙΑΓΡΑΦΕΝΤΕΣ'
						Set nStatusFoit = 2
					Else If cmbStatusFoit = 'ΑΝΑΓΟΡΕΥΜΕΝΟΙ'
						Set nStatusFoit = 3
					Else If cmbStatusFoit = 'ΛΙΜΝΑΖΟΝΤΕΣ'
						Set nStatusFoit = 4
					Else
						Set nStatusFoit = 0
					If cbAllPrograms = TRUE
						Set nProgramFoit = 0
					Else
						If cmbProgramFoit = STRING_Null
							Set nProgramFoit = 0
						Else
							Set nProgramFoit = SalStrToNumber( cmbProgramFoit.GetSelection())
				If cbAllPrograms = TRUE or cmbProgramFoit = STRING_Null
					Set nProgramFoit = 0
				Else
					Set nProgramFoit = SalStrToNumber( cmbProgramFoit.GetSelection() )
				Call SqlDisconnect( LhSql )
				Call SqlDisconnect( MhSql )
				Set bPrint_e_file = cbPrint_e_file
				If bPrint
					Call SalEndDialog(hWndForm, TRUE )
				Else
					Call SalEndDialog(hWndForm, FALSE )
