							If NOT bApplication_WEB_KY
								If SalMessageBox (' Είστε σίγουροι ότι θέλετε να προχωρήσετε στην εκτύπωση των πιστοποιητικών; ',
										 'Προειδοποίηση' , MB_YesNo | MB_IconExclamation ) = IDYES
									Set bMultiplePrints = TRUE
									Set nPrinted = 0
									If bFrmEgrCreate
										Call SalDestroyWindow( frmEgrafi )
										Set bFrmEgrCreate = FALSE
									! Διάβασμα του αρχείου ή του πίνακα γραμμή γραμμή και εκκίνηση της διαδικασίας εκτύπωσης
									While frmWebAitisi.Tbl_GetColValue(tblWebAitisi,sAA,sCodeFoit,sCodePistop,sArPrvt, sCopies) = TRUE
										!
										! Boolean μηνυμάτων εκτύπωσης πριν από κάθε εκτύπωση γίνεται FALSE
										Set bMsgPrint = FALSE
										! Μήνυμα λάθους κατά την εκτύπωση. Πριν από κάθε εκτύπωση γίνεται STRING_Null
										Set sMsgPrint = STRING_Null
										! Αν η αίτηση έχει επιλεγεί για εκτύπωση συνέχισε ...
										If frmWebAitisi.tblWebAitisi.colStatusRep = 1
											! Βρες τη φόρμα που αντιστοιχεί το πιστοποιητικό
											Call MultiRep_Form(sCodePistop,sForm)
											! Εκτέλεσε την διαδικασία εκτύπωσης του πιστοποιητικού
											If Proc_PrintReport(sForm)
												Set nPrinted = nPrinted + 1
												! Μετά την εκτύπωση ενημέρωσε το status της συγκεκριμένης αίτησης ότι ολοκληρώθηκε
												! Εάν δεν υπάρχουν σχόλια ή υπάρχουν και πρόκειται
														 για τα 3 παρακάτω πιστοποιητικά τύπωσε ότι ολοκληρώθηκε
												If NOT bMsgPrint 
													Call UpdateWebAitisi(3)
											Set frmWebAitisi.dfAllAit = 'Εκτυπώθηκαν ' || SalNumberToStrX(nPrinted,0) || ' από ' || SalNumberToStrX(nPrintAit,0) || ' συνολικά. '
											If bApplication_WEB_KY
												Set frmSplash.dfMsgToUser = dfAllAit
									! Εάν οι εκτυπώσεις τελειώσουν κλείσε τις φόρμες
										Set sStatusRep = '2'
									If bFrmEgrCreate
										Call SalDestroyWindow(frmEgrafi)
										Set bFrmEgrCreate = FALSE
									Call SalSendMsg( tblWebAitisi ,  PAM_POPULATE, 0 , 0 )
							Else
								Set bMultiplePrints = TRUE
								Set nPrinted = 0
								If bFrmEgrCreate
									Call SalDestroyWindow( frmEgrafi )
									Set bFrmEgrCreate = FALSE
								! Διάβασμα του αρχείου ή του πίνακα γραμμή γραμμή και εκκίνηση της διαδικασίας εκτύπωσης
								While Tbl_GetColValue(tblWebAitisi,sAA,sCodeFoit,sCodePistop,sArPrvt, sCopies) = TRUE
									!
									! Boolean μηνυμάτων εκτύπωσης μετά από κάθε εκτύπωση γίνεται FALSE
									Set bMsgPrint = FALSE
									! Αν η αίτηση έχει επιλεγεί για εκτύπωση συνέχισε ...
									If frmWebAitisi.tblWebAitisi.colStatusRep = 1
										! Βρες τη φόρμα που αντιστοιχεί το πιστοποιητικό
										Call MultiRep_Form(sCodePistop,sForm)
										! Εκτέλεσε την διαδικασία εκτύπωσης του πιστοποιητικού
										If Proc_PrintReport(sForm)
											Set nPrinted = nPrinted + 1
											! Μετά την εκτύπωση ενημέρωσε το status της συγκεκριμένης αίτησης ότι ολοκληρώθηκε
											! Εάν δεν υπάρχουν σχόλια ή υπάρχουν και πρόκειται
													 για τα 3 παρακάτω πιστοποιητικά τύπωσε ότι ολοκληρώθηκε
											If NOT bMsgPrint OR ( bMsgPrint and ( sCodePistop = '2'  or  sCodePistop = '4' or sCodePistop = '6' ) )
												If NOT UpdateWebAitisi(3)
										Set dfAllAit = 'Εκτυπώθηκαν ' || SalNumberToStrX(nPrinted,0) || ' από ' || SalNumberToStrX(nPrintAit,0) || ' συνολικά. '
										If bApplication_WEB_KY
											Set frmSplash.dfMsgToUser = dfAllAit
								! Εάν οι εκτυπώσεις τελειώσουν κλείσε τις φόρμες
									Set sStatusRep = '2'
								If bFrmEgrCreate
									Call SalDestroyWindow(frmEgrafi)
									Set bFrmEgrCreate = FALSE
								If bFrmDiplCreate
									! Call SalDestroyWindow(frmDiplvmat)
									Set bFrmDiplCreate = FALSE
