							If SalMessageBox (' Είστε σίγουροι ότι θέλετε να προχωρήσετε στην ομαδική καταχώρηση ηλεκτρονικών εγγραφών; ',
									 'Προειδοποίηση' , MB_YesNo | MB_IconExclamation ) = IDYES
								Set bMultipleEgr = TRUE
								Set nEggrafes = 0
								! Διάβασμα του αρχείου ή του πίνακα γραμμή γραμμή και εκκίνηση της διαδικασίας αμαδικών εγγραφών
								Call SalTblSetFocusRow( tblWebKartl, 0 )
								While frmWebKartl.Tbl_GetColValue(tblWebKartl,sEgrTmimaCode,sEgrProgCode,sEgrMitroo, sEgrAk, sEgrPeriod, sEgrCourCode) = TRUE
									If bFrmDilosiCreate
										Call SalDestroyWindow( frmMetDilosi )
										Set bFrmDilosiCreate = FALSE
									!
									! Boolean μηνυμάτων εκτύπωσης πριν από κάθε εγγραφή γίνεται FALSE
									Set bMsgPrint = FALSE
									! Μήνυμα λάθους κατά την εγγραφή. Πριν από κάθε εγγραφή γίνεται STRING_Null
									Set sMsgPrint = STRING_Null
									! Αν η αίτηση έχει επιλεγεί για εγγραφή συνέχισε ...
									If tblWebKartl.colKStatusRep = 1
										! Εκτέλεσε την διαδικασία εγγραφής
										If frmWebKartl.Proc_PrintReport("frmMetDilosi")
											! Μετά την εκτύπωση ενημέρωσε το status της συγκεκριμένης αίτησης ότι ολοκληρώθηκε
											! Εάν δεν υπάρχουν σχόλια ύπωσε ότι ολοκληρώθηκε
											If NOT bMsgPrint 
												! Call UpdateWebAitisi(3)
												Set nEggrafes = nEggrafes + 1
												Set tblWebKartl.colKStatusLektAit = 'ολοκληρώθηκε'
											Else
												Set tblWebKartl.colKStatusLektAit = 'δεν δικαιούται'
												Set tblWebKartl.colKPistoReply = sMsgPrint
											Call SalSendMsg( colKStatusLektAit, SAM_Validate, 0, 0 )
										Set frmWebKartl.dfAllAit = 'Καταχωρήθηκαν ' || SalNumberToStrX(nEggrafes,0) || ' από ' || SalNumberToStrX(nPrintAit,0) || ' εγγραφές συνολικά. '
								! Εάν οι εκτυπώσεις τελειώσουν κλείσε τις φόρμες
									Set sStatusRep = '2'
								If bFrmDilosiCreate
									Call SalDestroyWindow(frmMetDilosi)
									Set bFrmDilosiCreate = FALSE
								Call SalSendMsg( tblWebKartl ,  PAM_SAVE, 0 , 0 )
								Call SalSendMsg( tblWebKartl ,  PAM_POPULATE, 0 , 0 )
