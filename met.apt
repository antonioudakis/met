						If nNum = 5.5
							Set sNumText = 'ΠΕΝΤΕ ΚΑΙ ΜΙΣΟ'
						If nNum = 6
							Set sNumText = 'ΕΞΙ'
						If nNum = 6.5
							Set sNumText = 'ΕΞΙ ΚΑΙ ΜΙΣΟ'
