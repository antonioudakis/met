					Else
						!
						Call SalEndDialog( dlgLogin,FALSE)
					! Γεμίζω τον πίνακα metprogat με τιμές για το τρέχων έτος αντλώντας τις από το τελευταίο έτος που έχουν ενημερωθεί και μόνο αν δεν υπάρχουν τιμές για το τρέχων έτος
					Call GetTrexon()
					If NOT hProgat.bConnected
						Call hProgat.Connect()
					Call hProgat.Initialize('Select PROGCODE FROM METPROGRAM into :nProgcode Where tmimacode = :nCodeTmima')
					Call hProgat.Prepare()
					Call hProgat.Execute()
					Call hProgat.Next(nFetch)
					While nFetch != FETCH_EOF
						Set nAk = NUMBER_Null
						Call SqlImmediate( 'Select ak from metprogat into :nAk where PROGCODE= :nProgcode and TMIMACODE =:nCodeTmima and ak = :nAkTrexon' )
						Call SqlClearImmediate(  )
						If nAk = NUMBER_Null
							Set nLimit = NUMBER_Null
							Set nLimitDipl = NUMBER_Null
							Set nwdipl = NUMBER_Null
							Set narcourdipl = NUMBER_Null
							Set sroundmo = STRING_Null
							Call SqlImmediate( 'Select limit,limitDipl,roundmo,wdipl,arcourdipl,mini,maxi,lectper,ypapof,minieng,maxieng from metprogat into :nLimit, :nLimitDipl, :sroundmo, :nwdipl, :narcourdipl, :sMINI, :sMAXI, :sLECTPER, :sYPAPOF, :sMAXIENG, :sMINIENG
									where PROGCODE= :nProgcode and TMIMACODE =:nCodeTmima order by ak desc' )
							Call SqlClearImmediate(  )
							Set sSelect = "Insert into metprogat values("||SalNumberToStrX( nCodeTmima, 0 )
							Set sSelect = sSelect||","||SalNumberToStrX( nProgcode, 0 )
							Set sSelect = sSelect||","||SalNumberToStrX( nAkTrexon, 0 )
							If nLimit = NUMBER_Null
								Set sSelect = sSelect||",NULL"
							Else
								Set sSelect = sSelect||","||SalNumberToStrX( nLimit, 2 )
								Set sSelect = SalStrLeftX( sSelect, SalStrLength( sSelect ) -3)||'.'||SalStrRightX( sSelect, 2)
							If nLimitDipl = NUMBER_Null
								Set sSelect = sSelect||",NULL"
							Else
								Set sSelect = sSelect||","||SalNumberToStrX( nLimitDipl, 2 )
								Set sSelect = SalStrLeftX( sSelect, SalStrLength( sSelect ) -3)||'.'||SalStrRightX( sSelect, 2)
							If sroundmo = STRING_Null
								Set sSelect = sSelect||",NULL"
							Else
								Set sSelect = sSelect||",'"||sroundmo||"'"
							If nwdipl = NUMBER_Null
								Set sSelect = sSelect||",NULL"
							Else
								Set sSelect = sSelect||","||SalNumberToStrX( nwdipl, 2 )
								Set sSelect = SalStrLeftX( sSelect, SalStrLength( sSelect ) -3)||'.'||SalStrRightX( sSelect, 2)
							If narcourdipl = NUMBER_Null
								Set sSelect = sSelect||",NULL"
							Else
								Set sSelect = sSelect||","||SalNumberToStrX( narcourdipl, 0 )
							If sMINI = STRING_Null
								Set sSelect = sSelect||",NULL"
							Else
								Set sSelect = sSelect||",'"||sMINI||"'"
							If sMAXI = STRING_Null
								Set sSelect = sSelect||",NULL"
							Else
								Set sSelect = sSelect||",'"||sMAXI||"'"
							If sLECTPER = STRING_Null
								Set sSelect = sSelect||",NULL"
							Else
								Set sSelect = sSelect||",'"||sLECTPER||"'"
							If sYPAPOF = STRING_Null
								Set sSelect = sSelect||",NULL"
							Else
								Set sSelect = sSelect||",'"||sYPAPOF||"'"
							If sMINIENG = STRING_Null
								Set sSelect = sSelect||",NULL"
							Else
								Set sSelect = sSelect||",'"||sMINIENG||"'"
							If sMAXIENG = STRING_Null
								Set sSelect = sSelect||",NULL"
							Else
								Set sSelect = sSelect||",'"||sMAXIENG||"'"
							Set sSelect = sSelect||")"
							If SalStrRightX( sSelect, 55 ) != "NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL)"
								Call SqlImmediate(sSelect)
								Call SqlClearImmediate(  )
						Call hProgat.Next(nFetch)
					Call hProgat.Disconnect()
