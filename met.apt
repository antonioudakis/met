			Function: Write_Met_Web_Data
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					Number: nFetch
					Number: nFetch1
					String: sExtension
				Actions
					Set sFile = ''
					! TOM
					Set dfMessage = 'Αρχίζει η διαδικασία με το αρχείο των σταθερών στοιχείων...'
					Set nCountPerc = NUMBER_Null
					Set nPerc = NUMBER_Null
					Set nDivST_F_WEB = NUMBER_Null
					Call SqlImmediate (' Select count(*) from MET_WEB into :nCountST_F_WEB ')
					Set nCountST_F_WEB = (5 * nCountST_F_WEB)/100
					If nCountST_F_WEB >= 1
						Set nDivST_F_WEB = (nCountST_F_WEB)/SalNumberTruncate(nCountST_F_WEB,6,0)
					! TOM
					!
					! Προσδιόρισε το όνομα του αρχείου
					Set sFile_Stathera = Define_FileName_Plus_Date_Hour( 'met_stathera_' )
					If nCodeTmima = 2
						! Set sFile = Define_FileName_Plus_Date_Hour( '\\\\' || sPathU || '\\web\\met_stathera_' )
						Set sFile = '\\\\' || sPathU || '\\web\\' || sFile_Stathera
					Else
						! Set sFile = Define_FileName_Plus_Date_Hour( 'c:\\george\\web\\met_stathera_' )
						Set sFile = 'C:\\NTUARIS\\WEB\\' || sFile_Stathera
					!
					! ’νοιξε το αρχείο
					If VisFileOpen( hWriteFile , sFile , OF_Create | OF_Text ) =  VTERR_Ok
						!
						! Call VisFileSetAttribute(sFile,FA_Standard, TRUE )
						Call SalMessageBeep( -1 )
						! Call SalFileOpen( hWriteFile, sFile, OF_ReadWrite )
						Call VisFileOpen( hWriteFile , sFile , OF_Create | OF_Write | OF_Text )
					Else
						!
						Call SalMessageBox( 'An error has occured during opening the  ASCII file ' || sFile ,'' , MB_Ok )
						Return FALSE
					!
					If NOT hWeb.bConnected
						!
						If NOT hWeb.Connect()
							Return FALSE
					!
					If NOT hWeb_Lect.bConnected
						!
						If NOT hWeb_Lect.Connect()
							Return FALSE
					!
					!
					! Αρχικοποίσε τον αριθμό των Insert , Update , Delete
					Set nCountInsert = 0
					Set nCountUpdate = 0
					Set nCountDelete = 0
					!
					! Το παρακάτω SELECT παίρνει ένα ROW  από τον MET_WEB
					Call hWeb.Initialize('
							SELECT
							  MODIFTYPE , DATEMODIF , MITROO ,  TMIMACODE ,  @TRIM(AR_T) , @TRIM(AMKA), @TRIM(EPVN) , @TRIM(ONOMA) , @TRIM(PATR) ,
							  @TRIM(MHTR) , @TRIM(SEX) , @TRIM(YPHK) , @TRIM(COORIG) ,  @TRIM(NOMOSORIG) , @TRIM(POLIORIG) , @TRIM(BIRTHDATE)  ,  @TRIM(NOMOSMHTR) ,
							 @TRIM(POLIMHTR) , @TRIM(DIMOTOLOGIO) , @TRIM(COKAT) ,  @TRIM(NOMOSKAT) , @TRIM(POLIKAT) ,   @TRIM(PERIOXI) , @TRIM(ODOS),
							@TRIM(TT) , @TRIM(TEL1) , @TRIM(TEL2)  , @TRIM(EMAIL) ,MET_SEQNO
							INTO
							    :sMODIF_TYPE , :dDATE_MODIF ,  :sWMITROO ,  :nWTMIMACODE, :sWAR_T , :sWAMKA, :sWEPVN , :sWONOMA ,  :sWPATR ,
							   :sWMHTR , :sWSEX , :sWYPHK , :sWCOORIG , :sWNOMOSORIG , :sWPOLIORIG , :dWBIRTHDATE , :sWNOMOSMHTR , 
							   :sWPOLIMHTR ,  :sWDIMOTOLOGIO , :sWCOKAT, :sWNOMOSKAT , :sWPOLIKAT ,   :sWPERIOXI , :sWODOS ,  
							   :sWTT , :sWTEL1 , :sWTEL2  , :sWEMAIL, :nWMET_SEQNO
							FROM MET_WEB
							ORDER BY DATEMODIF ' )
					!
					If NOT hWeb.Prepare()
						Return FALSE
					!
					If NOT hWeb.Execute()
						Return FALSE
					!
					! Το παρακάτω SELECT παίρνει τα λεκτικά από τον PLTAB.
					Call hWeb_Lect.Initialize("
							SELECT
							   @trim(PLTAB.PLTEXT)
							INTO
							   :sLectiko
							FROM PLTAB
							WHERE
							      PLTAB.PLCODE = :sWValue  AND PLTAB.PLLANG='G'  AND   PLTAB.PLCOLUMN= :sPlCol " )
					!
					If NOT hWeb_Lect.Prepare()
						Return FALSE
					!
					! If NOT hWeb.Next( nFetch )
						Return FALSE
					Call hWeb.Next( nFetch )
					!
					!
					Set sWriteBuffer = ''
					Set sLectiko = ''
					!
					! Πάρε και επεξεργάσου όλα τα ROWS του MET_WEB
					While nFetch != FETCH_EOF
						!
						! TOM
						If nCountPerc < nCountST_F_WEB
							Set nCountPerc = nCountPerc + nDivST_F_WEB
						Else
							Set nPerc = nPerc + 5
							Set nCountPerc = nDivST_F_WEB
							Call SalSendMsg( Progress_Meter, SAM_Create, 0, 0 )
						! TOM
						!
						!
						! Set sSN_ROW =  SalStrMidX(sROWID,28,8)
						! Set sUPDT_ROW = SalStrMidX(sROWID,8,10)
						Call Calculate_Modifications( sMODIF_TYPE )
						!
						Set sWriteBuffer = sMODIF_TYPE || '~' || Define_Date( dDATE_MODIF ) ||'~'||SalNumberToStrX( nWMET_SEQNO, 0 ) 
						Set sWriteBuffer = sWriteBuffer || '~' || sWMITROO || '~' || SalNumberToStrX(  nWTMIMACODE, 0 )  || '~' || sWAR_T  || '~' || sWAMKA || '~' || sWEPVN
						Set sWriteBuffer = sWriteBuffer || '~' ||  sWONOMA || '~' || sWPATR || '~' || sWMHTR
						!
						Set sWValue = sWSEX
						Set sPlCol = 'FYLO'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko
						!
						Set sLectiko = ''
						Set sWValue = sWYPHK
						Set sPlCol = 'YPHK'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko
						!
						Set sLectiko = ''
						Set sWValue = sWCOORIG
						Set sPlCol = 'YPHK'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko
						!
						Set sLectiko = ''
						Set sWValue = sWNOMOSORIG
						Set sPlCol = 'NOMOI'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko
						!
						Set sLectiko = ''
						Set sWValue = sWPOLIORIG
						Set sPlCol = 'POLI'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko || '~' ||  Define_Date ( dWBIRTHDATE )
						!
						Set sLectiko = ''
						Set sWValue = sWNOMOSMHTR
						Set sPlCol = 'NOMOI'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko
						!
						Set sLectiko = ''
						Set sWValue = sWPOLIMHTR
						Set sPlCol = 'DHMOI'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko
						!
						Set sLectiko = ''
						Set sWValue = sWDIMOTOLOGIO
						Set sPlCol = 'DHMOI'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko
						!
						Set sLectiko = ''
						Set sWValue = sWCOKAT
						Set sPlCol = 'YPHK'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko
						!
						Set sLectiko = ''
						Set sWValue = sWNOMOSKAT
						Set sPlCol = 'NOMOI'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko
						!
						Set sLectiko = ''
						Set sWValue = sWPOLIKAT
						Set sPlCol = 'POLI'
						If NOT hWeb_Lect.Execute()
							Return FALSE
						Call hWeb_Lect.Next(nFetch1)
						Set sWriteBuffer = sWriteBuffer ||'~'|| sLectiko || '~' || sWPERIOXI || '~' || sWODOS || '~' || sWTT|| '~' || sWTEL1 || '~' || sWTEL2 || '~' || sWEMAIL 
						!
						If VisFileWriteString( hWriteFile, sWriteBuffer ) != VTERR_Ok
							Call SalMessageBox( 'An error has occured during  writing the ASCII file ','Ενημέρωση',MB_Ok )
							If VisFileOpen( hWriteFile , sFile , OF_Exist )
								Call VisFileOpen( hWriteFile , sFile , OF_Delete )
							Return FALSE
						!
						! If NOT  hWeb.Next(nFetch)
							Return FALSE
						Call hWeb.Next(nFetch)
						Set sWriteBuffer = ''
					! TOM
					Set nPerc = 100
					Call SalSendMsg( Progress_Meter, SAM_Create, 0, 0 )
					! TOM
					!
					! Βάλε μία επιπλέον γραμμή που περιέχει τον αριθμό των κινήσεων  Insert , Update , Delete που υπάρχουν στον MET_WEB
					Set sWriteBuffer = 'c' || '~' || SalNumberToStrX( nCountInsert, 0 ) || '~' || SalNumberToStrX( nCountUpdate, 0 ) || '~' || SalNumberToStrX( nCountDelete, 0 )
					! 39 '~'
					Set sWriteBuffer = sWriteBuffer || '~~~~~~~~~~~~~~~~~~~~~~~'
					!
					! Γράψε την γραμμή
					If VisFileWriteString( hWriteFile, sWriteBuffer ) != VTERR_Ok
						Call SalMessageBox( 'An error has occured during  writing the ASCII file ','Ενημέρωση',MB_Ok )
						If VisFileOpen( hWriteFile , sFile , OF_Exist )
							Call VisFileOpen( hWriteFile , sFile , OF_Delete )
						Return FALSE
					!
					! Κλείσε το αρχείο
					Set hCloseFile = hWriteFile
					If VisFileClose( hCloseFile ) != VTERR_Ok
						Call SalMessageBox( 'An error has occured during  closing the ASCII file' || sFile , '' , MB_Ok )
						! Σβήσε το αρχείο αν συμβεί λάθος κατά το κλείσιμο
						If VisFileOpen( hWriteFile , sFile , OF_Exist )
							Call VisFileOpen( hWriteFile , sFile , OF_Delete )
						Return FALSE
					!
					If hWeb.bConnected
						!
						Call hWeb.Disconnect()
					If hWeb_Lect.bConnected
						!
						Call hWeb_Lect.Disconnect()
					Return TRUE
