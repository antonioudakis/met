					On SAM_Create
						!
						Set sTabName = SalStrTrimX( 'METEGR')
						Set sWhereClauseTbl =sTabName || '.MITROO = :dfStudCodeEgr
								                           and PROGCODE = :dlgEgrafi.sPROGCODE and TMIMACODE = :nCodeTmima ORDER BY AK,PERIOD,DATEEGR'
						Set sTabColList ='MITROO, PROGCODE, TMIMACODE'
						Set sTabValList = ':dfStudCodeEgr, :dlgEgrafi.sPROGCODE, :nCodeTmima'
						Set sWhereTbl = sTabName || '.MITROO = :dfStudCodeEgr and PROGCODE = :dlgEgrafi.sPROGCODE
								                           and TMIMACODE = :nCodeTmima'
						Set nFill = 1
					On WM_KEYDOWN
						!
						If wParam = INS
							!
							Call KeyManipulation( wParam )
							If not bFindMaxSeqnoMetegr
								Call SqlImmediate( 'SELECT MAX (SEQNO) FROM METEGR WHERE TMIMACODE =:nCodeTmima AND PROGCODE = :dlgEgrafi.sPROGCODE AND MITROO = :dfStudCodeEgr INTO  :nSeqnoMetegr')
								Call SqlClearImmediate(  )
								Set bFindMaxSeqnoMetegr = TRUE
							Set nSeqnoMetegr = nSeqnoMetegr + 1
							Set colSeqnoMetegr = nSeqnoMetegr
							Set colAkEgr = nAkTrexon
						Else
							!
							Call KeyManipulation( wParam )
					On PAM_POPULATE
						!
						Call Populate()
						! Call colPeriodCod.SetSelectionCol(tblEgrafi,colPeriod)
						Call colPeriod.SetSelectionCol(tblEgrafi,colPeriodCod)
