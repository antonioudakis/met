									 INTO  :nProgramCodeMath, :sProgramMath, :sProgTypos, :nBase
									   WHERE METPROGRAM.TMIMACODE = :nWorkTmima AND
									METPROGRAM.TYPOS = pltab.plcode and plcolumn = \'PROTYPOS\' 
									and pllang = \'G\' '
							                                                                                                                                                                                                                                Call hGetProgram.Initialize( sSelect)
							                                                                                                                                                                                                                                Call hGetProgram.Prepare()
							                                                                                                                                                                                                                                Call hGetProgram.Execute()
							                                                                                                                                                                                                                                Call hGetProgram .Next( nFetch )
							                                                                                                                                                                                                                                If nFetch = FETCH_EOF
								                                                                                                                                                                                                                                Return FALSE
							!
							                                                                                                                                                                                                                                Set sSelect =  'SELECT METKATEF.TITLOS, PLTAB.PLTEXT, METPROSPU.PERIOD, METCOURSE.TITLOS
									 FROM METPROSPU,METCOURSE, METKATEF, PLTAB
									 INTO  :sKatefProgram, :sEidosMaPr, :sPeriodMath, :sMathima
									WHERE METPROSPU.PROGCODE = :nProgramCodeMath AND metcourse.courcode = metprospu.courcode and
									METKATEF.KAT = METPROSPU.KAT AND
									METPROSPU.EIDOSMA = pltab.plcode and plcolumn = \'EIDOSMA\' 
									and pllang = \'G\' AND
									METPROSPU.TMIMACODE = :nWorkTmima order by metprospu.kat,METPROSPU.PERIOD, eidosma'
							                                                                                                                                                                                                                                Call hGetCourse.Initialize(sSelect )
							                                                                                                                                                                                                                                Call hGetCourse.Prepare()
							                                                                                                                                                                                                                                Return TRUE
							                                                                                                                                                                                                                                Break 
