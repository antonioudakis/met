											!
											Return FALSE
									Set frmEgrafi.bTropos = FALSE
									Set frmEgrafi.bOnlyOneYear = FALSE
									Set frmEgrafi.bnotMO = FALSE
									Call SalSendMsg( frmEgrafi, PAM_Report, BASICSU_Report, 0 )
									Set bPrintReportOk = TRUE
									!
								!
								! πιστοπ. αναλυτικής βαθμολογίας για ένα έτος με μέσο όρο εξαμήνου
								If sCodePistop = '5'
									If frmEgrafi.cmbStatus.GetSelection() = '05'
										If NOT CheckAit_Diag()
