										Set sErrorText = "Δεν υπάρχει καταχωρημένη τιμή στο πεδίο αριθμός πιστωτικών μονάδων για απόκτηση πτυχίου. "
										Return FALSE
									Else
										Set sSelect = "select sum(MONADES) from METKARTL,METCOURSE into :nCoursesPassed where METKARTL.COURCODE = METCOURSE.COURCODE AND METCOURSE.AK= :nAk AND METKARTL.TMIMACODE = :nTmimacode
													and METKARTL.PROGCODE = :nProgram and MITROO = '"||sMitroo||"'
