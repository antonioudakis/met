								                                                                                                                                                          Call hGetSyn.Initialize( sSelect )
								                                                                                                                                                          Call hGetSyn.Prepare()
								                                                                                                                                                          Call hGetSyn.Execute()
								                                                                                                                                                          Call hGetSyn.Next( nFetch )
								                                                                                                                                                          While nFetch != FETCH_EOF
