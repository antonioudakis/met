					On SAM_Create
						!
						!
						Set sTabName = 'METWEBAITPISTOP'
						Set sWhereClauseTbl =  " METWEBAITPISTOP.TMIMACODE  = :nCodeTmima   "
						Set sTabColList = 'TMIMACODE'
						Set sTabValList = ':nCodeTmima '
						! Set sWhereTbl =  " WEBAIT_PISTOP.k_tm = :nCodeTmima and WEBAIT_PISTOP.UriID = :colUriIDRep and WEBAIT_PISTOP.k_Pistop = :colK_Pistop"
						Set sWhereTbl =  " METWEBAITPISTOP.TMIMACODE= :nCodeTmima AND METWEBAITPISTOP.AID = :frmWebAitisi.tblWebAitisi.colAidRep "
						Set nFill = TBL_FillAll
						! Set nFill = TBL_FillNormal
					On PAM_POPULATE
						!
						Set nPrintAit = 0
						Call Populate()
						Call colPistopLekt.SetSelectionCol(tblWebAitisi,colK_Pistop)
						Call colEpvnRep.SetSelectionCol(tblWebAitisi,colMitrooRep)
						Call colOnomaRep.SetSelectionCol(tblWebAitisi,colMitrooRep)
						Call colStatusLektAit.SetSelectionCol(tblWebAitisi,colPistoStatus)
						Call CalcAllRows(tblWebAitisi)
						Set dfAllAit = SalNumberToStrX(nPrintAit,0) || ' επιλεγμένες αιτήσεις '
					On SAM_FetchRowDone
						! Μετά το populate της γραμμής ελέγχει το status της αίτησης και ανάλογα χρωματίζει τα πεδία
						! η αίτηση σε εξέλιξη ...
						If tblWebAitisi.colPistoStatus = 0
							Call SalTblSetCellTextColor( tblWebAitisi.colA_A, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colMitrooRep, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colEpvnRep, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colOnomaRep, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colPistopLekt, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colArPrvt, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colAitDate, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colPistoStatus, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colAitio, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colStatusLektAit, COLOR_Black, FALSE )
							! για να εκτυπωθεί ...
							Set tblWebAitisi.colStatusRep = 1
							Set nPrintAit = nPrintAit + 1
						! η διαδικασία ολοκληρώθηκε αλλά η εκτύπωση δεν έγινε για λόγους που αφορούν τον φοιτητή . Υπάρχουν σχόλια.
								 Από εμάς θεωρείται τελειωμένη και στέλνεται στον Server του πρωτοκόλλου.
						Else If tblWebAitisi.colPistoStatus = 2
							Call SalTblSetCellTextColor( tblWebAitisi.colA_A, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colMitrooRep, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colEpvnRep, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colOnomaRep, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colPistopLekt, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colArPrvt, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colAitDate, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colPistoStatus, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colAitio, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colStatusLektAit, COLOR_Red, FALSE )
							! Set colStatusRep = 1
							! Set nPrintAit = nPrintAit + 1
						! η διαδικασία της εκτύπωσης του πιστοποιητικού ολοκληρώθηκε.
						Else If tblWebAitisi.colPistoStatus = 1
							Call SalTblSetCellTextColor( tblWebAitisi.colA_A, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colMitrooRep, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colEpvnRep, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colOnomaRep, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colPistopLekt, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colArPrvt, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colAitDate, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colPistoStatus, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colAitio, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebAitisi.colStatusLektAit, COLOR_DarkGreen, FALSE )
					On SAM_RowSetContext
						Set LsAitnoPrint = STRING_Null
						! Call SqlImmediate (' Select PISTOREPLY  from WEBAIT_PISTOP INTO :LsAitnoPrint WHERE UriID = :colUriIDRep and K_PISTOP = :colK_Pistop ')
						Call SqlImmediate (' SELECT PISTOREPLY  FROM METWEBAITPISTOP INTO :LsAitnoPrint WHERE METWEBAITPISTOP.AID = :frmWebAitisi.tblWebAitisi.colAidRep AND TMIMACODE = :nCodeTmima  ')
