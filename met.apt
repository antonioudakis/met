								Call oUpdate.Initialize( sSqlStatement )
								Call oUpdate.Prepare()
								Call oUpdate.Execute()
								! Call SalTblDoUpdates ( tblKartelaVa, oUpdate.sqlHandle, TRUE )
								Call oUpdate.Commit()
								! Call FindSYGXVN()
								! Call FindDilMauForDelete()
							Else If SalTblQueryRowFlags(tblKartelaVa, frmVathmi.nCurrentRow , ROW_MarkDeleted )
