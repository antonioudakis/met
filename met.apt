								 		INTO  :sPar2, :sPar3, :nPar4, :sPar5, :dPar6, :nPar7, :nPar8, :nPar9, :nPar10, :nPar11
										WHERE PISTOSTATUS = 0 AND  AithseisRequestedItems.URIID = AITHSEIS.URIID '
