							!
							Set nErr = 1
							Set sQrpPath = sDriver || "\\NTUARIS\\EKDOSEIS\\REPORTS\\MQRP\\catallpl.QRP"
							! Set sQrpPath = sQrpPath || 'catallpl.qrp'
