			Function: ChkContain
				Description:
				Returns
					Boolean:
				Parameters
					Window Handle: lbParam
					String: sText
				Static Variables
				Local variables
					Boolean: bFound
					Number: nMax
					Number: nIndex
					String: sRet
				Actions
					!
					Set nIndex = 0
					Set bFound = FALSE
					Set nMax = SalListQueryCount( lbParam )
					While NOT bFound AND nIndex <= nMax
						!
						Call SalListQueryText( lbParam, nIndex, sRet )
						If sRet = sText
							Set bFound = TRUE
							Break
						Set nIndex = nIndex + 1
					Return bFound
			Function: IfExistTbl
				Description:
				Returns
					Boolean:
				Parameters
					String: sSub
					Window Handle: TblParam
					Number: nPos
				Static Variables
				Local variables
					String: sCode
					Number: nRow
					Number: nMinRow
					Number: nMaxRow
					Number: nCurrentRow
					Boolean: bFound
				Actions
					!
					Call SalTblSetRow(TblParam, TBL_SetFirstRow )
					Call SalTblQueryScroll( TblParam, nRow, nMinRow, nMaxRow )
					Loop
						!
						If nCurrentRow <= nMaxRow
							!
							Call SalTblGetColumnText(TblParam, nPos, sCode )
							If sSub = sCode
								!
								Set bFound = TRUE
								Break
							Set nCurrentRow = nCurrentRow + 1
							Call SalTblSetRow(TblParam, TBL_SetNextRow )
						Else
							!
							Set bFound = FALSE
							Break
					Return bFound
			Function: DeleteTbl
				Description:
				Returns
				Parameters
					String: sSub
					Window Handle: TblParam
					Number: nPos
				Static Variables
				Local variables
					Number: nRow
					Number: nMinRow
					Number: nMaxRow
					Number: nCurrentRow
					String: sCode
					Boolean: bOk
				Actions
					!
					Call SalTblSetRow(TblParam, TBL_SetFirstRow )
					Call SalTblQueryScroll( TblParam, nRow, nMinRow, nMaxRow )
					!
					Set nCurrentRow = nMinRow
					Loop
						!
						If nCurrentRow <= nMaxRow
							!
							Call SalTblGetColumnText(TblParam, nPos, sCode )
							If sSub = sCode
								!
								If SalTblQueryRowFlags( TblParam, lParam,ROW_New )
									!
									Call SalTblDeleteRow( TblParam, nCurrentRow, TBL_Adjust )
								Else
									!
									Set bOk = SalTblSetRowFlags( TblParam, nCurrentRow, ROW_MarkDeleted, TRUE )
								Break
							Set nCurrentRow = nCurrentRow + 1
							Call SalTblSetRow(TblParam, TBL_SetNextRow )
						Else
							!
							Break
