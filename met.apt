			Function: Yperarithmoi
				Description:
				Returns
					Number:
				Parameters
					Number: nProgCode
					Date/Time: dDateIns
					Number: nDepCode
				Static Variables
				Local variables
					String: sSelect
					String: sDateIns
					Number: nYperarithmoi
					Number: nYpoloipo
				Actions
					Call SalDateToStr(dDateIns,sDateIns)
					Set nYperarithmoi = NUMBER_Null
					Set sSelect = "SELECT  COUNT(METDSYNERG.MITROO)
							FROM METDIPLGRADE,METDSYNERG,METSTUPROG
							INTO :nYperarithmoi
							WHERE  METDSYNERG.CODESYN = METDIPLGRADE.CODESYN AND METDSYNERG.TMIMACODE = :nWorkTmima AND 
							METDSYNERG.PROGCODE = :nProgCode AND METDIPLGRADE.TYPEDEP = 1 AND 
							METDIPLGRADE.DATEDEL IS NULL  AND METSTUPROG.MITROO = METDSYNERG.MITROO AND 
							METSTUPROG.MITROO = METDSYNERG.MITROO AND METSTUPROG.PROGCODE =  METDSYNERG.PROGCODE
							AND METDIPLGRADE.DEPCODE = :nDepCode AND METSTUPROG.DATEANAG IS NULL 
							AND METDSYNERG.MITROO NOT 
							IN (SELECT MITROO FROM METDIAKOP WHERE PROGCODE = :nProgCode AND DATEAPO <= "||sDateIns||" AND (DATEEOS IS NULL OR DATEEOS >= "||sDateIns||" )) 
							AND @MID(METDIPLGRADE.DATEINS,0,4)< "||SalStrLeftX( sDateIns, 4 )
					Call SqlImmediate( sSelect )
					If nYperarithmoi < 6
						Set nYpoloipo = 3
					Else If nYpoloipo < 11
						Set nYpoloipo = 2
					Else
						Set nYpoloipo = 1
					Set nYperarithmoi = NUMBER_Null
					Set sSelect = "SELECT  COUNT(METDSYNERG.MITROO)
							FROM METDIPLGRADE,METDSYNERG,METSTUPROG
							INTO :nYperarithmoi
							WHERE  METDSYNERG.CODESYN = METDIPLGRADE.CODESYN AND METDSYNERG.TMIMACODE = :nWorkTmima AND 
							METDSYNERG.PROGCODE = :nProgCode AND METDIPLGRADE.TYPEDEP = 1 AND 
							METDIPLGRADE.DATEDEL IS NULL  AND METSTUPROG.MITROO = METDSYNERG.MITROO AND 
							METSTUPROG.MITROO = METDSYNERG.MITROO AND METSTUPROG.PROGCODE =  METDSYNERG.PROGCODE
							AND METDIPLGRADE.DEPCODE = :nDepCode AND METSTUPROG.DATEANAG IS NULL 
							AND METDSYNERG.MITROO NOT 
							IN (SELECT MITROO FROM METDIAKOP WHERE PROGCODE = :nProgCode AND DATEAPO <= "||sDateIns||" AND (DATEEOS IS NULL OR DATEEOS >= "||sDateIns||" )) 
							AND @MID(METDIPLGRADE.DATEINS,0,4) = "||SalStrLeftX( sDateIns, 4 )
					Call SqlImmediate( sSelect )
					Return nYpoloipo - nYperarithmoi
