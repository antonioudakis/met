									      and @if(@abs(@if(@isna(gradenor),0,gradenor)-@if(@isna(gradepan),0,gradepan)) - (@if(@isna(gradenor),0,gradenor)-@if(@isna(gradepan),0,gradepan)),gradepan,@if(@isna(gradenor),0,gradenor))>=5 )
									      and mitroo = '"||dfStf||"' and progcode="||sProodDipl )
							Call SqlClearImmediate(  )
							If numCoursesFailed = 0
								Set coursesPassed = TRUE
							Else
								Set coursesPassed = FALSE
							! Δεν δουλεύει το join
							! Call SqlImmediate("
									select metanathesi.dateejet
									into :dateEjet
									from metanathesi right join (metdsynerg right join metstuprog on metdsynerg.mitroo = metstuprog.mitroo and metdsynerg.progcode= metstuprog.progcode) on metanathesi.codesyn = metdsynerg.codesyn 
									where metstuprog.mitroo = '"||dfStf||"' and metstuprog.progcode="||sProodDipl )
							Call SqlImmediate("
									select metanathesi.dateejet
									into :dateEjet
									from metanathesi,metdsynerg,metstuprog where metdsynerg.mitroo = metstuprog.mitroo and metdsynerg.progcode = metstuprog.progcode and metanathesi.codesyn = metdsynerg.codesyn
									and metstuprog.mitroo = '"||dfStf||"' and metstuprog.progcode="||sProodDipl||' and dateejet is not null')
							Call SqlClearImmediate(  )
							If dateEjet = DATETIME_Null
								Set ejet = FALSE
							Else
								Set ejet = TRUE
						Else
							Set coursesPassed = TRUE
							! Δεν δουλεύει το join
							! Call SqlImmediate("
									select metanathesi.dateejet
									into :dateEjet
									from metanathesi right join (metdsynerg right join metstuprog on metdsynerg.mitroo = metstuprog.mitroo and metdsynerg.progcode= metstuprog.progcode) on metanathesi.codesyn = metdsynerg.codesyn 
									where metstuprog.mitroo = '"||dfStf||"' and metstuprog.progcode="||sProodDipl )
							Call SqlImmediate("
									select metanathesi.dateejet
									into :dateEjet
									from metanathesi,metdsynerg,metstuprog where metdsynerg.mitroo = metstuprog.mitroo and metdsynerg.progcode = metstuprog.progcode and metanathesi.codesyn = metdsynerg.codesyn
									and metstuprog.mitroo = '"||dfStf||"' and metstuprog.progcode="||sProodDipl||' and dateejet is not null')
							Call SqlClearImmediate(  )
							If dateEjet = DATETIME_Null
								Set ejet = FALSE
							Else
								Set ejet = TRUE
					Else
						Set coursesPassed = TRUE
						Call SqlImmediate("
								select metanathesi.dateejet
								into :dateEjet
								from metanathesi,metdsynerg,metstuprog where metdsynerg.mitroo = metstuprog.mitroo and metdsynerg.progcode = metstuprog.progcode and metanathesi.codesyn = metdsynerg.codesyn
								and metstuprog.mitroo = '"||dfStf||"' and metstuprog.progcode="||sProodDipl||' and dateejet is not null')
						Call SqlClearImmediate(  )
						If dateEjet = DATETIME_Null
							Set ejet = FALSE
						Else
							Set ejet = TRUE
