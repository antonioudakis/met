						Call SqlPrepare( hSQLBasePop, strSQL )
						Set strTempSQL = 'SELECT AITREGISTRATION, AITUSEROTHERINFO, PISTOID,
											AITIO, AITDATE, PISTOSTATUS,  AITHSEIS.URIID, AID, PISTOFLAG, 2
