									Set sSignTextDie = 'THE DIRECTOR OF THE PROGRAM'
								Else If nMode=2
									Set sSignTextDie = 'THE DIRECTOR OF THE PROGRAM'
								Else
									Set sSignTextDie = 'THE DEAN OF THE SCHOOL'
							! Βάζω πλέον ολόκληρο το Ονοματεπώνυμο του Διευθυντή του προγράμματος
							! Set nOffset = SalStrScan( sDEP, ' ' )
							! Set sDepEpvn = SalStrMidX( sDEP, nOffset+1, SalStrLength( sDEP )-nOffset )
							! Set nOffset = SalStrScan( sDEP, '-' )
							! If nOffset != -1
								Set sDEP = SalStrLeftX( sDEP, 1 )||'-'||SalStrMidX( sDEP, nOffset+1, 1 )||'. '||sDepEpvn
							! Else
								Set sDEP = SalStrLeftX( sDEP, 1 )||'. '||sDepEpvn
						Else
							Set sDEP = STRING_Null
							Set sSignTextDie = STRING_Null
						If nLimit = NUMBER_Null
							Set nLimit = 5
						If nLimitDipl = NUMBER_Null
							Set nLimitDipl = 5.5
					Else
						Set sProodDiplFun = sLProodDipl
						Set sEgrAk = sProgDipl
						If bEnglish
							Call SqlImmediate ("Select  LIMIT,LIMITDIPL,ROUNDMO,MINIENG,MAXIENG,YPAPOF,LECTPER from METPROGAT  Into  :nLimit, :nLimitDipl, :sRound, :sEPVN, :sONOMA, :sYpApof, :sLectPer
									Where   PROGCODE = :sProodDiplFun and tmimacode = :nCodeTmima and ak = :sEgrAk")
						Else
							Call SqlImmediate ("Select  LIMIT,LIMITDIPL,ROUNDMO,MINI,MAXI,YPAPOF,LECTPER from METPROGAT  Into  :nLimit, :nLimitDipl, :sRound, :sEPVN, :sONOMA, :sYpApof, :sLectPer
									Where   PROGCODE = :sProodDiplFun and tmimacode = :nCodeTmima and ak = :sEgrAk")
						If NOT hDiplPr.bConnected
							!
							Call hDiplPr.Connect()
						If nMode = 1
							Set sSelect = "Select @TRIM(ONOMA)||' '||@TRIM(EPVN), FYLO from METPROGRAM, DEP
									Into  :sDEP, :sFyloDEP
									Where  TMIMACODE = :nCodeTmima  And METPROGRAM.PROGCODE = :sProodDiplFun 
									 And METPROGRAM.AM_DEP = DEP.AM_DEP "
						Else If nMode = 2
							Set sSelect = "Select @TRIM(ONOMA)||' '||@TRIM(EPVN), FYLO from METPROGRAM, DEP
									Into  :sDEP, :sFyloDEP
									Where  TMIMACODE = :nCodeTmima  And METPROGRAM.PROGCODE = :sProodDiplFun 
									 And METPROGRAM.AM_AN_DEP = DEP.AM_DEP "
						Else
							Set sSelect = "Select @TRIM(ONOMA)||' '||@TRIM(EPVN), FYLO from METTMIMA, DEP
									Into  :sDEP, :sFyloDEP
									Where  TMIMACODE = :nCodeTmima  And METTMIMA.AM_PRO = DEP.AM_DEP "
						Set sSelect1 = "Select  PLTEXT ,TYPOS from METPROGRAM, PLTAB 
								Into   :sProgTitle, :sTyposPr
								Where  TMIMACODE = :nCodeTmima  And PROGCODE = :sProodDiplFun and PLLANG ='G' and PLCOLUMN = 'PROTYPOS' 
								and PLCODE = TYPOS "
						Call SqlImmediate( sSelect1 )
						If bEnglish
							Set sProgTitle = "INTERDEPATMENTAL PROGRAM OF POSTGRADUATE STUDIES IN"
						Call hDiplPr.Initialize( sSelect)
						Call hDiplPr.Prepare()
						Call hDiplPr.Execute()
						Call hDiplPr.Next( nFetch )
						If nFetch != FETCH_EOF
							!
							If sYpApof   != STRING_Null
								If bEnglish
									Set sYpApof = '(M.D. '||sYpApof||')'
								Else
									Set sYpApof = '('||sYpApof||')'
							If nCodeTmima = 9 and sProodDiplFun != '3'
								If sProodDiplFun != '4' or (sProodDiplFun = '4' and nWhich !=6 and nWhich !=7 and nWhich !=8 and nWhich !=9 and nWhich !=13 and nWhich !=14)
									Set sDEP = STRING_Null
									Set sSignTextDie = STRING_Null
								Else
									If nMode=1
										If sFyloDEP = '2'
											Set sSignTextDie = 'Η ΔΙΕΥΘΥΝΤΡΙΑ ΤΟΥ ΔΠΜΣ'
										Else
											Set sSignTextDie = 'Ο ΔΙΕΥΘΥΝΤΗΣ ΤΟΥ ΔΠΜΣ'
									Else If nMode=2
										If sFyloDEP = '2'
											Set sSignTextDie = 'Η ΑΝΑΠΛ. ΔΙΕΥΘΥΝΤΡΙΑ ΤΟΥ ΔΠΜΣ'
										Else
											Set sSignTextDie = 'Ο ΑΝΑΠΛ. ΔΙΕΥΘΥΝΤΗΣ ΤΟΥ ΔΠΜΣ'
									Else
										If sFyloDEP = '2'
											Set sSignTextDie = 'Η ΠΡΟΕΔΡΟΣ ΤΗΣ ΣΧΟΛΗΣ'
											Set sSignTextDie = 'Η ΚΟΣΜΗΤΟΡΑΣ ΤΗΣ ΣΧΟΛΗΣ'
										Else
											Set sSignTextDie = 'Ο ΠΡΟΕΔΡΟΣ ΤΗΣ ΣΧΟΛΗΣ'
											Set sSignTextDie = 'Ο ΚΟΣΜΗΤΟΡΑΣ ΤΗΣ ΣΧΟΛΗΣ'
							! Προσωρινό  για τους Χημικούς ,λείπει ο Διευθυντής Τομέα.
							! If nCodeTmima = 5 and sProodDiplFun = '2'
								     If sFyloDEP = '1'
									     Set sSignTextDie = 'Ο ΑΝΑΠΛΗΡΩΤΗΣ ΔΙΕΥΘΥΝΤΗΣ ΤΟΥ ΔΠΜΣ'
								     Else 
									     Set sSignTextDie = 'Η ΑΝΑΠΛΗΡΩΤΡΙΑ ΔΙΕΥΘΥΝΤΡΙΑ ΤΟΥ ΔΠΜΣ'
							! Else If nCodeTmima = 5
								     If sFyloDEP = '1'
									     Set sSignTextDie = 'Ο  ΔΙΕΥΘΥΝΤΗΣ ΤΟΥ ΔΠΜΣ'
								     Else 
									     Set sSignTextDie = 'Η ΔΙΕΥΘΥΝΤΡΙΑ ΤΟΥ ΔΠΜΣ'
							Else
								If nMode=1
									If sFyloDEP = '2'
										Set sSignTextDie = 'Η ΔΙΕΥΘΥΝΤΡΙΑ ΤΟΥ ΔΠΜΣ'
									Else
										Set sSignTextDie = 'Ο ΔΙΕΥΘΥΝΤΗΣ ΤΟΥ ΔΠΜΣ'
								Else If nMode=2
									If sFyloDEP = '2'
										Set sSignTextDie = 'Η ΑΝΑΠΛ. ΔΙΕΥΘΥΝΤΡΙΑ ΤΟΥ ΔΠΜΣ'
									Else
										Set sSignTextDie = 'Ο ΑΝΑΠΛ. ΔΙΕΥΘΥΝΤΗΣ ΤΟΥ ΔΠΜΣ'
								Else
									If sFyloDEP = '2'
										Set sSignTextDie = 'Η ΠΡΟΕΔΡΟΣ ΤΗΣ ΣΧΟΛΗΣ'
										Set sSignTextDie = 'Η ΚΟΣΜΗΤΟΡΑΣ ΤΗΣ ΣΧΟΛΗΣ'
									Else
										Set sSignTextDie = 'Ο ΠΡΟΕΔΡΟΣ ΤΗΣ ΣΧΟΛΗΣ'
										Set sSignTextDie = 'Ο ΚΟΣΜΗΤΟΡΑΣ ΤΗΣ ΣΧΟΛΗΣ'
							! Βάζω πλέον ολόκληρο το Ονοματεπώνυμο του Διευθυντή του προγράμματος
							! Set nOffset = SalStrScan( sDEP, ' ' )
							! Set sDepEpvn = SalStrMidX( sDEP, nOffset+1, SalStrLength( sDEP )-nOffset )
							! Set nOffset = SalStrScan( sDEP, '-' )
							! If nOffset != -1
								Set sDEP = SalStrLeftX( sDEP, 1 )||'-'||SalStrMidX( sDEP, nOffset+1, 1 )||'. '||sDepEpvn
							! Else
								Set sDEP = SalStrLeftX( sDEP, 1 )||'. '||sDepEpvn
						Else
							Set sDEP = STRING_Null
							Set sSignTextDie = STRING_Null
						If nLimit = NUMBER_Null
							Set nLimit = 5
						If nLimitDipl = NUMBER_Null
							Set nLimitDipl = 5.5
			Function: FindDiplVa
				Description: Βρίσκει το μέσο όρο βαθμού διπλωματικής
				Returns
					String:
				Parameters
					String: sLRound
					String: sLKF
					String: sLProodDipl
				Static Variables
				Local variables
					String: sSelect
					Number: nMODipl
					Number: nModDipl
					Number: nCountMO
				Actions
					!
					Set sKFFun = sLKF
					Set sProodDiplFun = sLProodDipl
					If NOT hDiplPr.bConnected
						!
						Call hDiplPr.Connect()
					Set sSelect = "Select CODESYN  from METDSYNERG  Into  :nCodeSynStud
							Where   PROGCODE = :sProodDiplFun And MITROO = :sKFFun"
					Call hDiplPr.Initialize( sSelect)
					Call hDiplPr.Prepare()
					Call hDiplPr.Execute()
					Call hDiplPr.Next( nFetch )
					If nFetch != FETCH_EOF
						!
						Set nMODipl = 0
						Set nCountMO = 0
						Set sSelect = "Select grade from METDIPLGRADE  Into  :nGradeDipl
								Where  CODESYN = :nCodeSynStud"
						Call hDiplPr.Initialize( sSelect)
						Call hDiplPr.Prepare()
						Call hDiplPr.Execute()
						Set sSelect = STRING_Null
						Call hDiplPr.Next( nFetch )
						While nFetch != FETCH_EOF
							!
							If nGradeDipl != NUMBER_Null
								Set nMODipl = nMODipl + nGradeDipl
								Set nCountMO = nCountMO + 1
							Call hDiplPr.Next( nFetch )
						If nCountMO > 0
							If sLRound = '1'
								Set nMODipl = nMODipl / nCountMO
								Set nModDipl = SalNumberMod( nMODipl*100, 100 )
								Set nMODipl = nMODipl - (nModDipl /100)
								If nModDipl >= 75
									Set nMODipl = nMODipl + 1
								If nModDipl >= 25 and nModDipl < 75
									Set nMODipl = nMODipl + 0.5
									Set sSelect = SalNumberToStrX( nMODipl , 2 ) ||' ('|| sNumberText(nMODipl - 0.5)||' ΚΑΙ ΜΙΣΟ).
											'
								Else
									Set sSelect = SalNumberToStrX( nMODipl , 2 ) ||' ('|| sNumberText(nMODipl)||').
											'
							Else
								Set nMODipl = SalNumberRound((nMODipl / nCountMO) *100)/100
								Set nModDipl = SalNumberMod( nMODipl*100, 100 )
								If nModDipl > 0
									Set sSelect = SalNumberToStrX( nMODipl , 2 ) ||' ('|| sNumberText(nMODipl - nModDipl/100)||' ΚΑΙ '|| SalNumberToStrX( nModDipl , 0 ) ||' %).
											'
								Else
									Set sSelect = SalNumberToStrX( nMODipl , 2 ) ||' ('|| sNumberText(nMODipl ) ||').
											'
					Else
						!
						Set sSelect = STRING_Null
					Return sSelect
			! Function: FindMauVa
				Description: Υπολογίζει το  βαθμό διπλώματος ελέγχοντας τα προαπαιτούμενα και 
						κάνοντας στρογγυλοποίηση αν απαιτείται
				Returns 
					Number: 
				Parameters 
					String: sLKF
					String: sLProodDipl
					Boolean: bCourseOnly
					! Number: nAk
				Static Variables 
				Local variables 
					String: sSelect
					! String: sCourCode
					Number: nMODipl
					Number: nModDipl
					Number: nCountMO
					Number: nwDipl
					Number: nCourDipl
					String: sLRound
					String: sMixanikMaster
					String: Mixanik
					String: sPM
				Actions 
					!
					Set sKFFun = sLKF
					Set sProodDiplFun = sLProodDipl
					Set nMau = NUMBER_Null
					Set nMauDiorth = NUMBER_Null
					Set nGradeVaDipl =NUMBER_Null
					Set nNor = NUMBER_Null
					Set nEpan = NUMBER_Null
					Set nwDipl = NUMBER_Null
					Set nCourDipl = NUMBER_Null
					Set sLRound = STRING_Null
					If NOT CheckCoursesPassed(nCodeTmima,SalStrToNumber( sProodDiplFun),sKFFun,nErrorCode,sErrorText)
						If NOT bMultiplePrints
							Call SalMessageBox( sErrorText, '', MB_Ok )
						Return NUMBER_Null
					If NOT hDiplAvg.bConnected
						!
						Call hDiplAvg.Connect()
					! ! Ελάχιστη διάρκεια σπουδών για το πρόγραμμα
					Set sSelect = 
							"Select  LIMIT,LIMITDIPL  
							 from METPROGAT  
							 into  :nNor, :nEpan
							 where PROGCODE = :sProodDiplFun and tmimacode = :nCodeTmima and ak = :frmEgrafi.tblMetStudProg.colAkStud"
					Call hDiplAvg.Initialize( sSelect)
					Call hDiplAvg.Prepare()
					Call hDiplAvg.Execute()
					Call hDiplAvg.Next( nFetch )
					If nNor = NUMBER_Null
						Set nNor = 5
					If nEpan = NUMBER_Null
						Set nEpan = 5.5
					! ! Συντελεστής βαρύτητας της Διπλωματικής Εργασίας για το συγκεκριμένο πρόγραμμα
					Call SqlImmediate (
							"select  WDIPL, ARCOURDIPL,ROUNDMO 
							 from METPROGAT  
							 into  :nwDipl, :nCourDipl, :sLRound
							 where   PROGCODE = :sProodDiplFun and tmimacode = :nCodeTmima and ak = :frmEgrafi.nAk")
					! If nCodeTmima = 3 and sProodDiplFun = '1'
						Set sMixanikMaster = CheckMixanikMaster(sKFFun)
						If SalStrLeftX( sMixanikMaster, 1 )  = '0'
							If SalStrRightX( sMixanikMaster, 1 ) = '0'
								Set nCourDipl = 6
							Else 
								Set nCourDipl = 6
						Else 
							If SalStrRightX( sMixanikMaster, 1 ) = '0'
								Set nCourDipl = 6
							Else 
								Set nCourDipl = 4
					If nCodeTmima = 3 and sProodDiplFun = '1'
						Call SqlImmediate( 
								"select OFEILES 
								 from MET 
								 into :Mixanik 
								 where mitroo ='"||sKFFun||"'")
						If Mixanik = '1' 
							! Set nCourDipl = 4
							Set nCourDipl = 6
						Else 
							Set nCourDipl = 6
					Else If nCodeTmima != 3 and sProodDiplFun = '1'
						Set sMixanikMaster = CheckMixanikMaster(sKFFun)
						If sMixanikMaster = STRING_Null
							Return NUMBER_Null
						Else If sMixanikMaster = '11'
							! Set nCoursesNeeded = 4
							Set nCourDipl = 6
						Else If sMixanikMaster = '10'
							Set nCourDipl = 6
						Else 
							Call SqlImmediate (
									"select PM 
									 from METSTUPROG
									 into :sPM
									 where METSTUPROG.MITROO ="||sKFFun||" and METSTUPROG.PROGCODE="||sProodDiplFun)
							Call SqlClearImmediate()
							If sPM ='1'
								Set nCourDipl = 6
							Else 
								Return NUMBER_Null
					! !!!
					! ! Εύρεση των μαθημάτων που έχει δηλώσει στο πρόγραμμα και είναι προαπαιτούμενα
					If NOT hDiplPr.bConnected
						!
						Call hDiplPr.Connect()
					Set sSelect = 
							"select distinct COURCODE  
							 from METKARTL  
							 into  :nCourCode
							 where   PROGCODE = :sProodDiplFun and MITROO = :sKFFun and typos = 'PR'"
					Call hDiplPr.Initialize( sSelect)
					Call hDiplPr.Prepare()
					Call hDiplPr.Execute()
					Call hDiplPr.Next( nFetch )
					! ! ΄Ελεγχος αν έχει προβιβάσιμο βαθμό στα προαπαιτούμενα μαθήματα
					While nFetch != FETCH_EOF
						If NOT hDiplVa.bConnected
							!
							Call hDiplVa.Connect()
						Set sSelect = 
								"select max(gradenor) ,max(gradepan)  
								 from METKARTL  
								 into  :nGradeN, :nGradeP
								 where  PROGCODE = :sProodDiplFun and MITROO = :sKFFun and COURCODE = :nCourCode and typos = 'PR'"
						Call hDiplVa.Initialize( sSelect)
						Call hDiplVa.Prepare()
						Call hDiplVa.Execute()
						Call hDiplVa.Next( nFetch1)
						If (nGradeN >= nNor or nGradeP >= nNor)
							Call hDiplPr.Next( nFetch )
						Else 
							If NOT bMultiplePrints
								Call SalMessageBox( 'Ο φοιτητής δεν έχει προβιβάσιμο βαθμό σε προαπαιτούμενα μαθήματα!', '', MB_IconStop | MB_Ok )
							Return NUMBER_Null
						Set nMau = NUMBER_Null
					! !!!!
					If NOT hDiplPr.bConnected
						!
						Call hDiplPr.Connect()
					! ! Εύρεση των μαθημάτων που έχει δηλώσει στο πρόγραμμα και ΔΕΝ είναι προαπαιτούμενα 
					Set sSelect = 
							"select distinct COURCODE  
							 from METKARTL  
							 into  :nCourCode
							 where   PROGCODE = :sProodDiplFun and MITROO = :sKFFun and (TYPOS is null or TYPOS != 'PR') "
					Call hDiplPr.Initialize( sSelect)
					Call hDiplPr.Prepare()
					Call hDiplPr.Execute()
					Call hDiplPr.Next( nFetch )
					! Βρίσκει τον μέγιστο βαθμό κανονικής και επαναληπτικής περιόδου και το ακαδημαϊκό έτος που πήρε τον βαθμό 
							! Αν έχει μία τουλάχιστον απαλλαγή στο μάθημα θα εμφανίζει το ακαδημαϊκό έτος που πήρε την απαλλαγή και θα αγνοηθεί ο βαθμός, αν υπάρχει
					While nFetch != FETCH_EOF
						!
						Set nMODipl = 0
						Set nCountMO = 0
						Set bFTime = TRUE
						If NOT hDiplVa.bConnected
							!
							Call hDiplVa.Connect()
						Set sSelect = 
								"select @if(@abs(gradenor-gradepan)-(gradenor-gradepan),gradepan,gradenor),apalag,epepil,ak 
								 from METKARTL  
								 into  :nGradeVaMau, :nApalag,:sEpEpil,:nAkMau 
								 where PROGCODE = :sProodDiplFun and MITROO = :sKFFun and COURCODE = :nCourCode 
								       and (TYPOS is null or  TYPOS != 'PR') 
								 order by 2 desc,3 desc,1 desc"
						Call hDiplVa.Initialize( sSelect)
						Call hDiplVa.Prepare()
						Call hDiplVa.Execute()
						Call hDiplVa.Next(nFetch1)
						If nApalag != 1 or nApalag = NUMBER_Null
							If sEpEpil != '1' or sEpEpil = STRING_Null
								If NOT hDiplAvg.bConnected
									!
									Call hDiplAvg.Connect()
								Set sSelect = 
										"select MONADES  
										 from METCOURSE  
										 into  :wFactor
										 where   COURCODE = :nCourCode and AK = :nAkMau"
								Call hDiplAvg.Initialize( sSelect)
								Call hDiplAvg.Prepare()
								Call hDiplAvg.Execute()
								Call hDiplAvg.Next( nFetch)
								If wFactor != NUMBER_Null
									If nGradeVaMau >= nNor
										Set nGradeVaDipl = nGradeVaDipl + (nGradeVaMau * wFactor)
										Set nMau = nMau + 1
										Set nMauDiorth = nMauDiorth + wFactor
								Else 
									! Προεπιλεγμένη τιμή 1
									If nGradeVaMau >= nNor
										Set nGradeVaDipl = nGradeVaDipl + nGradeVaMau
										Set nMau = nMau + 1
										Set nMauDiorth = nMauDiorth + 1
						Else 
							Set nCourDipl = nCourDipl - 1
						Call hDiplPr.Next( nFetch )
						! !
					! ! Ελέγχει αν ο μ.φ. έχει περάσει τον απαιτούμενο αριθμό μαθημάτων στο πρόγραμμα
					If nCourDipl > nMau
						Return NUMBER_Null
					! Αν υπολογίζω τον μέσο όρο μόνο των μαθημάτων
					If (bCourseOnly =TRUE)
						! If sLRound = '1' !! Στρογγυλοποίηση στο πρώτο δεκαδικό !!
							Set nGradeVaDipl = SalNumberRound((nGradeVaDipl/nMauDiorth)*10)/10
						! Else
							Set nGradeVaDipl = SalNumberRound((nGradeVaDipl/nMauDiorth)*100)/100
						Set nGradeVaDipl = SalNumberRound((nGradeVaDipl/nMauDiorth)*100)/100
						Return nGradeVaDipl
					! Αλλιώς αν υπολογίζω τον μέσο όρο των μαθημάτων σε συνδιασμό με την διπλωματική εργασία
					Else 
						! !
						! !!!!!!!!Υπολογισμός Βαθμού Διπλωματικής!!!!!!!!!!!!!!
						! !
						If NOT hDiplVa.bConnected
							!
							Call hDiplVa.Connect()
						Set sSelect = 
								"select CODESYN  
								 from METDSYNERG  
								 into  :nCodeSynStud
								 where   PROGCODE = :sProodDiplFun and MITROO = :sKFFun"
						Call hDiplVa.Initialize( sSelect)
						Call hDiplVa.Prepare()
						Call hDiplVa.Execute()
						Call hDiplVa.Next( nFetch )
						If nFetch != FETCH_EOF
							!
							Set nMODipl = 0
							Set nCountMO = 0
							Set sSelect = 
									"select grade 
									 from METDIPLGRADE  
									 into  :nGradeDipl
									 where  CODESYN = :nCodeSynStud"
							Call hDiplAvg.Initialize( sSelect)
							Call hDiplAvg.Prepare()
							Call hDiplAvg.Execute()
							Call hDiplAvg.Next( nFetch )
							While nFetch != FETCH_EOF
								!
								If nGradeDipl != NUMBER_Null
									Set nMODipl = nMODipl + nGradeDipl
									Set nCountMO = nCountMO + 1
								Call hDiplAvg.Next( nFetch )
							If nCountMO > 0
								If sLRound = '1'
									Set nMODipl = nMODipl / nCountMO
									Set nModDipl = SalNumberMod( nMODipl*100, 100 )
									Set nMODipl = nMODipl - (nModDipl /100)
									If nModDipl >= 75
										Set nMODipl = nMODipl + 1
									If nModDipl >= 25 and nModDipl < 75
										Set nMODipl = nMODipl + 0.5
								Else 
									Set nMODipl = nMODipl / nCountMO
									Set nMODipl = SalNumberRound(( nMODipl ) *100)/100
								Set nGradeVaDipl = SalNumberRound((nGradeVaDipl + ( (nwDipl/nCourDipl) * nMauDiorth * nMODipl))/ ( ((nwDipl+nCourDipl)/nCourDipl) * nMauDiorth)*100)/100
								If nMODipl < nEpan
									Call SalMessageBox( 'Ο Φοιτητής έχει βαθμό διπλωματικής εργασίας μικρότερο του βαθμού βάσης διπλωματικής εργασίας !', '', MB_IconStop | MB_Ok )
									Return NUMBER_Null
							Else 
								Call SalMessageBox( 'Ο Φοιτητής δεν έχει βαθμό διπλωματικής εργασίας, ενημερώστε τη φόρμα της Ανάθεσης !', '', MB_IconStop | MB_Ok )
								Return NUMBER_Null
						Else 
							!
							Set nModDipl = NUMBER_Null
							Call SalMessageBox( 'Ο Φοιτητής δεν έχει κωδικό συνεργασίας, ενημερώστε τη φόρμα της Ανάθεσης !', '', MB_IconStop | MB_Ok )
							Return VALIDATE_Cancel
						Return nGradeVaDipl
					If hDiplVa.bConnected
						!
						Call hDiplVa.Disconnect()
					If hDiplAvg.bConnected
						!
						Call hDiplAvg.Disconnect()
					If hDiplPr.bConnected
						!
						Call hDiplPr.Disconnect()
