						Call SalTblGetColumnText( whTbl, 14, sCol5Par )
						Call SalTblGetColumnText( whTbl, 8, sCol6Par )
						!
						Set nRow = nRow + 1
						Return TRUE
					!
					Else
						Set bOtherRow = FALSE
						Return FALSE
			Function: UpdateWebAitisi
				Description:
				Returns
					Boolean:
				Parameters
					Number: nMode
				Static Variables
				Local variables
				Actions
					Select Case nMode
						Case 1
							If sForm = 'frmEgrafi'
								! If NOT SqlImmediate ( 'UPDATE WEBAIT_PISTOP SET PRINTERROR = :frmEgrafi.nErrR WHERE UriID = :sAA and k_pistop = :sCodePistop ')
									Return FALSE
								If NOT SqlImmediate ( 'UPDATE METWEBAITPISTOP SET PRINTERROR = :frmEgrafi.nError WHERE AID = :sAA AND TMIMACODE = :nCodeTmima ')
									Return FALSE
							Call SqlClearImmediate()
							Return TRUE
						Case 2
							If sForm = 'frmEgrafi'
								! If NOT SqlImmediate ( 'UPDATE WEBAIT_PISTOP SET PRINTERROR = :frmEgrafi.nErrR WHERE UriID = :sAA and k_pistop = :sCodePistop ')
									Return FALSE
								If NOT SqlImmediate ( 'UPDATE METWEBAITPISTOP SET PRINTERROR = :frmEgrafi.nError WHERE AID = :sAA AND TMIMACODE = :nCodeTmima  ')
									Return FALSE
