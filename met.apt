						Set frmSplash.dfMsgToUser = 'Η διαδικασία ολοκληρώθηκε επιτυχώς'
						Set bEnableTimer = SalTimerSet ( frmSplash, 1, 4000 )
				! Else
					! Call SqlCommit( hSQLBase )
					! Call SqlSetIsolationLevel( hSQLBasePop, strSQLBaseIsolation )
					Set strSQLBaseIsolation = 'RL'
					Call SqlSetIsolationLevel( hSQLBase, strSQLBaseIsolation )
					Call SetParameter(DBP_PRESERVE, TRUE, '')
					Call Disconnect()
					If hMSSQLServer
						Call SqlDisconnect(hMSSQLServer)
					If hSQLBase
						Call SqlDisconnect(hSQLBase)
					If hSQLBasePop
						Call SqlDisconnect(hSQLBasePop)
