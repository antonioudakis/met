					!
					Select Case nRep
						Case CATALOG_Report
							!
							If NOT hPln.bConnected
								!
								Call hPln.Connect()
							Return TRUE
							Break
						Case BASIC_Report
							!
							If NOT hPla.bConnected
								!
								Call hPla.Connect()
							Return TRUE
							Break
