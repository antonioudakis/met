		Functions 
			! ! !
			Function: UsersBuildSelect
				Description: 
				Returns 
				Parameters 
				Static Variables 
				Local variables 
				Actions 
					Call SalShowWindow(tblKartella_ist.colUsersEpvn)
					Call SalShowWindow(tblKartella_ist.colUsersOnoma)
					Call SalShowWindow(tblKartella_ist.colUsersPatr)
					Call SalShowWindow(tblKartella_ist.colUsersKmTitlos)
					Set sFirstWhere =  ' KARTELLA_IST.K_TM  = :nCodeTmima AND KARTELLA_IST.K_TM = ST_M.K_TM AND KARTELLA_IST.AK = ST_M.AK AND KARTELLA_IST.XU = ST_M.XU'
					Set tblKartella_ist.sWhereClauseTbl = STRING_Null
					If dfUsersDateApo != DATETIME_Null
						Set tblKartella_ist.sWhereClauseTbl = sFirstWhere || " AND DATE_MODIF BETWEEN :dfUsersDateApo AND :dfUsersDateEos "
						! Set tblKartella_ist.sWhereClauseTbl = sFirstWhere || " AND DATE_MODIF >=  :dfUsersDateApo AND DATE_MODIF <= :dfUsersDateEos "
					Else 
						Set tblKartella_ist.sWhereClauseTbl = sFirstWhere
					If sUsersName != 'ALL USERS'
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " AND KARTELLA_IST.USERNAME = :sUsersName"
					If dfUsersEtos != NUMBER_Null
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " AND KARTELLA_IST.AK = :dfUsersEtos "
					If sXU_Users != STRING_Null
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " AND KARTELLA_IST.XU = :sXU_Users "
					If dfUsersKf != STRING_Null
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " AND KARTELLA_IST.K_F LIKE '"
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || dfUsersKf || "%' "
					If dfUsersKm != NUMBER_Null
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || ' AND KARTELLA_IST.K_M  = :dfUsersKm '
					If SalListQuerySelection ( cmbType) = 3
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " ORDER BY kartella_ist.date_modif desc, kartella_ist.modif_type, st_f.epvn, st_f.onoma, st_f.patr "
					Else If SalListQuerySelection ( cmbType) = 0
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " AND modif_type = 'i'   ORDER BY kartella_ist.date_modif desc, st_f.epvn, st_f.onoma, st_f.patr"
					Else If SalListQuerySelection ( cmbType ) = 1
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " AND modif_type = 'u'  ORDER BY kartella_ist.date_modif desc, st_f.epvn, st_f.onoma, st_f.patr"
					Else If SalListQuerySelection ( cmbType ) = 2
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " AND modif_type = 'd'  ORDER BY kartella_ist.date_modif desc, st_f.epvn, st_f.onoma, st_f.patr"
					Else 
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " ORDER BY kartella_ist.date_modif desc, kartella_ist.modif_type, st_f.epvn, st_f.onoma, st_f.patr"
			Function: UsersBuildNativeSelect
				Description: 
				Returns 
				Parameters 
				Static Variables 
				Local variables 
				Actions 
					Call SalHideWindow(tblKartella_ist.colUsersEpvn)
					Call SalHideWindow(tblKartella_ist.colUsersOnoma)
					Call SalHideWindow(tblKartella_ist.colUsersPatr)
					Call SalHideWindow(tblKartella_ist.colUsersKmTitlos)
					Set sFirstWhere =  ' KARTELLA_IST.K_TM  = :nCodeTmima '
					Set tblKartella_ist.sWhereClauseTbl = STRING_Null
					If dfUsersDateApo != DATETIME_Null
						Set tblKartella_ist.sWhereClauseTbl = sFirstWhere || " AND DATE_MODIF BETWEEN :dfUsersDateApo AND :dfUsersDateEos "
						! Set tblKartella_ist.sWhereClauseTbl = sFirstWhere || " AND DATE_MODIF >=  :dfUsersDateApo AND DATE_MODIF <= :dfUsersDateEos "
					Else 
						Set tblKartella_ist.sWhereClauseTbl = sFirstWhere
					If sUsersName != 'ALL USERS'
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " AND KARTELLA_IST.USERNAME = :sUsersName"
					If dfUsersEtos != NUMBER_Null
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " AND KARTELLA_IST.AK = :dfUsersEtos "
					If sXU_Users != STRING_Null
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " AND KARTELLA_IST.XU = :sXU_Users "
					If dfUsersKf != STRING_Null
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " AND KARTELLA_IST.K_F LIKE '"
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || dfUsersKf || "%' "
					If dfUsersKm != NUMBER_Null
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || ' AND KARTELLA_IST.K_M  = :dfUsersKm '
					If SalListQuerySelection ( cmbType) = 3
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " ORDER BY kartella_ist.date_modif desc, kartella_ist.modif_type, kartella_ist.k_f "
					Else If SalListQuerySelection ( cmbType) = 0
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " AND modif_type = 'i'   ORDER BY kartella_ist.date_modif desc "
					Else If SalListQuerySelection ( cmbType ) = 1
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " AND modif_type = 'u'  ORDER BY kartella_ist.date_modif desc "
					Else If SalListQuerySelection ( cmbType ) = 2
						Set tblKartella_ist.sWhereClauseTbl = tblKartella_ist.sWhereClauseTbl || " AND modif_type = 'd'  ORDER BY kartella_ist.date_modif desc "
					Else 
