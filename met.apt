					String: sTextCol
				Message Actions
					On SAM_Create
						!
						!
						Set sTabName = 'METWEBAITKARTL'
						Set sWhereClauseTbl =  " METWEBAITKARTL.TMIMACODE  = :nCodeTmima   "
						Set sTabColList = 'TMIMACODE'
						Set sTabValList = ':nCodeTmima '
						! Set sWhereTbl =  " WEBAIT_PISTOP.k_tm = :nCodeTmima and WEBAIT_PISTOP.UriID = :colUriIDRep and WEBAIT_PISTOP.k_Pistop = :colK_Pistop"
						Set sWhereTbl =  " METWEBAITKARTL.TMIMACODE= :nCodeTmima "
						Set nFill = TBL_FillAll
						! Set nFill = TBL_FillNormal
					On PAM_POPULATE
						!
						Set nPrintAit = 0
						Call Populate()
						Call tblWebKartl.colKEpvnRep.SetSelectionCol(tblWebKartl,tblWebKartl.colKMitrooRep)
						Call tblWebKartl.colKOnomaRep.SetSelectionCol(tblWebKartl,tblWebKartl.colKMitrooRep)
						Call tblWebKartl.colKStatusLektAit.SetSelectionCol(tblWebKartl,tblWebKartl.colKPistoStatus)
						Call tblWebKartl.colKPeriodEgrText.SetSelectionCol(tblWebKartl, tblWebKartl.colKPeriodEgrCode)
						Call CalcAllRows(tblWebKartl)
						Set dfAllAit = SalNumberToStrX(nPrintAit,0) || ' επιλεγμένες αιτήσεις '
					On SAM_FetchRowDone
						! Call SqlImmediate( 
								"select titlos 
								 from metcourse
								 into :tblWebKartl.colKCourText 
								 where tmimacode = :nCodeTmima and courcode = :tblWebKartl.colKCourCode and period = :tblWebKartl.colKPeriodEgrCode and ak = :tblWebKartl.colKAkEgr" )
						Call SqlImmediate( 
								"select titlos 
								 from metcourse
								 into :tblWebKartl.colKCourText 
								 where tmimacode = :nCodeTmima and courcode = :tblWebKartl.colKCourCode and ak = :tblWebKartl.colKAkEgr" )
						Call SqlClearImmediate(  )
						! Μετά το populate της γραμμής ελέγχει το status της αίτησης και ανάλογα χρωματίζει τα πεδία
						! η αίτηση σε εξέλιξη ...
						If tblWebKartl.colKPistoStatus = 0
							Call SalTblSetCellTextColor( tblWebKartl.colKA_A, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKMitrooRep, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKEpvnRep, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKOnomaRep, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKAkEgr, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKPeriodEgrText, COLOR_Black, FALSE )
							! Call SalTblSetCellTextColor( tblWebKartl.colKArPrvt, COLOR_Black, FALSE )
							! Call SalTblSetCellTextColor( tblWebKartl.colKAitDate, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKCourCode, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKPistoStatus, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKStatusLektAit, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKPistoReply, COLOR_Black, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKCourText, COLOR_Black, FALSE )
							! για να εκτυπωθεί ...
							Set tblWebKartl.colKStatusRep = 1
							Set nPrintAit = nPrintAit + 1
						! η διαδικασία ολοκληρώθηκε αλλά η εκτύπωση δεν έγινε για λόγους που αφορούν τον φοιτητή . Υπάρχουν σχόλια.
								 Από εμάς θεωρείται τελειωμένη και στέλνεται στον Server του πρωτοκόλλου.
						Else If tblWebKartl.colKPistoStatus = 2
							Call SalTblSetCellTextColor( tblWebKartl.colKA_A, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKMitrooRep, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKEpvnRep, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKOnomaRep, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKAkEgr, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKPeriodEgrText, COLOR_Red, FALSE )
							! Call SalTblSetCellTextColor( tblWebKartl.colKArPrvt, COLOR_Red, FALSE )
							! Call SalTblSetCellTextColor( tblWebKartl.colKAitDate, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKCourCode, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKPistoStatus, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKStatusLektAit, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKPistoReply, COLOR_Red, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKCourText, COLOR_Red, FALSE )
							! Set colStatusRep = 1
							! Set nPrintAit = nPrintAit + 1
						! η διαδικασία της εκτύπωσης του πιστοποιητικού ολοκληρώθηκε.
						Else If tblWebKartl.colKPistoStatus = 1
							Call SalTblSetCellTextColor( tblWebKartl.colKA_A, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKMitrooRep, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKEpvnRep, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKOnomaRep, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKAkEgr, COLOR_DarkGreen, FALSE )
							Call SalTblSetCellTextColor( tblWebKartl.colKPeriodEgrText, COLOR_DarkGreen, FALSE )
							! Call SalTblSetCellTextColor( tblWebKartl.colKArPrvt, COLOR_DarkGreen, FALSE )
							! Call SalTblSetCellTextColor( tblWebKartl.colKAitDate, COLOR_DarkGreen, FALSE )
