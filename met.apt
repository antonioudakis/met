		Functions
		Window Parameters
			! Number: nVal
			Receive Number: nProtPar
			Receive Number: nEpil
			Receive Number: nEpil1
			Receive Number: nParab
			Receive Boolean: bEnglish
			! Receive Number: nDiplPar
		Window Variables
			Number: nEpilSign
			Number: nEpilSign1
		Message Actions
			! On SAM_Create
				!
				! Set sLang = 'G'
				                                                                                                                                                                                                                                  Set sTabName = 'TREXON'
				                                                                                                                                                                                                                                  Call InitializeClass('BASE', 'SYSADM', 'sysadm' )
				! Time to connect the sql handle
				                                                                                                                                                                                                                                  Call Connect()
				! Set DBP_PRESERVE to TRUE
				                                                                                                                                                                                                                                  Call SetIsolationLevel('RL')
				                                                                                                                                                                                                                                  Call SetParameter(DBP_PRESERVE, TRUE, '')
			On SAM_CreateComplete
				!
				! Set dfAkPrint = nAkPar
				Set dfArProt = nProtPar
				! Set dfArDiplot = nDiplPar
				! Μόνο Διπλώτυπο και Έτος
				! If nVal = 1
					                                                                                                                                                                                                                                Call SalDisableWindow( dfArDiplot )
				! Μόνο   Έτος
				! If nVal = 2
					! Call SalDisableWindow( dfArDiplot )
					                                                                                                                                                                                                                                Call SalDisableWindow(  dfArProt )
				! Μόνο  Πρωτόκολλο
				! If nVal = 3
					! Call SalDisableWindow( dfArDiplot )
					! Call SalDisableWindow(  dfAkPrint )
				! Μόνο Διπλώτυπο και Πρωτόκολλο
				! If nVal = 4
					                                                                                                                                                                                                                                Call SalDisableWindow(  dfAkPrint )
				! Μόνο Υπογραφή
				! If nVal = 5
					                                                                                                                                                                                                                                Call SalDisableWindow(  dfAkPrint )
					                                                                                                                                                                                                                                Call SalDisableWindow( dfArDiplot )
					                                                                                                                                                                                                                                Call SalDisableWindow(  dfArProt )
				! Μόνο Υπογραφή και όχι υπογραφή από πρόεδρο
				! If nVal = 6
					                                                                                                                                                                                                                                Call SalDisableWindow(  dfAkPrint )
					                                                                                                                                                                                                                                Call SalDisableWindow( dfArDiplot )
					                                                                                                                                                                                                                                Call SalDisableWindow(  dfArProt )
					                                                                                                                                                                                                                                Call SalDisableWindow(  rbPresident )
				Set nEpilSign = nEpil
				If nEpilSign = 0 or nEpilSign = NUMBER_Null
					!
					Set nEpilSign = 1
				Select Case nEpilSign
					Case 1
						!
						Set rbGram = TRUE
						Break
					Case 2
						!
						Set rbGramAn = TRUE
						Break
					Case 3
						!
						Set rbPresident = TRUE
						Break
					Case 4
						!
						Set rbPresidentAn = TRUE
						Break
				Set nEpilSign1 = nEpil1
				If nEpilSign1 = 0 or nEpilSign1 = NUMBER_Null
					!
					Set nEpilSign1 = 1
				Select Case nEpilSign1
					Case 1
						!
						Set rbDieuth = TRUE
						Break
					Case 2
						!
						Set rbAnDieuth = TRUE
						Break
					Case 3
						!
						Set rbPresident1 = TRUE
						Break
				Set bGram           = FALSE
				Set bGramAn       = FALSE
				Set bPresident     = FALSE
				Set bPresidentAn = FALSE
				Set dfArProt = NUMBER_Null
				Set bEnglish         = FALSE
				Set cbEnglish       = FALSE
				! Set dfArDiplot = NUMBER_Null
				! Set dfAkPrint = nAkTrexon
			On SAM_Close
				!
				! Call Disconnect()
				! Set nAkPar = dfAkPrint
				Set nProtPar = dfArProt
				Set nParab = dfArParab
				! Set nDiplPar = dfArDiplot
				If rbGramAn = TRUE 
					Set nEpilSign = 2
				Else If rbPresident = TRUE
					Set nEpilSign = 3
				Else If rbPresidentAn = TRUE
					Set nEpilSign = 4
				Else
					Set nEpilSign = 1
				If rbAnDieuth = TRUE 
					Set nEpilSign1 = 2
				Else If rbPresident1 = TRUE
					Set nEpilSign1 = 3
				Else
					Set nEpilSign1 = 1
				Set nEpil = nEpilSign
				Set nEpil1 = nEpilSign1
				If cbEnglish = TRUE
					Set bEnglish = TRUE
				Else
					Set bEnglish = FALSE
				Call SalEndDialog(hWndForm, TRUE )
