			Function: YpoxrFoit
				Description: Function Υπεύθυνη για την χρέωση υποχρεώσεων
						(υποχρεωτικών ονομαστικά ,επιλογών αριθμητικά) στον φοιτητή
				Returns 
					Boolean: 
				Parameters 
					String: sEjSelected
					String: sKatSelected
					Number: nAKSelected
				Static Variables 
				Local variables 
					Number: nEjYp
					Number: nTest
					Number: nFetch
				Actions 
					! ****************************************************************************************************************************
					Set sEjSelectedEgr  = sEjSelected
					Set sKatSelectedEgr = sKatSelected
					Set nAKSelectedEgr = nAKSelected
					! ****************************************************************************************************************************
					Set nTest = SalStrToNumber( sEjSelected )
					If (nTest != 0 )  and (nTest != 10 )  and ( sK_fEgr  != '  '  or  sK_fEgr  != STRING_Null )
						!
						If NOT hGetCourse.bConnected
							!
							Call hGetCourse.Connect()
						!
						!
						! υποχρεωτικά
						Call hGetCourse.Initialize(' SELECT k_m
								                                          INTO :dlgEgrafi.nK_mYp
								                                          FROM oblistud
								                                          WHERE k_f = :sK_fEgr  AND
								                                                         ej = :sEjSelectedEgr AND
								                                                         k_tm = :nCodeTmima ' )
						Call hGetCourse.Prepare()
						Call hGetCourse.Execute()
						Call hGetCourse.Next( nFetch )
						! Αν  δεν έχει  χρεωθεί υποχρεώσεις
						If nFetch = FETCH_EOF
							Call hGetCourse.Initialize(' SELECT k_m
									                                          INTO :dlgEgrafi.nK_mYp
									                                          FROM ypcourse
									                                          WHERE kat = :sKatSelectedEgr  AND
									                                                         ej = :sEjSelectedEgr  AND
									                                                         k_tm = :nCodeTmima  AND
									                                                         ak = :nAKSelectedEgr AND
									                                                         omada is null' )
							Call hGetCourse.Prepare()
							Call hGetCourse.Execute()
							Call hGetCourse.Next( nFetch )
							If NOT hSetCourse.bConnected
								!
								Call hSetCourse.Connect()
							Call hSetCourse.Initialize(' INSERT
									                                         INTO oblistud ( k_f , k_tm , ej ,k_m , ak )
									                                         VALUES ( :sK_fEgr  , :nCodeTmima , :sEjSelectedEgr, :dlgEgrafi.nK_mYp , :nAKSelectedEgr  )')
							Call hSetCourse.Prepare()
							While nFetch != FETCH_EOF
								Call hSetCourse.Execute()
								Call hGetCourse.Next( nFetch )
							Call hSetCourse.Commit()
						! αριθμός επιλογών από ομάδες
						If nCodeTmima =15
							! Για αυτό το τμήμα δεν χρεώνω υποχρεώσεις επιλογών/Το κάνω από την κατεύθυνση της δήλωσης
							Return TRUE
						Set dlgEgrafi.nMaxTotCh = 0
						Call hGetCourse.Initialize(' SELECT totepil
								                                          INTO :dlgEgrafi.nTotal
								                                          FROM oblistu
								                                          WHERE k_f = :sK_fEgr   AND
								                                                         ej = :sEjSelectedEgr  AND
								                                                         k_tm = :nCodeTmima ' )
						Call hGetCourse.Prepare()
						Call hGetCourse.Execute()
						Call hGetCourse.Next( nFetch )
						! Αν  έχει ήδη χρεωθεί υποχρεώσεις
						If nFetch != FETCH_EOF
							If hGetCourse.bConnected
								!
								Call hGetCourse.Disconnect()
							If hSetCourse.bConnected
								!
								Call hSetCourse.Disconnect()
							Return TRUE
						Else 
							!
							Call hGetCourse.Initialize(' SELECT SUM( omadepim.maxchoice )
									                                          INTO :dlgEgrafi.nMaxTotCh
									                                          FROM omadepim
									                                          WHERE  omadepim.ej = :sEjSelectedEgr AND
									                                                          omadepim.k_tm = :nCodeTmima  AND
									                                                          omadepim.kat = :sKatSelectedEgr  AND
									                                                          ak = :nAKSelectedEgr ' )
							Call hGetCourse.Prepare()
							Call hGetCourse.Execute()
							Call hGetCourse.Next( nFetch )
							! αριθμός επιλογών από υπερομάδες
							Call hGetCourse.Initialize(' SELECT SUM( ypmadepm.maxchoice )
									                                         INTO  :dlgEgrafi.nMaxChYp
									                                         FROM ypmadepm
									                                         WHERE  ypmadepm.ej = :sEjSelectedEgr  AND
									                                                        ypmadepm.k_tm = :nCodeTmima  AND
									                                                        ypmadepm.kat = :sKatSelectedEgr  AND
									                                                        ak = :nAKSelectedEgr ' )
							Call hGetCourse.Prepare()
							Call hGetCourse.Execute()
							Call hGetCourse.Next( nFetch )
							If nFetch != FETCH_EOF
								!
								Set dlgEgrafi.nMaxTotCh = dlgEgrafi.nMaxTotCh + dlgEgrafi.nMaxChYp
							If NOT hInsCourse.bConnected
								!
								Call hInsCourse.Connect()
							Call hInsCourse.Initialize(' INSERT
									                                        INTO oblistu ( k_f , k_tm , ej , totepil )
									                                        VALUES ( :sK_fEgr , :nCodeTmima, :sEjSelectedEgr,  :dlgEgrafi.nMaxTotCh)  ' )
							Call hInsCourse.Prepare()
							Call hInsCourse.Execute()
							Call hInsCourse.Commit()
					!
					If hGetCourse.bConnected
						!
						Call hGetCourse.Disconnect()
					If hSetCourse.bConnected
						!
						Call hSetCourse.Disconnect()
					If hInsCourse.bConnected
						!
						Call hInsCourse.Disconnect()
