			Function: Read_Kart_Disk
				Description:
				Returns
				Parameters
					! String: sFile
					! Number: nMode
				Static Variables
				Local variables
					String: sSelect
					Number: nFetch
					Boolean: bFirstTime
				Actions
					If NOT hkart_disk.bConnected
						!
						Call hkart_disk.Connect()
					Call SalTblReset( tblKartelaVa )
					Set bFirstTime = TRUE
					Set sSelect = " SELECT progcode,courcode, lcourcode,ak, period, mitroo , epvn , onoma , patr  , gradenor , gradepan
							                        FROM met_kart_disk
							                        INTO :frmVathmi.nProgCode,:dfCourseCodeV, :dfTitlosCourseV, :dfAkEtosVa, :sPeriod, :sK_F , :sEpvn , :sOnoma , :sPatr , :nGradeNor , :nGradePan
							                        ORDER BY  epvn , onoma , patr "
					Call hkart_disk.Initialize( sSelect )
					Call hkart_disk.Prepare()
					Call hkart_disk.Execute()
					Call hkart_disk.Next( nFetch )
					While nFetch != FETCH_EOF
						!
						If bFirstTime
							Set cmbProgCode.sValues[ cmbProgCode.nPosInClass ] = SalNumberToStrX( frmVathmi.nProgCode, 0 )
							Call cmbProgCode.SetSelection()
							Set cmbPeriodTest.sValues[ cmbPeriodTest.nPosInClass ] = sPeriod
							Call cmbPeriodTest.SetSelection()
							Set bFirstTime = FALSE
						Call SalTblInsertRow( tblKartelaVa, TBL_MaxRow )
						Call SalTblSetColumnText( tblKartelaVa, 1, sK_F )
						Call SalTblSetCellTextColor( tblKartelaVa.colK_fVa, COLOR_Red, FALSE )
						Call SalTblSetColumnText( tblKartelaVa, 2, sEpvn )
						Call SalTblSetColumnText( tblKartelaVa, 3, sOnoma )
						Call SalTblSetColumnText( tblKartelaVa, 4, sPatr )
						! Διαχωρίστηκε και γίνεται ξεχωριστό select για τους βαθμούς της κανονικής για την περίπτωση που τα δελτία κανονικής και επαναληπτικής
								γίνανε μαζί οπότε στο δελτίο της επαν. δεν έχουν καταχωρηθεί οι βαθμοί της κανονικής. 
						If sPeriod = '1'
							Call SqlImmediate (' Select gradepan INTO :nGradePan from metkartl where progcode=:frmVathmi.nProgCode and mitroo = :sK_F and courcode = :dfCourseCodeV and ak = :dfAkEtosVa' )
						Else If sPeriod = '2'
							Call SqlImmediate (' Select gradenor INTO :nGradeNor from metkartl where progcode=:frmVathmi.nProgCode and mitroo = :sK_F and courcode = :dfCourseCodeV and ak = :dfAkEtosVa' )
						Call SalTblSetColumnText( tblKartelaVa, 5, SalNumberToStrX( nGradeNor, 2 ) )
						Call SalTblSetColumnText( tblKartelaVa, 6, SalNumberToStrX( nGradePan, 2 )  )
						Call hkart_disk.Next( nFetch )
					! Call SalSendMsg( cmbProgCode, SAM_Click, 0, 0 )
					If hkart_disk.bConnected
						!
						Call hkart_disk.Disconnect()
					Call SalTblSetFlagsAnyRows( tblKartelaVa , ROW_New , FALSE , 0 , 0 )
					Call SalTblSetFlagsAnyRows( tblKartelaVa , ROW_Edited , TRUE , 0 , 0 )
					Call SqlClearImmediate()
