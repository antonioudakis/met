								"select  count(courcode) from metkartl where progcode = :sProgCode and tmimacode = :nCodeTmima and mitroo = '"||sMitroo||"' and (gradenor >=5 or gradepan >=5) and epepil
								 from metprogat  
								 into  :nLimit, :nLimitDipl, :nwDipl, :nCourDipl, :sLRound
								 where progcode = :sProgCode and tmimacode = :nCodeTmima 
								       and ak = (select ak from metstuprog where mitroo = '"||sMitroo||"' and progcode = :sProgCode)")
					! ! Συλλέγω τις ιδιότητες του προγράμματος
					Call SqlImmediate(
							"select  limit,limitDipl,wdipl,arcourdipl,roundmo 
							 from metprogat  
							 into  :nLimit, :nLimitDipl, :nwDipl, :nCourDipl, :sLRound
							 where progcode = :sProgCode and tmimacode = :nCodeTmima 
							       and ak = (select ak from metstuprog where mitroo = '"||sMitroo||"' and progcode = :sProgCode)")
					Call SqlClearImmediate(  )
					If nLimit = NUMBER_Null
						Set nLimit = 5
					If nLimitDipl = NUMBER_Null
						Set nLimitDipl = 5.5
					If nCodeTmima = 3 and sProgCode = '1'
						Call SqlImmediate( 
								"select ofeiles 
								 from met 
								 into :Mixanik 
								 where mitroo ='"||sMitroo||"'")
						If Mixanik = '1' 
							! Set nCourDipl = 4
							Set nCourDipl = 6
						Else
							Set nCourDipl = 6
					Else If nCodeTmima != 3 and sProgCode = '1'
						Set sMixanikMaster = CheckMixanikMaster(sMitroo)
						If sMixanikMaster = STRING_Null
							Return NUMBER_Null
						Else If sMixanikMaster = '11'
							! Set nCoursesNeeded = 4
							Set nCourDipl = 6
						Else If sMixanikMaster = '10'
							Set nCourDipl = 6
						Else
							Call SqlImmediate (
									"select pm 
									 from metstuprog
									 into :sPM
									 where metstuprog.mitroo ='"||sMitroo||"' and metstuprog.progcode=:sProgCode")
							Call SqlClearImmediate()
							If sPM ='1'
								Set nCourDipl = 6
							Else
								Return NUMBER_Null
					! !!!
					! ! Εύρεση των μαθημάτων που έχει δηλώσει στο πρόγραμμα και είναι προαπαιτούμενα
					Call SqlConnect(hCourse)
					Set sSelect = 
							"select distinct courcode  
							 from metkartl  
							 into  :nCourCode
							 where progcode = :sProgCode and mitroo = '"||sMitroo||"' and typos = 'PR'"
					Call SqlPrepare( hCourse,sSelect)
					Call SqlExecute(hCourse)
					Call SqlFetchNext( hCourse, nFetch )
					! ! ΄Ελεγχος αν έχει προβιβάσιμο βαθμό στα προαπαιτούμενα μαθήματα
					While nFetch != FETCH_EOF
						If SqlConnect(hGrade)
							Set bGradeHandler = TRUE
						Set sSelect = 
								"select @if(@abs(@nullvalue(gradenor,0)-@nullvalue(gradepan,0)) - (@nullvalue(gradenor,0)-@nullvalue(gradepan,0)),gradepan,gradenor)  
								 from metkartl  
								 into  :nGrade
								 where  progcode = :sProgCode and mitroo = '"||sMitroo||"' and courcode = :nCourCode and typos = 'PR'"
						Call SqlPrepare(hGrade,sSelect)
						Call SqlExecute(hGrade)
						Call SqlFetchNext( hGrade,nFetch1)
						If (nGrade >= nLimit)
							Call SqlFetchNext( hCourse,nFetch)
						Else
							If NOT bMultiplePrints
								Call SalMessageBox( 'Ο φοιτητής δεν έχει προβιβάσιμο βαθμό σε προαπαιτούμενα μαθήματα!', '', MB_IconStop | MB_Ok )
							If bGradeHandler
								Call SqlDisconnect(hGrade)
							Call SqlDisconnect(hCourse)
							Return NUMBER_Null
					! !!!!
					Call SqlImmediate(
							"select  ak from metstuprog into :nAk where mitroo = '"||sMitroo||"' and progcode = :sProgCode")
					Call SqlClearImmediate(  )
					! ! Εύρεση των μαθημάτων που έχει δηλώσει στο πρόγραμμα και ΔΕΝ είναι προαπαιτούμενα 
					Set sSelect = 
							"select distinct courcode  
							 from metkartl  
							 into  :nCourCode
							 where progcode = :sProgCode and mitroo = '"||sMitroo||"' and (typos is null or typos != 'PR') 
							       and courcode not in (select courcode from metkartl where progcode = :sProgCode and mitroo = '"||sMitroo||"' and (typos is null or typos != 'PR') and datedel is not null)"
					Call SqlPrepare(hCourse,sSelect)
					Call SqlExecute(hCourse)
					Call SqlFetchNext( hCourse,nFetch)
					! Βρίσκει τον μέγιστο βαθμό κανονικής και επαναληπτικής περιόδου και το ακαδημαϊκό έτος που πήρε τον βαθμό 
							! Αν έχει μία τουλάχιστον απαλλαγή στο μάθημα θα εμφανίζει το ακαδημαϊκό έτος που πήρε την απαλλαγή και θα αγνοηθεί ο βαθμός, αν υπάρχει
					While nFetch != FETCH_EOF
						!
						Set nMODipl = 0
						Set nCountMO = 0
						Set bFTime = TRUE
						Set sSelect = 
								"select @if(@abs(@nullvalue(gradenor,0)-@nullvalue(gradepan,0))-(@nullvalue(gradenor,0)-@nullvalue(gradepan,0)),gradepan,gradenor),apalag,passfail,epepil,ak 
								 from metkartl  
								 into  :nGradeVaMau, :nApalag, :nPassFail,:sEpEpil,:nAkMau 
								 where progcode = :sProgCode and mitroo = '"||sMitroo||"' and courcode = :nCourCode 
								       and (typos is null or  typos != 'PR') 
								 order by 3desc, 2 desc,1 desc,4 desc"
						Call SqlImmediate( sSelect )
						Call SqlClearImmediate()
						Set sSelect = "select ak from metstuprog where mitroo ='"||sMitroo||"' and progcode = :sProgCode"
						Set sSelect = 
								"select monades  
								 from metcourse  
								 into  :wFactor
								 where   courcode = :nCourCode and ak = :nAkMau"
						Call SqlImmediate( sSelect )
						Call SqlClearImmediate()
						If (nApalag != 1 or nApalag = NUMBER_Null) and (nPassFail !=1 or nPassFail = NUMBER_Null) 
							If sEpEpil != '1' or sEpEpil = STRING_Null
								If wFactor != NUMBER_Null
									If (nWorkTmima = 3 and sProgCode = '2')
										If nAk <= 2014
											Set nGradeVaDipl = nGradeVaDipl + (nGradeVaMau * wFactor)
											Set nMau = nMau + 1
											Set nMauDiorth = nMauDiorth + wFactor
										Else
											If nGradeVaMau >= nLimit
												Set nGradeVaDipl = nGradeVaDipl + (nGradeVaMau * wFactor)
												Set nMau = nMau + 1
												Set nMauDiorth = nMauDiorth + wFactor
									Else If (nWorkTmima = 3 and sProgCode = '3' and nAk <= 2014)
										If nGradeVaMau >= nLimit
											Set nGradeVaDipl = nGradeVaDipl + nGradeVaMau
											Set nMau = nMau + 1
											Set nMauDiorth = nMauDiorth + 1
									Else
										If nGradeVaMau >= nLimit
											Set nGradeVaDipl = nGradeVaDipl + (nGradeVaMau * wFactor)
											Set nMau = nMau + 1
											Set nMauDiorth = nMauDiorth + wFactor
								Else
									! Προεπιλεγμένη τιμή 1
									If nWorkTmima = 3 and sProgCode = '2'
										Set nGradeVaDipl = nGradeVaDipl + nGradeVaMau
										Set nMau = nMau + 1
										Set nMauDiorth = nMauDiorth + 1
									Else
										If nGradeVaMau >= nLimit
											Set nGradeVaDipl = nGradeVaDipl + nGradeVaMau
											Set nMau = nMau + 1
											Set nMauDiorth = nMauDiorth + 1
						Else
							If wFactor != NUMBER_Null
								Set nCourDipl = nCourDipl - wFactor
							Else
								Set nCourDipl = nCourDipl - 1
						Call SqlFetchNext( hCourse,nFetch)
						! !
					! ! Ελέγχει αν ο μ.φ. έχει περάσει τον απαιτούμενο αριθμό μαθημάτων στο πρόγραμμα
					If NOT UseECTS(sMitroo,nWorkTmima,sProgCode)
						If (nWorkTmima = 3 and sProgCode = '2')
							Set nCourDipl = nMau
					Else
						Set nMau = nMauDiorth
					If nCourDipl > nMau
						If bGradeHandler
							Call SqlDisconnect(hGrade)
						Call SqlDisconnect(hCourse)
						Return NUMBER_Null
					! Αν υπολογίζω τον μέσο όρο μόνο των μαθημάτων
					If (bCourseOnly =TRUE)
						! If sLRound = '1' !! Στρογγυλοποίηση στο πρώτο δεκαδικό !!
							Set nGradeVaDipl = SalNumberRound((nGradeVaDipl/nMauDiorth)*10)/10
						! Else
							Set nGradeVaDipl = SalNumberRound((nGradeVaDipl/nMauDiorth)*100)/100
						Set nGradeVaDipl = SalNumberRound((nGradeVaDipl/nMauDiorth)*100)/100
						Call SqlDisconnect(hCourse)
						If bGradeHandler
							Call SqlDisconnect(hGrade)
						Return nGradeVaDipl
					! Αλλιώς αν υπολογίζω τον μέσο όρο των μαθημάτων σε συνδιασμό με την διπλωματική εργασία
					Else
						! !
						! !!!!!!!!Υπολογισμός Βαθμού Διπλωματικής!!!!!!!!!!!!!!
						! !
						Set sSelect = 
								"select codesyn  
								 from metdsynerg  
								 into  :nCodeSynStud
								 where   progcode = :sProgCode and mitroo = '"||sMitroo||"'"
						Call SqlImmediate( sSelect )
						Call SqlClearImmediate(  )
						If nCodeSynStud != NUMBER_Null or nCodeSynStud != 0
							!
							Set nMODipl = 0
							Set nCountMO = 0
							Set sSelect = 
									"select grade 
									 from metdiplgrade  
									 into  :nGradeDipl
									 where  codesyn = :nCodeSynStud"
							If (bGradeHandler != TRUE)
								If SqlConnect(hGrade)
									Set bGradeHandler = TRUE
							Call SqlPrepare(hGrade,sSelect)
							Call SqlExecute(hGrade)
							Call SqlFetchNext( hGrade,nFetch )
							While nFetch != FETCH_EOF
								!
								If nGradeDipl != NUMBER_Null
									Set nMODipl = nMODipl + nGradeDipl
									Set nCountMO = nCountMO + 1
								Call SqlFetchNext( hGrade,nFetch )
							If nCountMO > 0
								If sLRound = '1'
									Set nMODipl = nMODipl / nCountMO
									Set nModDipl = SalNumberMod( nMODipl*100, 100 )
									Set nMODipl = nMODipl - (nModDipl /100)
									If nModDipl >= 75
										Set nMODipl = nMODipl + 1
									If nModDipl >= 25 and nModDipl < 75
										Set nMODipl = nMODipl + 0.5
								Else
									Set nMODipl = nMODipl / nCountMO
									Set nMODipl = SalNumberRound(( nMODipl ) *100)/100
								If UseECTS(sMitroo,nWorkTmima,sProgCode)
									Set nGradeVaDipl = SalNumberRound((nGradeVaDipl + (nMODipl*nwDipl))/ ( nMauDiorth + nwDipl )*100)/100
								Else If (nWorkTmima = 3 and sProgCode = '2')
									Set nGradeVaDipl = SalNumberRound((nGradeVaDipl + (nMODipl*nwDipl))/ ( nMauDiorth + nwDipl )*100)/100
								Else If (nWorkTmima = 3 and sProgCode = '3')
									Set nGradeVaDipl = SalNumberRound((nGradeVaDipl + (nMODipl*nwDipl))/ ( nMauDiorth + nwDipl )*100)/100
								Else
									Set nGradeVaDipl = SalNumberRound((nGradeVaDipl + ( (nwDipl/nCourDipl) * nMauDiorth * nMODipl))/ ( ((nwDipl+nCourDipl)/nCourDipl) * nMauDiorth)*100)/100
								If nMODipl < nLimitDipl
									Call SalMessageBox( 'Ο Φοιτητής έχει βαθμό διπλωματικής εργασίας μικρότερο του βαθμού βάσης διπλωματικής εργασίας !', '', MB_IconStop | MB_Ok )
									If bGradeHandler
										Call SqlDisconnect(hGrade)
									Call SqlDisconnect(hCourse)
									Return NUMBER_Null
							Else
								Call SalMessageBox( 'Ο Φοιτητής δεν έχει βαθμό διπλωματικής εργασίας, ενημερώστε τη φόρμα της Ανάθεσης !', '', MB_IconStop | MB_Ok )
								If bGradeHandler
									Call SqlDisconnect(hGrade)
								Call SqlDisconnect(hCourse)
								Return NUMBER_Null
						Else
							!
							Set nModDipl = NUMBER_Null
							Call SalMessageBox( 'Ο Φοιτητής δεν έχει κωδικό συνεργασίας, ενημερώστε τη φόρμα της Ανάθεσης !', '', MB_IconStop | MB_Ok )
							If bGradeHandler
								Call SqlDisconnect(hGrade)
							Call SqlDisconnect(hCourse)
							Return NUMBER_Null
						If bGradeHandler
							Call SqlDisconnect(hGrade)
						Call SqlDisconnect(hCourse)
						Return nGradeVaDipl
			Function: CheckSameCoursePassedBefore
				Description:
				Returns
					Boolean:
				Parameters
