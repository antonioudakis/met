			Function: FoitExists
				Description:
				Returns
					Boolean:
				Parameters
					String: sMitroo
					Number: nProgCode
				Static Variables
				Local variables
				Actions
					! Ελέγχω αν ανήκει στους Υποψήφιους Διδάκτορες
					Set nExistFoit = NUMBER_Null
					Call SqlImmediate ( 
							"select count(met.mitroo) 
							 from met,metstuprog
							 into :nExistFoit 
							 where met.mitroo = metstuprog.mitroo and  met.mitroo = :sMitroo and metstuprog.progcode = :nProgCode")
					Call SqlClearImmediate()
					If nExistFoit = NUMBER_Null or nExistFoit = 0
						Return FALSE
					Else
						Return TRUE
			Function: Proc_PrintReport
				Description:
				Returns
					Boolean:
				Parameters
					String: sFormRep
				Static Variables
				Local variables
					Date/Time: dRealEgrDate
				Actions
					Set sMsgPrint = STRING_Null
					! ! Η συνάρτηση επιστρέφει το μέρος του αριθμού πρωτοκόλλου που χρειάζεται για την εκτύπωση
					! Set bFrmEggrafesCreate = TRUE
					If nCodeTmima = 3
						Call Search_CutStr('/',sArPrvt, sArPrvt)
					Else If nCodeTmima = 1
						Call Search_CutStr('|',sArPrvt, sArPrvt)
					Else
						Call Search_CutStr('/',sArPrvt, sArPrvt)
					Set nArProt = SalStrToNumber(sArPrvt)
					! ! Έλεγχος για την υπογραφή
					! ’νοιγμα της φόρμας και αναζήτηση του φοιτητή
					! Ελέγχω αν ανήκει στους προπτυχιακούς φοιτητές
					If FoitExists(sEgrMitroo,SalStrToNumber(sEgrProgCode))
						!
						! Τρέχουσες τιμές
						Call GetTrexon()
						Set bEnglish = FALSE
						Call GetTitlos(TRUE)
						! Φτιάχνει το κείμενο της υπογραφής
						! Call SignText( nSign, sSignText, sOrderBy, sGIFRA )
						!
						! Αν υπάρχει εγγραφή στο τρέχον έτος ή δεν υπάρχει αλλά ζητείται πιστοπ. που μπορεί να δοθεί συνεχίζει την διαδικασία κανονικά
						If NOT bFrmEggrafesCreate
							Call SalCreateWindowEx(dlgEgrafi,hWndForm,0,0,0,0,CREATE_Hidden, STRING_Null,STRING_Null,STRING_Null,STRING_Null,STRING_Null)
							Set bFrmEggrafesCreate = TRUE
						Set dlgEgrafi.sPROGCODE = sEgrProgCode
						Set dlgEgrafi.dfStudCodeEgr = sEgrMitroo
						Call SalSendMsg( dlgEgrafi.tblEgrafi, PAM_POPULATE, 0, 0 )
						Call SalSendMsg( dlgEgrafi.pbNewRec, SAM_Click, 0, 0 )
						Set dlgEgrafi.tblEgrafi.colPeriodCod = frmWebEgr.tblWebEgr.colEPeriodEgrCode
						Set dlgEgrafi.tblEgrafi.colDateEgr = SalStrToDate( sEgrDate )
						Call SalSendMsg( pbSaveEgrafi, SAM_Click, 0, 0 )
						If sMsgPrint = "Ο φοιτητής έχει ήδη εγγραφεί στο συγκεκριμένο Ακαδημαϊκό έτος και περίοδο."
							Call SqlImmediate( 
									"select dateegr 
									 from metegr 
									 into :dRealEgrDate
									 where mitroo = :dfStudCodeEgr and progcode = :sPROGCODE and tmimacode = :nCodeTmima and ak = :tblEgrafi.colAkEgr and period = :tblEgrafi.colPeriodCod")
							Set sMsgPrint = sMsgPrint||" - Ημερομηνία εγγραφής "||SalNumberToStrX( SalDateDay( dRealEgrDate ), 0 )||"/"||SalNumberToStrX( SalDateMonth( dRealEgrDate ), 0 )||"/"||SalNumberToStrX( SalDateYear( dRealEgrDate ), 0 )
							Call SqlClearImmediate(  )
						Else If sMsgPrint = STRING_Null or sMsgPrint = ""
							Set colEEgrDate = dlgEgrafi.tblEgrafi.colDateEgr
						Else
							Set colEEgrDate = DATETIME_Null
					Else
						Set sMsgPrint = ' Ο φοιτητής δεν υπάρχει στο αρχείο των Υποψηφίων Διδακτόρων. Να γίνει έλεγχος.'
						Set bMsgPrint = TRUE
					!
					! If bPrintReportOk
						! Set nErrR = 1
						!
						! PRINTER
						! Set strPrinterName = MTblPrintGetDefPrinterName()
						! If SalReportGetPrinterSettings ( SalNumberToWindowHandle(nRepType), strPrinterName, nOrientation, nPaperType, nWidth, nHeight) = TRUE
							Set strPrinterName = strPrinterName
						! If SalReportSetPrinterSettings ( SalNumberToWindowHandle(nRepType), strPrinterName, RPT_Landscape, RPT_PaperA5, 0, 0) = TRUE
							Set strPrinterName = strPrinterName
						! If NOT OpenPrinterA('HP1220CA3',hPrinter,0)
							Call ErrorMessageOk( ' Ο φοιτητής δεν εχει εγγραφές στο δοσμένο ακαδημαϊκό έτος ! ', '', 1 )
						! Call SalSendMsg(tblWebAitisi,WM_SPOOLERSTATUS,0,0)
						! PRINTER
						!
						Call SalModalDialog(dlgTimerMessage, hWndForm, frmEgrafi.dfStf )
						Call SalReportPrint( frmEgrafi, sQrpPath, sVariables, sInputs, SalStrToNumber(sCopies), RPT_PrintAll ,0,0, nErrR )
						! Call SalSendMsg( frmEgrafi , SAM_ReportFinish, 0 , 0 )
						! If sCodePistop = '2' or sCodePistop = '3' or sCodePistop = '4' or sCodePistop = '5' or sCodePistop = '6'
							Call SalDestroyWindow(frmReport)
						! Εάν εμφανιστούν μηνύματα κατά την εκτύπωση βάλτα στον πίνακα WebAitisi  ώστε να τα δεί ο χρήστης.
						If bMsgPrint
							Call UpdateWebAitisi(4)
						! Αν συμβεί λάθος κατά την εκτύπωση ενημέρωσε τον πίνακα webaitisi και επέστεψε FALSE ώστε να μην εκτελέσει τον υπόλοιπο κώδικα
						If nErrR != 0
							If NOT UpdateWebAitisi(2)
							Return FALSE
						! Αν όχι απλά δώσε την τιμή στο πεδίο printerror
						Else
							If NOT UpdateWebAitisi(1)
								Return FALSE
					! Else
						If NOT UpdateWebAitisi(4)
						Return FALSE
					!
					!
					! If bPrintReportOk
						!
						! PRINTER
						! Set strPrinterName = MTblPrintGetDefPrinterName()
						! If SalReportGetPrinterSettings ( SalNumberToWindowHandle(nRepType), strPrinterName, nOrientation, nPaperType, nWidth, nHeight) = TRUE
							Set strPrinterName = strPrinterName
						! If SalReportSetPrinterSettings ( SalNumberToWindowHandle(nRepType), strPrinterName, RPT_Landscape, RPT_PaperA5, 0, 0) = TRUE
							Set strPrinterName = strPrinterName
						! If NOT OpenPrinterA('HP1220CA3',hPrinter,0)
							Call ErrorMessageOk( ' Ο φοιτητής δεν εχει εγγραφές στο δοσμένο ακαδημαϊκό έτος ! ', '', 1 )
						! Call SalSendMsg(tblWebAitisi,WM_SPOOLERSTATUS,0,0)
						! PRINTER
						!
						If sForm = 'frmEgrafi'
							Call SalModalDialog(dlgTimerMessage, hWndForm, frmEgrafi.dfStf )
							Call SalReportPrint( frmEgrafi, sQrpPath, sVariables, sInputs, SalStrToNumber(sCopies), RPT_PrintAll ,0,0, nError )
						! Εάν εμφανιστούν μηνύματα κατά την εκτύπωση βάλτα στον πίνακα METWEBAITPISTOP  ώστε να τα δεί ο χρήστης.
						If bMsgPrint
							Call UpdateWebAitisi(4)
						! Αν συμβεί λάθος κατά την εκτύπωση ενημέρωσε τον πίνακα METWEBAITPISTOP και επέστεψε FALSE ώστε να μην εκτελέσει τον υπόλοιπο κώδικα
						If nError != 0
							If NOT UpdateWebAitisi(2)
								Return FALSE
						! Αν όχι απλά δώσε την τιμή στο πεδίο printerror
						Else 
							If NOT UpdateWebAitisi(1)
								Return FALSE
					! Else
						If NOT UpdateWebAitisi(4)
							Return FALSE
					!
					Return TRUE
			Function: Tbl_GetColValue
				Description:
				Returns
				Parameters
					Window Handle: whTbl
					Receive String: sCol1Par
					Receive String: sCol2Par
					Receive String: sCol3Par
					Receive String: sCol4Par
					Receive String: sCol5Par
					Receive String: sCol6Par
					Receive String: sCol7Par
				Static Variables
					Number: nPosRow
					Number: nMinRow
					Number: nMaxRow
					Number: nRow
					Boolean: bOtherRow
				Local variables
				Actions
					!
					! Αν γίνεται για πρώτη φορά ...
					If NOT bOtherRow
						Call SalTblQueryScroll ( whTbl, nPosRow, nMinRow, nMaxRow )
						Set nRow = nMinRow
						Set bOtherRow = TRUE
					!
					If (nRow <= nMaxRow)
						Call SalTblFetchRow( whTbl, nRow )
						Call SalTblGetColumnText( whTbl, 8, sCol1Par )
						Call SalTblGetColumnText( whTbl, 2, sCol2Par )
						Call SalTblGetColumnText( whTbl, 16, sCol3Par )
						Call SalTblGetColumnText( whTbl, 3, sCol4Par )
						Call SalTblGetColumnText( whTbl, 6, sCol5Par )
						Call SalTblGetColumnText( whTbl, 14, sCol6Par )
						Call SalTblGetColumnText( whTbl, 11, sCol7Par )
						Set nRow = nRow + 1
						Return TRUE
					!
					Else
						Set bOtherRow = FALSE
						Return FALSE
			Function: UpdateWebAitisi
				Description:
				Returns
					Boolean:
				Parameters
					Number: nMode
				Static Variables
				Local variables
				Actions
					Select Case nMode
						Case 1
							If sForm = 'frmEgrafi'
								! If NOT SqlImmediate ( 'UPDATE WEBAIT_PISTOP SET PRINTERROR = :frmEgrafi.nErrR WHERE UriID = :sAA and k_pistop = :sCodePistop ')
									Return FALSE
								If NOT SqlImmediate ( 'UPDATE METWEBAITPISTOP SET PRINTERROR = :frmEgrafi.nError WHERE AID = :sAA AND TMIMACODE = :nCodeTmima ')
									Return FALSE
							Call SqlClearImmediate()
							Return TRUE
						Case 2
							If sForm = 'frmEgrafi'
								! If NOT SqlImmediate ( 'UPDATE WEBAIT_PISTOP SET PRINTERROR = :frmEgrafi.nErrR WHERE UriID = :sAA and k_pistop = :sCodePistop ')
									Return FALSE
								If NOT SqlImmediate ( 'UPDATE METWEBAITPISTOP SET PRINTERROR = :frmEgrafi.nError WHERE AID = :sAA AND TMIMACODE = :nCodeTmima  ')
									Return FALSE
							! If NOT SqlImmediate ( 'UPDATE WEBAIT_PISTOP SET PISTOSTATUS = 0 WHERE UriID = :sAA and k_pistop = :sCodePistop ')
								Return FALSE
							If NOT SqlImmediate ( 'UPDATE METWEBAITPISTOP SET PISTOSTATUS = 0 WHERE AID = :sAA AND TMIMACODE = :nCodeTmima  ')
								Return FALSE
							Call SqlClearImmediate()
							Return TRUE
						Case 3
							If NOT SqlImmediate ( 'UPDATE METWEBAITPISTOP SET PISTOREPLY = :sMsgPrint WHERE AID = :sAA AND TMIMACODE = :nCodeTmima  ')
								Return FALSE
							! If NOT SqlImmediate ( " UPDATE WEBAIT_PISTOP SET PISTOSTATUS = 1 WHERE UriID = :sAA and k_pistop = :sCodePistop " )
								Return FALSE
							If NOT SqlImmediate ( " UPDATE METWEBAITPISTOP SET PISTOSTATUS = 1 WHERE AID = :sAA and TMIMACODE = :nCodeTmima  " )
								Return FALSE
							Call SqlClearImmediate()
							Return TRUE
						Case 4
							! If NOT SqlImmediate ( 'UPDATE WEBAIT_PISTOP SET PISTOREPLY = :sMsgPrint WHERE UriID = :sAA and k_pistop = :sCodePistop ')
								Return FALSE
							If NOT SqlImmediate ( 'UPDATE METWEBAITPISTOP SET PISTOREPLY = :sMsgPrint WHERE AID = :sAA AND TMIMACODE = :nCodeTmima  ')
								Return FALSE
							! If NOT SqlImmediate ( " UPDATE WEBAIT_PISTOP SET PISTOSTATUS = 2  WHERE UriID = :sAA and k_pistop = :sCodePistop " )
								Return FALSE
							If NOT SqlImmediate ( "UPDATE METWEBAITPISTOP SET PISTOSTATUS = 2  WHERE AID = :sAA AND TMIMACODE = :nCodeTmima  " )
								Return FALSE
							Call SqlClearImmediate()
							Return TRUE
