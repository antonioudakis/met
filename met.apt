		Functions
			Function: Report
				Description: Main function called for report.
				Returns
				Parameters
					Number: nRep
				Static Variables
				Local variables
				Actions
					!
					Set nRepType = nRep
					Select Case nRep
						Case BASIC_Report
							!
							Set nErrR = RPT_NoToolbar
							Set sQrpPath = sDriver || "\\NTUARIS\\EKDOSEIS\\REPORTS\\MQRP\\MetDep.QRP"
							Call SalCreateWindow( frmReport, hWndForm, 'Στοιχεία Μέλους ΔΕΠ ' )
							Call SalReportView( frmdep, frmReport,sQrpPath,
									 ':sLECTSX, :sTmima1,:sTmima2, :dfAmdep, :dfEponDep, :dfOnomaDep, :dfPatrDep,
									:dfArtayDep, :sSexDep, :sTownDep, :dfOdosDep, :dfTel1Dep, :dfTel2Dep,
									:sTmimaYpag, :sTomDep, :dfGnostiko,  :sTrdep, :sLevelDep, :sOrigUnivDep',
									'sSxTm, sTmima1,sTmima2, nKvdDep, sEpvnDep, sOnomaDep, sPatrDep,
									sDepId, sSexDep, sTownDep, sAddrDep, sTel1Dep, sTel2Dep,
									sTmimaDep, sTomeasDep, sSubjectDep,  sWay, sBathmida, sOrigUnivDep', nErrR )
							Break
			Function: ReportStart
				Description:
				Returns
					Boolean:
				Parameters
					Number: nRep
				Static Variables
				Local variables
				Actions
					!
					Select Case nRep
						Case BASIC_Report
							!
							If NOT hTitle.bConnected
								!
								Call hTitle.Connect()
							!
							If NOT hTitleTom.bConnected
								!
								Call hTitleTom.Connect()
							Return TRUE
							Break
					Return TRUE
			Function: ReportFetchInit
				Description:
				Returns
					Boolean:
				Parameters
					Number: nRep
				Static Variables
				Local variables
					String: sSelect
				Actions
					!
					Set bFirstTime = TRUE
					Select Case nRep
						Case BASIC_Report
							!
							! !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
							Set sSelect = 'SELECT METTMIMA.TITLOS  
									   INTO :sTmimaYpag  FROM DEP, METTMIMA
									 WHERE DEP.K_TMPAR = METTMIMA.TMIMACODE 
									 AND AM_DEP = :dfAmdep'
							Call hTitle.Initialize( sSelect)
							Call hTitle.Prepare()
							! !
							Set sSelect = 'SELECT  METTOMEIS.TITLOS  
									   INTO  :sTomDep  FROM DEP, METTOMEIS
									 WHERE  
									 METTOMEIS.TOMCODE = DEP.K_TOMPAR AND AM_DEP = :dfAmdep'
							Call hTitleTom.Initialize( sSelect)
							Call hTitleTom.Prepare()
							Set sTmimaYpag = STRING_Null 
							Set sTomDep = STRING_Null 
							! !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
							Call clsSqlHandleSelect.Execute(  )
							Return TRUE
							Break
					Return TRUE
			Function: ReportFetchNext
				Description:
				Returns
					Boolean:
				Parameters
					Number: nRep
				Static Variables
				Local variables
					Number: nFetch
				Actions
					!
					Select Case nRep
						Case BASIC_Report
							!
							If not clsSqlHandleSelect.Next(bOk )
								!
								Return FALSE
							Call cmbFyloDep.SetSelection()
							Set sSexDep = SalListQueryTextX( cmbFyloDep, SalListQuerySelection( cmbFyloDep ) )
							Call cmbPoliDep.SetSelection()
							Set sTownDep = SalListQueryTextX( cmbPoliDep, SalListQuerySelection(  cmbPoliDep ) )
							Call cmbTrdep.SetSelection()
							Set sTrdep = SalListQueryTextX( cmbTrdep, SalListQuerySelection(  cmbTrdep ) )
							Call cmbLevelDep.SetSelection()
							Set sLevelDep = SalListQueryTextX( cmbLevelDep, SalListQuerySelection(  cmbLevelDep ) )
							Call cmbIdrdep.SetSelection()
							Set sOrigUnivDep = SalListQueryTextX( cmbIdrdep, SalListQuerySelection(  cmbIdrdep ) )
							!
							Call hTitle.Execute()
							Call hTitle.Next( nFetch )
							Call hTitleTom.Execute()
							Call hTitleTom.Next( nFetch )
							Return TRUE
							Break
		Window Parameters
		Window Variables
			Boolean: bOk
			Number: nRep
			Number: nRepType
			Number: nErrR
			String: sSexDep
			String: sTownDep
			String: sTmimaYpag
