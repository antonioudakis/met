				Menu Item: Ενημέρωση Αριθμού Διπλώματος Σε Αναγορευμένους Φοιτητές
					Resource Id: 59001
					Keyboard Accelerator: (none)
					Status Text:
					Menu Settings
						Enabled when: SalIsNull(dfStf) and nWorkTmima = 0
						Checked when:
					Menu Actions
						Call SqlConnect( hArDipl )
						Call SqlImmediate( "SELECT NEXTSN FROM TABLESN INTO :nMaxArDipl WHERE TABLENAME = 'METARDIPL' ")
						Call SqlClearImmediate(  )
						Call SqlPrepare( hArDipl, "SELECT MITROO,PROGCODE FROM METSTUPROG INTO :sArDiplMitroo, :nArDiplProgCode WHERE DATEANAG IS NOT NULL AND ARDIPL IS NULL ORDER BY DATEANAG,MITROO" )
						Call SqlExecute( hArDipl )
						Call SqlFetchNext( hArDipl, nArDipl )
						Set nTrexonArDipl = nMaxArDipl
						While nArDipl != FETCH_EOF
							Set sSelect = "UPDATE METSTUPROG SET ARDIPL="||SalNumberToStrX( nTrexonArDipl , 0 )||" WHERE MITROO ='"||sArDiplMitroo||"' AND PROGCODE="||SalNumberToStrX( nArDiplProgCode, 0 )
							Call SqlImmediate( sSelect)
							Call SqlClearImmediate(  )
							Set nTrexonArDipl = nTrexonArDipl + 1
							Call SqlFetchNext( hArDipl, nArDipl )
						If nMaxArDipl = nTrexonArDipl
							Call SalMessageBox( "Οι αριθμοί διπλώματος των αναγορευμένων φοιτητών είναι ήδη ενημερωμένοι", "Ενημέρωση αριθμού διπλώματος", MB_Ok )
						Else
							Set sSelect = "UPDATE TABLESN SET NEXTSN="||SalNumberToStrX( nTrexonArDipl, 0 )||" WHERE TABLENAME='METARDIPL'"
							Call SqlImmediate( sSelect)
							Call SqlClearImmediate(  )
							Call SqlDisconnect( hArDipl  )
							Call SalMessageBox( "H διαδικασία ενημέρωσης του αριθμού διπλώματος των αναγορευμένων φοιτητών ολοκληρώθηκε επιτυχώς", "Ενημέρωση αριθμού διπλώματος", MB_Ok )
